	<build>
		<plugins>
			<plugin>
				<artifactId>maven-clean-plugin</artifactId>
				<version>${maven-clean-plugin-version}</version>
				<configuration>
					<filesets>
						<fileset>
							<directory>.</directory>
							<includes>
								<include>*.log</include>
							</includes>
						</fileset>
						<fileset>
							<directory>src/main/webapp</directory>
							<includes>
								<include>files/**</include>
								<include>imgs/user/**</include>
								<include>imgs/webdata/**</include>
							</includes>
							<excludes>
								<exclude>**/readme</exclude>
							</excludes>
						</fileset>
						<fileset>
							<directory>src/main/webapp/WEB-INF/Data</directory>
							<includes>
								<include>*.del</include>
								<include>**</include>
							</includes>
							<excludes>
								<exclude>Index/index.json</exclude>
								<exclude>Templ/**</exclude>
								<exclude>SEO/seo.json</exclude>
								<exclude>**/readme</exclude>
								<exclude>Official/**</exclude>
								<exclude>Export/account/**</exclude>
							</excludes>
							<followSymlinks>false</followSymlinks>
						</fileset>
					</filesets>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>${maven-compiler-plugin-version}</version>
				<configuration>
					<encoding>UTF-8</encoding>
					<source>${java-version}</source>
					<target>${java-version}</target>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-resources-plugin</artifactId>
				<version>${maven-resources-plugin-version}</version>
				<configuration>
					<source>${java-version}</source>
					<target>${java-version}</target>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-surefire-plugin</artifactId>
				<version>${maven-surefire-plugin-version}</version>
				<configuration>
					<!-- <skip>true</skip> -->
					<runOrder>alphabetical</runOrder>
					<!-- <includes> -->
					<!-- <include>**/InitTestSuite.java</include> -->
					<!-- </includes> -->
					<threadCount>1</threadCount>
					<excludes>
						<exclude>**/global/**</exclude>
					</excludes>
					<!-- <testFailureIgnore>true</testFailureIgnore> -->
				</configuration>
				<dependencies>
					<dependency>
						<groupId>org.apache.maven.surefire</groupId>
						<artifactId>surefire-junit47</artifactId>
						<version>2.11</version>
					</dependency>
				</dependencies>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-war-plugin</artifactId>
				<version>${maven-war-plugin-version}</version>
				<configuration>
					<archiveClasses>false</archiveClasses>
					<!-- <packagingExcludes>WEB-INF/classes/com/**,WEB-INF/classes/me/**</packagingExcludes> -->
					<!-- <warSourceExcludes>imgs/**,js/**,css/**,ckeditor/**</warSourceExcludes> -->
					<webResources>
						<resource>
							<directory>src/main/webapp</directory>
							<!-- <excludes> -->
							<!-- <exclude>**/js</exclude> -->
							<!-- <exclude>**/css</exclude> -->
							<!-- <exclude>/**</exclude> -->
							<!-- </excludes> -->
						</resource>
					</webResources>
				</configuration>
			</plugin>
			<plugin>
				<artifactId>maven-assembly-plugin</artifactId>
				<version>${maven-assembly-plugin-version}</version>
				<executions>
					<execution>
						<id>makejar</id>
						<phase>package</phase>
						<goals>
							<goal>single</goal>
						</goals>
						<configuration>
							<archive>
								<manifest>
									<mainClass>com.grand.app.RabbitMQMain</mainClass>
								</manifest>
							</archive>
							<skipAssembly>false</skipAssembly>
							<appendAssemblyId>false</appendAssemblyId>
							<outputDirectory>
								${project.build.directory}/${project.build.finalName}/WEB-INF/lib
							</outputDirectory>
							<descriptors>
								<descriptor>src/assemble/jar.xml</descriptor>
							</descriptors>
						</configuration>
					</execution>
					<execution>
						<id>makeweb</id>
						<phase>package</phase>
						<goals>
							<goal>single</goal>
						</goals>
						<configuration>
							<descriptors>
								<descriptor>src/assemble/webtargz.xml</descriptor>
							</descriptors>
						</configuration>
					</execution>
					<execution>
						<id>makestatic</id>
						<phase>package</phase>
						<goals>
							<goal>single</goal>
						</goals>
						<configuration>
							<descriptors>
								<descriptor>src/assemble/static.xml</descriptor>
							</descriptors>
						</configuration>
					</execution>
					<execution>
						<id>makeconfig</id>
						<phase>package</phase>
						<goals>
							<goal>single</goal>
						</goals>
						<configuration>
							<descriptors>
								<descriptor>src/assemble/config.xml</descriptor>
							</descriptors>
						</configuration>
					</execution>
					<execution>
						<id>makewebdata</id>
						<phase>package</phase>
						<goals>
							<goal>single</goal>
						</goals>
						<configuration>
							<descriptors>
								<descriptor>src/assemble/webdata.xml</descriptor>
							</descriptors>
						</configuration>
					</execution>
				</executions>
			</plugin>
			<plugin>
				<groupId>org.apache.tomcat.maven</groupId>
				<artifactId>tomcat7-maven-plugin</artifactId>
				<version>${maven-tomcat7-plugin-version}</version>
				<configuration>
					<server>SuperGeniusServ-admin</server>
					<path>/${project.build.finalName}</path>
					<port>9080</port>
					<update>true</update>
					<charset>utf-8</charset>
					<uriEncoding>UTF-8</uriEncoding>
				</configuration>
			</plugin>
		</plugins>
		<finalName>proj-supergenius-admin</finalName>
	</build>
</project>
