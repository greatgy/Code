package com.supergenius.core.converters;

import org.springframework.core.convert.converter.Converter;

import com.supergenius.xo.finance.enums.EGrade;

/**
 * 配置spring mvc自动接收EGrade
 * 
 * @author LiJiacheng
 */
public class StringToEGradeConverter implements Converter<String, EGrade> {

	@Override
	public EGrade convert(String source) {
		String value = source.trim();
		if ("".equals(value)) {
			return null;
		}
		return EGrade.get(Integer.parseInt(source));
	}

}
