@charset "utf-8";
/* CSS Document */
.left{ float:left;}
.right{ float:right;}
.clear{ clear:both;}
.hd{display: none;}

/******************以上为公共样式********************/ 
body { min-width: 1100px;}
.wraper {width: 100%;} 
.wraper *{ font-family: "Microsoft Yahei";}
/* 底部bottom开始 */
.bottom{width: 100%;height: 220px;background-color: #323030;clear:both;font-family: "Microsoft Yahei";}
.bottom_content { width: 800px;padding-top: 45px;  margin: 0 auto;text-align: center;}
.bottom_top { border-bottom: 1px dashed #fff;padding-bottom: 15px;}
.bottom_top img { margin-right: 60px;}
.bottom p{text-align:center;font-family: "Microsoft Yahei";}
.bot_01{color: #fff;margin-top: 15px;}
.bot_01 a{color: #fff;font-size: 13px;margin: 16px;font-family: "Microsoft Yahei";}
.bot_01 a:hover { color: #ff0000;}
.bot_02{color: #9c9b9b;font-size: 13px;margin-top: 10px;margin-bottom: 15px;}
.spare { color: #fff;vertical-align: middle;font-size: 13px;font-family: "Microsoft Yahei";}
.spare img { width: 35px;height: 35px;overflow: hidden;vertical-align: middle;margin-left: 5px;}
.wx_code { display: none; position: relative;width: 200px;height: 200px;top: -240px;left: 80px;background: url("../../imgs/default/wx_bg.png") no-repeat;}
.app_img { width: 150px;height: 150px;overflow: hidden;position: absolute;top: 15px;left: 25px;}
/* 底部bottom结束 */

/* 头部header_tou开始 */
.header_tou .top{width:100%; height:37px; background-color:#4e4d4d;}
.header_tou .top .top_main1{width:1023px;margin:0 auto;}
.header_tou .top .top_left{list-style:none; float:left; }
.header_tou .top .top_left li{float:left;}
.header_tou .top .top_left li a{font-size:12px; color:#b8b8b8; display:block; margin:10px 15px;}
.header_tou .top .top_left li a:hover{color:#fff;}
.header_tou .top .top_left li a.tl_act{color:#FFF;}
.header_tou .top .top_right{list-style:none; float:right;position: relative;z-index: 2;height: 37px;line-height: 37px;}
.top_right li{float:left;}
.top_right li a{font-size:12px; color:#ccc; display:block;padding-right: 14px;padding-left:8px;height: 29px;font-family: "微软雅黑";}
.top_right li.tr_act{display:block;width:55px; height:39px; background-color:#d93126;position: relative;z-index: 20;}
.top_right .toprightleft { color: #fff;line-height: 22px;margin-top: 6px;font-size: 13px;margin-right: 50px;}
.top_right .toprightleft:hover a { opacity: 1;}
.toprightleft .toprigleftname { color: #fff;display: inline-block;opacity: .8;}
.toprightleft .topusername { color: #B9B9B9;margin-left: 10px;}
.top_right .toprightleft:hover .topusername { color: #ff0000;text-decoration: underline;}
.toprightleft .clickbutton { opacity: .8;display: inline-block;font-size: 18px;color: #fff;width: 15px;height: 15px;line-height: 18px;overflow: hidden;}
.top_right .exit { float: right;color: #EBEBEB;display: block;position: absolute;right: 0;top: 0;font-size: 12px;opacity: .8;}
.top_right .exit:hover { opacity: 1;text-decoration: underline;color: #FB0909;font-size: 13px;}
.userdetails { width: 155px;position: absolute;z-index: 10;top: 26px;padding: 12px 0 0;}
.userdetails .triangle { width: 0px;height: 0px;border-style: solid;border-width: 0px 8px 10px 8px;border-color: transparent transparent #494c52 transparent;}
.userdetails .userdetailists { background: #585B60;height: 175px;padding-top: 10px;}
.userdetailists li { width: 155px;height: 40px;}
.userdetailists li a { text-align: center;padding: inherit;color: #fff;height: 40px;line-height: 40px;font-size: 15px;}
.userdetailists li a:hover { background: #333333;color: #23d9a1;}
.header_tou .top_02{width:100%;height: 70px;position: relative;z-index: 1; border-bottom: 6px solid #ccc;background:#fff;}
.header_tou .top_main2{width:1023px;height:70px; background-color:#fff; background:url("../../imgs/web/logo.png") left center no-repeat;margin: 0 auto; }
.header_tou .top_main2 span { display: inline-block;float: left;height: 70px;line-height: 70px;margin-left: 165px;font-size: 20px;font-family: "microsoft yahei";color: #01295d;}
.header_tou .top_main2 .top_22{  font-size:14px;width:1000px; position:relative;}
.top_main2 .navuser { display: inline-block;float: right;text-align: left;top: -60px;width: 60px;height: 50px;margin-top: 10px;overflow: hidden;}
.navuser img { width: 56px;height: 50px;position: relative;overflow: hidden;}
.header_tou .navuser .r_circle { width: 8px;height: 8px;background: #FC0B37;border-radius: 50%;position: absolute;right: inherit;margin-left: -5px;top: 12px;overflow: hidden;}
.navuser .usernews { position: absolute;width: 122px;overflow: hidden;z-index: 100;right: 315px;}
.usernews .triangle { width: 0px;height: 0px;border-style: solid;border-width: 0px 15px 12px 15px;border-color: transparent transparent #F2F2F2 transparent;margin-left: 65px;}
.usernews .contentsection { background: #fff;padding-bottom: 20px;border-radius: 2px;border: 1px solid #E6E6E6;border-top: transparent;}
.contentsection .registrationbtn { display: inline-block;width: 68px;height: 20px;text-align: center;background: #f7a642;line-height: 20px;border-radius: 4px;margin: 10px;color: #fff;box-shadow: 1px 1px 0 1px #DEA864;}
.contentsection .registrationbtn:hover { background-color: #F99522;}
.contentsection .newtipslist li { position: relative;}
.newtipslist li a { display: block;}
.newtipslist li a:hover { background-color: #efefef;}
.newtipslist li a:hover .newstitle { color: #63ada4;}
.header_tou .top_main2 .newstitle { text-align: left;height: 30px;float: initial;color: #414240;font-size: 15px;margin-left: 10px;line-height: 30px;}
.header_tou .top_main2 .totalnews { width: 20px;height: 12px;font-size: 12px;margin-left: auto;line-height: 12px;position: absolute;right: 10px;top: 10px;color: #fff;text-align: center;background: #ff0000;border-radius: 4px;overflow: hidden;}

/* 头部header_tou结束 */

.container {
	min-height: 700px;
	background: #f2f2f2;
}
.top_right li a:hover {
	color: #fff;
}
.tipscontent {
	width: 1000px;
	height: 300px;
	margin: 0 auto;
	padding-top: 200px;
	line-height: 280px;
	overflow: hidden;
	text-align: center;
}
.tipscontent img {
	width: 80px;
	height: 65px;
	overflow: hidden;
	vertical-align: middle;
	margin-right: 20px;
}
.tipscontent .word {
	font-size: 20px;
	color: #F31021;
	overflow: inherit;
}