/*CSS Document */
.left{ float:left;}
.right{ float:right;}
.clear{ clear:both;}
.hd{display: none;}

/******************以上为公共样式********************/
/**头部共用样式**/
body { background: #ffffff;overflow: auto;}
body * { font-family: "Microsoft Yahei";}
.header_tou {
    border-bottom: 1px solid #e2e2e2;
}
.header_fix {
    position: fixed;
    top: 37px;
    left: 0;
    background: #fff;
    z-index: 1000;
    width: 100%;
}
.firstTopnav {
    margin-bottom: 10px;
    width: 1100px;
    overflow: hidden;
    height: 47px;
    text-align: left;
}
.logo {
    display: inline-block;
    width: 10%;
    height: 47px;
    overflow: hidden;
    vertical-align: middle;
}
.logo img {
    max-width: 100%;
    height: 47px;
}
.searchMiddle {
    display: inline-block;
    margin: 0 8% 0 12%;
    text-align: center;
    width: 50%;
    height: 39px;
    overflow: hidden;
    vertical-align: middle;
}
.searchMiddle input {
    padding: 0 10px;
    height: 39px;
    border: 1px solid #ccc;
    width: 434px;
    font-size: 15px;
    line-height: 22px;
    color: #adadad;
}
.searchimg {
    margin-left: -5px;
    width: 76px;
    height: 39px;
    display: inline-block;
    background: #fe5700;
    vertical-align: top;
    line-height: 39px;
    text-align: center;
}
.searchimg img {
    width: 30px;
    height: 30px;
    overflow: hidden;
    margin: 4.5px auto;
}
.loginbox {
    display: inline-block;
    width: 18%;
    text-align: right;
    line-height: 39px;
}
.votebtn {
    display: inline-block;
    background: #f5f5f5;
    padding: 5px 8px;
    line-height: 20px;
    font-size: 16px;
    color: #fe5700;
    border-radius: 6px;
    border: 1px solid #ddd;
    vertical-align: middle;
}
.votebtn:hover {
    background: #e27405;
    color: #fff;
}
.logined {
    display: inline-block;
    margin-left: 18%;
    border-radius: 50%;
}
.logined img {
    border-radius: 50%;
    vertical-align: middle;
    width: 47px;
    height: 47px;
    overflow: hidden;
}
.topnav {
    width: 1100px;
    text-align: left;
    height: 50px;
    border-bottom: 2px solid #e2e2e2;
}
.navlist {
    display: inline-block;
    width: 100%;
    vertical-align: middle;
    background: #fdfdfd;
    border-top: 2px solid #fe5f0c;
}
.navlist li {
    float: left;
    line-height: 46px;
    width: auto;
    text-align: center;
    position: relative;
}
.navlist li a {
    color: #333;
    display: block;
    font-size: 16px;
    width: 100%;
    padding: 0 19.34px;
}
.navlist .current {
    background: #fe6816;
    color: #fdfdfd;
    font-family: "FZSEJW--JB1-0";
}
.navlist .current a {
    color: #fdfdfd;
    /* font-weight: bold; */
}
.navlist li a:hover {
    background: #fe6816;
    color: #fdfdfd;
    font-family: "FZSEJW--JB1-0";
}
.labeltitle {
    color: #333;
    display: block;
    font-size: 16px;
    width: 100%;
    padding: 0 19.34px;
}
.rollnav {
    display: none;
    position: absolute;
    padding-bottom: 10px;
    left: 0;
    width: 100%;
    z-index: 10;
    background: #fff;
    box-shadow: 0 2px 2px 0 #cccccc;
}
.rollnav li {
    float: left;
    width: 100%;
    height: 42px;
    line-height: 42px;
    text-align: center;
    color: #333;
    font-size: 16px;
}
.rollnav .curselected {
    background: #fe5901;
    color: #fff;
}
.rollnav li a {
    padding: 0;
    color: #fe6816;
}
.rollnav .curselected a {
    color: #fff;
}
.rollnav li a:hover {
    background: #fe5901;
    color: #fff;
}
.navlist li:hover .rollnav {
    display: block;
}
.classType {
    font-size:  14px;
    color: #333;
    margin-left: 7px;
}
.containerBox {
    width: 100%;
    padding: 0 15px;
    margin: 172px auto 40px;
    text-align: left;
    overflow: hidden;
    min-height: 455px;
}
.contentLeft {
    display: inline-block;
    width: 65%;
    vertical-align: top;
    overflow: hidden;
}
.contentRight {
    display: inline-block;
    width: 30%;
    margin-left: 4%;
    margin-bottom: 20px;
    overflow: hidden;
}

.rightItem {
    margin-bottom: 30px;
}
.pictype {
    color: #ff7500;
}
.videotype {
    color: #9900ff;
}
.original {
    color: #00cc00;
}
.phoneFooter {
    margin: 20px auto;
    font-size: 0.8rem;
    color: #999;
    line-height: 22px;
    text-align: center;
    position: relative;
    bottom: 0;
    display: none;
    width: 90%;
}
.backBtn {
    font-size: 2rem;
    position: absolute;
    left: 4%;
    top: 0;
    color: #9b9b9b;
    line-height: 40px;
    display: none;
}
.swiper-button-next, .swiper-button-prev {
    display: none;
}
/* 分页 */
.pagenum {
    text-align: center;
    padding: 20px 0;
    width: auto;
}
.pagenum li {
    display: inline;
    margin-right: 5px;
}
.pagenum a {
    padding: 6px 12px;
    line-height: 40px;
    background: #FFF;
    border: 1px #ddd solid;
    color: #444;
    -moz-border-radius: 3px;
    -webkit-border-radius: 3px;
    border-radius: 3px;
    font-size: 0.8rem;
}
.pagenum .here {
    background: #ff8000;
    border: 1px #ff8000 solid;
    color: #FFF;
}
.pagenum a:hover {
    background: #ff8000;
    border: 1px #ff8000 solid;
    text-decoration: none;
    color: #FFF;
}

/* 职场困惑弹框 */
.maskContent {
    position: fixed;
    width: 50%;
    height: auto;
    top: 30%;
    left: 25%;
    background: #ddd;
    text-align: left;
    border-radius: 8px;
    z-index: 1002;
    display: none;
}
.maskContent h3 {
    margin-top: 30px;
}
.questionForm {
    /* width: 50%; */
    margin: 30px auto;
}
.questionForm .state {
    
}
.maskContent .cancelBtn {
    float: none;
    background: #fff;
    color: #333;
    border: 1px solid #ccc;
}
.maskContent .cancelBtn:hover {
    background: #a5a5a5;
    color: #fff;
}

/* 手机搜索弹出层 */
.searchMask {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    background: rgba(0, 0, 0, 0.5);
    z-index: 100;
    display: none;
}

/* 登录弹出遮罩层 */
.mask {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    background: rgba(0, 0, 0, 0.5);
    z-index: 1001;
    display: none;
}

/************** 轮播共用样式 **************/
.carousel-indicators {margin-bottom: -8px;}
.carousel-indicators li { background-color: #ffffff;width: 7px;height: 7px;}
.carousel-indicators .active { background-color: #d90d0e;width: 8px;height: 8px;border: 1px solid #d90d0e;}
.carousel { top: 70px;padding-bottom: 65px;}

/******************footer共用样式********************/
.footer { border-top: 1px solid #E4DFDF;padding-top: 20px;margin-top: 15px;margin-bottom: 20px;}
.container_follow { overflow: initial;}
.follow_bottom h4 { color: #848484;font-size: 15px;}
.copyright { display: inline;height: 0;float: none;background-color: transparent;font-size: 14px;color: #848484;margin-left: 5px;}
