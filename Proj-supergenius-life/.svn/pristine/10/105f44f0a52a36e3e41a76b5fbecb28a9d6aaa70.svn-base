.quizList img {
    border-radius: 50%;
    overflow: hidden;
    vertical-align: top;
    max-width: 40px;
    height: auto;
    width: 5%;
}
.quizRt {
    display: inline-block;
    width: 95%;
    vertical-align: middle;
    overflow: hidden;
}
.quizRt h4 {
    margin: 0;
    padding-bottom: 10px;
    font-size: 17px;
    color: #333;
    line-height: 24px;
    max-height: 58px;
    overflow: hidden;
}
.answerStatus {
    margin-left: 20px;
    color: #f97730;
    font-size: 16px;
}
.quizRt p {
    font-size: 14px;
    color: #8b8b8b;
    line-height: 20px;
    max-height: 40px;
    overflow: hidden;
}
.readitem {
    font-size: 17px;
    color: #666;
    text-align: center;
    overflow: hidden;
}
.readNum {
    display: block;
    margin: 15px 0;
}
.someQuestion {
    border-bottom: 1px solid #ddd;
}
.questionTitle {
    display: inline-block;
    margin: 0 auto 20px;
    max-width: 85%;
    line-height: 24px;
    font-size: 20px;
    color: #333;
}
.report {
    font-size: 14px;
    color: #595858;
    margin-left: 15px;
    background: #ececec;
    padding: 2px 5px;
    border-radius: 4px;
}
.report:hover {
    background: #eb6100;
    color: #fdfdfd;
}
.someQuestion .pubQuestion {
    padding: 6px 10px;
    font-size: 17px;
    background: none;
    color: #fe5700;
    border: 1px solid #fe5700;
    border-radius: 4px;
}
.someQuestion .pubQuestion:hover {
    background: #fe5700;
    color: #fff;
}
.qsintroduce {
    margin: 0 0 10px;
    font-size: 1rem;
    line-height: 22px;
    overflow: hidden;
}
.relatePiece {
    margin: 15px 0;
    font-size: 0.96rem;
    color: #666;
    height: 25px;
    overflow: hidden;
    line-height: 24px;
}
.replyNum {
    float: right;
}
.replyNum i {
    margin: 20px;
}
.replyNum img {
    width: 18px;
    height: 18px;
    overflow: hidden;
    margin-right: 4px;
    vertical-align: top;
}
.qsNewquiz {
    margin: 20px auto 40px;
}
.qsUserimg {
    display: inline-block;
    margin-right: 1%;
    width: 5%;
    overflow: hidden;
    vertical-align: top;
    padding: 2px;
}
.qsUserimg img {
    width: 100%;
    height: auto;
    overflow: hidden;
    border-radius: 50%;
}
.qsNewquiz .commentitemRt {
    width: 93%;
}
.qsUsername {
    font-size: 1rem;
    color: #ff6600;
}
.qsword {
    margin: 10px 0 15px;
    line-height: 20px;
    font-size: 16px;
    color: #333;
}
.qstime {
    font-size: 0.8rem;
    color: #878585;
}
.qsCommentOperate {
    display: inline-block;
    float: right;
    padding-right: 1%;
    overflow: hidden;
    width: 60%;
    text-align: right;
}
.qsCommentOperate a {
    margin-left: 5%;
    font-size: 0.98rem;
    color: #676767;
    line-height: 20px;
    overflow: hidden;
    display: inline-block;
}
.qsCommentOperate a img {
    margin-top: -5px;
    width: 28px;
    height: 32px;
    overflow: hidden;
    vertical-align: middle;
}
.qsCommentOperate .openReply {
    color: #1a3b9c;
    font-size: 0.9rem;
}
.qsCommentOperate .openReply img {
    width: 20px;
    height: 20px;
    overflow: hidden;
    vertical-align: middle;
    margin-left: 4px;
}
.qsNewquiz .zan {
    float: none;
}
.qsNewquiz .replyBox textarea {
    padding: 6px 10px;
    height: 60px;
    font-size: 0.8rem;
}
.qsWordpiece {
    margin: 8px 0;
}
.replyBox .submitBtn {
    margin-right: 8px;
    width: 60px;
    height: 30px;
    font-size: 1rem;
}
.replyBox .qsface {
    background: none;
    width: 40px;
    height: 40px;
    overflow: hidden;
}
.qsReplyList li {
    border-top: 1px solid #ddd;
    padding-top: 20px;
    padding-bottom: 5px;
    border-bottom: none;
    border-bottom: none!important;
}
.qsReplyList .reply {
    color: #1a3b9c;
}
.qsReplyList .userimg {
    width: 4%;
    margin-right: 1%;
}
.qsReplyList .userimg img {
    width: 100%;
    height: auto;
    max-width: 40px;
}
.qsReplyList .commentitemRt {
    width: 94%;
}
.qsReplyList .zan {
    float: right;
}
.reported {
    background: #eb6100;
    color: #fdfdfd;
}
.reported:hover {
    box-shadow: 0 1px 3px 0 #ba5a07;
}
/* 发布、回答问题 */
.askQuestionBox, .answerQuestionBox {
    border: 1px solid #ddd;
    border-radius: 8px;
    padding: 30px 30px 100px;
    margin: 0 auto 30px;
}
.askQuestionBox h3 , .answerQuestionBox h3 {
    padding-left: 10px;
    margin-bottom: 0;
    height: 22px;
    line-height: 20px;
    border-left: 5px solid #eb6100;
    font-size: 18px;
    color: #4d4c4c;
}
.askQuestionBox .questionForm {
    margin: 10px auto 30px;
    border-top: 1px solid #cfcfcf;
    padding: 50px 20px;
}
.askQuestionBox input {
    border: 1px solid #b5b5b5;
    font-size: 0.94rem;
    border-radius: 6px;
    height: 40px;
    width: 98%;
    padding: 0 10px;
    line-height: 36px;
    color: #333;
    overflow: hidden;
}
.state {
    margin: 35px 0 15px;
    font-size: 16px;
    color: #666666;
}
.askQuestionBox textarea , .answerQuestionBox textarea {
    width: 98%;
    border: 1px solid #b5b5b5;
    height: 370px;
    padding: 10px;
    line-height: 22px;
    color: #333;
    font-size: 0.92rem;
    resize: none;
    border-radius: 8px;
}
.questionBox .submitBtn {
    margin-top: 35px;
    margin-right: 2%;
    width: auto;
    padding: 10px 15px;
    height: auto;
    font-size: 18px;
    letter-spacing: 2px;
}
.askQuestionBox .cancelBtn, .answerQuestionBox .cancelBtn {
    float: none;
    background: #8c8b8b;
    color: #fffefe;
    border: 1px solid #ccc;
    padding: 8px 25px;
}
.askQuestionBox .cancelBtn:hover, .answerQuestionBox .cancelBtn:hover {
    background: #6c6b6b;
}
.answer {
    display: inline-block;
    padding: 30px 20px 0;
    margin-top: 10px;
    width: 100%;
    line-height: 24px;
    font-size: 16px;
    color: #5a5a5a;
    border-top: 1px solid #ddd;
}
.answer i {
    font-size: 1.1rem;
    margin-left: 10px;
    color: #333;
}
.answerQuestionBox .questionForm {
    padding: 0 20px;
}
.hidename {
    display: inline-block;
    float: right;
    margin-right: 30px;
    margin-top: 15px;
    font-size: 14px;
    color: #777777;
}
.hidename input[type="checkbox"] {
    vertical-align: bottom;
    width: auto;
    padding: 0;
    line-height: normal;
    height: auto;
}
.questionForm .hidename {
    margin-top: 5%;
}














