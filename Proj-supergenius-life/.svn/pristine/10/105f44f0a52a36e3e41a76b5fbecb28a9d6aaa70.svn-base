#lunbo {
    position: relative;
    width: 100%;
    height: 366px;
    overflow: hidden;
}
.banner li {
	position: absolute;
    left: 0;
    height: 366px;
    width: 100%;
}
.banneritem {
	display: block;
    width: 100%;
    height: 100%;
    border-radius: 8px;
}
.banneritem1 {
	background: #000 url(/imgs/default/banner1.png) no-repeat center center;
    background-size: 100% 100%;
}
.banneritem2 {
    background: #000 url(/imgs/default/banner2.png) no-repeat center center;
    background-size: 100% 100%;
}
.banneritem3 {
    background: #000 url(/imgs/default/banner1.png) no-repeat center center;
    background-size: 100% 100%;
}
.banneritem4 {
    background: #000 url(/imgs/default/banner1.png) no-repeat center center;
    background-size: 100% 100%;
}
#controller {
    height: 16px;
    text-align: right;
    position: absolute;
    bottom: 8px;
    left: 53%;
    z-index: 10;
    margin-left: -50px;
}
#controller li {
    display: inline;
    float: left;
    margin: 0 5px;
    width: 12px;
    height: 12px;
    cursor: pointer;
    background: #fff;
    border-radius: 50%;
}
#controller .active {
    background: #fe5700;
}
.mainBox {
    margin: 32px auto;
}
.tabBox li {
    position: relative;
    margin-bottom: 20px;
    border-bottom: 1px solid #e3e3e3;
    padding-bottom: 20px;
}
.tabitemimg {
    display: inline-block;
    width: 34%;
    vertical-align: top;
    overflow: hidden;
}
.tabitemimg img {
    width: 100%;
    height: 160px;
    overflow: hidden;
}
.tabitemRt {
    display: inline-block;
    width: 62%;
    margin-left: 2%;
    text-align: left;
}
.articleTitle {
    display: block;
    margin-bottom: 10px;
    font-size: 1.06rem;
    color: #333;
    line-height: 24px;
}
.descripe {
    margin: 0 auto 30px;
    color: #888;
    font-size: 0.9rem;
    line-height: 22px;
    max-height: 68px;
    overflow: hidden;
}
.timepiece {
    position: absolute;
    bottom: 15px;
    width: 60%;
    min-height: 20px;
    overflow: hidden;
    color: #666;
    font-size: 0.76rem;
    line-height: 20px;
}
.tabBox li:hover .tabitemimg img {
    transform: scale(1.2);
    -webkit-transform: scale(1.2);
    -o-transform: scale(1.2);
    -moz-transform: scale(1.2);
    transition: transform 0.8s;
    -webkit-transition: transform 0.8s;
    -o-transition: transform 0.8s;
    -moz-transition: transform 0.8s;
    -ms-transition: transform 0.8s;

}
.tabBox li:hover .articleTitle {
    color: #f58a08;
}
.operateBox {
    display: inline-block;
    text-align: right;
    float: right;
    margin-right: 2%;
