/*CSS Document */
.left{ float:left;}
.right{ float:right;}
.clear{ clear:both;}
.hd{display: none;}

/******************以上为公共样式********************/
/* 底部bottom开始 */
.bottom{width: 100%;height: 220px;background-color: #323030;clear:both;font-family: "Microsoft Yahei";}
.bottom_content { width: 800px;padding-top: 45px;}
.bottom_top { border-bottom: 1px dashed #fff;padding-bottom: 15px;}
.bottom_top img { margin-right: 60px;}
.bottom p{text-align:center;font-family: "Microsoft Yahei";}
.bot_01{color: #fff;margin-top: 15px;}
.bot_01 a{color: #fff;font-size: 13px;margin: 16px;font-family: "Microsoft Yahei";}
.bot_01 a:hover { color: #ff0000;}
.bot_02{color: #9c9b9b;font-size: 13px;margin-top: 10px;margin-bottom: 15px;}
.spare { color: #fff;vertical-align: middle;font-size: 13px;font-family: "Microsoft Yahei";}
.spare img { width: 35px;height: 35px;overflow: hidden;vertical-align: middle;margin-left: 5px;}
.wx_code { display: none; position: relative;width: 200px;height: 200px;top: -240px;left: 80px;background: url("../../imgs/default/wx_bg.png") no-repeat;}
.app_img { width: 150px;height: 150px;overflow: hidden;position: absolute;top: 15px;left: 25px;}
#bctop{width:60px;height:60px;background:url("../../imgs/default/bctop.png") no-repeat;cursor:pointer; position:fixed;bottom:20%;right:10%;clear:both;display:none;}

/**头部共用样式**/
body { min-width:1100px;background: #ffffff;overflow: auto;}
body * { font-family: "Microsoft Yahei";}
.header_tou {
    border-bottom: 1px solid #e2e2e2;
}
.header_fix {
    position: fixed;
    top: 52px;
    left: 0;
    background: #fff;
    z-index: 1000;
    width: 100%;
}
.firstTopnav {
    margin-bottom: 10px;
    width: 1100px;
    height: 47px;
    text-align: left;
    margin-top: 20px;
}
.logo {
    display: inline-block;
    width: 10%;
    height: 47px;
    overflow: hidden;
    vertical-align: middle;
}
.logo img {
    max-width: 100%;
    height: 47px;
}
.searchMiddle {
    display: inline-block;
    margin: 0 8% 0 12%;
    text-align: center;
    width: 50%;
    height: 39px;
    overflow: hidden;
    vertical-align: middle;
}
.searchMiddle input {
    padding: 0 10px;
    height: 39px;
    border: 1px solid #ccc;
    width: 434px;
    font-size: 15px;
    line-height: 22px;
    color: #adadad;
}
.searchimg {
    margin-left: -5px;
    width: 76px;
    height: 39px;
    display: inline-block;
    background: #fe5700;
    vertical-align: top;
    line-height: 39px;
    text-align: center;
}
.searchimg img {
    width: 30px;
    height: 30px;
    overflow: hidden;
    margin: 4.5px auto;
}
.loginbox {
    display: inline-block;
    width: 18%;
    text-align: right;
    line-height: 39px;
    position: relative;
}
.votebtn {
    display: inline-block;
    background: #f5f5f5;
    padding: 5px 8px;
    line-height: 20px;
    font-size: 16px;
    color: #fe5700;
    border-radius: 6px;
    border: 1px solid #ddd;
    vertical-align: middle;
}
.votebtn:hover {
    background: #e27405;
    color: #fff;
}
.logined {
    display: inline-block;
    margin-left: 18%;
    border-radius: 50%;
}
.logined img {
    border-radius: 50%;
    vertical-align: middle;
    width: 47px;
    height: 47px;
    overflow: hidden;
}
.loginbox .princeimg {
    width: 35px;
    height: 35px;
    border: none;
    position: absolute;
    right: -10px;
    margin-top: -18px;
}
.topnav {
    width: 1100px;
    text-align: left;
    height: 50px;
    border-bottom: 2px solid #e2e2e2;
}
.navlist {
    display: inline-block;
    width: 100%;
    vertical-align: middle;
    background: #fdfdfd;
    border-top: 2px solid #fe5f0c;
}
.navlist li {
    float: left;
    line-height: 46px;
    width: auto;
    text-align: center;
    position: relative;
}
.navlist li a {
    color: #333;
    display: block;
    font-size: 16px;
    width: 100%;
    padding: 0 19.34px;
}
.navlist .current {
    background: #fe6816;
    color: #fdfdfd;
    font-family: "FZSEJW--JB1-0";
}
.navlist .current a {
  /*   color: #fdfdfd; */
    /* font-weight: bold; */
}
.navlist li a:hover {
    background: #fe6816;
    color: #fdfdfd;
    font-family: "FZSEJW--JB1-0";
}
.labeltitle {
    color: #333;
    display: block;
    font-size: 16px;
    width: 100%;
    padding: 0 19.34px;
}
.rollnav {
    display: none;
    position: absolute;
    left: 0;
    width: 100%;
    z-index: 10;
    background: #fff;
    box-shadow: 0 2px 2px 0 #cccccc;
}
.rollnav li {
    float: left;
    width: 100%;
    height: 42px;
    line-height: 42px;
    text-align: center;
    color: #333;
    font-size: 16px;
}
.rollnav .curselected {
    background: #fe5901;
    color: #fff;
}
.rollnav li a {
    padding: 0;
    color: #fe6816;
}
.rollnav .curselected a {
    color: #fff;
}
.rollnav li a:hover {
    background: #fe5901;
    color: #fff;
}
.navlist li:hover .rollnav {
    display: block;
}
.classType {
    font-size:  14px;
    color: #333;
    margin-left: 2px;
}
.containerBox {
    width: 1100px;
    margin: 175px auto 40px;
    text-align: left;
    overflow: hidden;
    min-height: 455px;
}
.contentLeft {
    display: inline-block;
    width: 65%;
    vertical-align: top;
    overflow: hidden;
}
.contentRight {
    display: inline-block;
    width: 30%;
    margin-left: 4%;
    margin-bottom: 20px;
    overflow: hidden;
}

.rightItem {
    margin-bottom: 30px;
}
.pictype {
    color: #ff7500;
}
.videotype {
    color: #9900ff;
}
.original {
    color: #00cc00;
}
.phoneFooter {
    margin: 20px auto;
    font-size: 0.8rem;
    color: #999;
    line-height: 22px;
    text-align: center;
    position: relative;
    bottom: 0;
    display: none;
    width: 90%;
}
.backBtn {
    font-size: 2rem;
    position: absolute;
    left: 4%;
    top: 0;
    color: #9b9b9b;
    line-height: 40px;
    display: none;
}
.swiper-button-next, .swiper-button-prev {
    display: none;
}
/* 分页 */
.pagenum {
    text-align: center;
    padding: 20px 0;
    width: auto;
}
.pagenum li {
    display: inline;
    margin-right: 5px;
}
.pagenum a {
    padding: 6px 12px;
    line-height: 40px;
    background: #FFF;
    border: 1px #ddd solid;
    color: #444;
    -moz-border-radius: 3px;
    -webkit-border-radius: 3px;
    border-radius: 3px;
    font-size: 0.8rem;
}
.pagenum .here {
    background: #ff8000;
    border: 1px #ff8000 solid;
    color: #FFF;
}
.pagenum a:hover {
    background: #ff8000;
    border: 1px #ff8000 solid;
    text-decoration: none;
    color: #FFF;
}

/* 职场困惑弹框 */
.maskContent {
    position: fixed;
    width: 50%;
    height: auto;
    top: 30%;
    left: 25%;
    background: #ddd;
    text-align: left;
    border-radius: 8px;
    z-index: 1002;
    display: none;
}
.maskContent h3 {
    margin-top: 30px;
}
.questionForm {
    /* width: 50%; */
    margin: 30px auto;
}
.questionForm .state {
    
}
.maskContent .cancelBtn {
    float: none;
    background: #fff;
    color: #333;
    border: 1px solid #ccc;
}
.maskContent .cancelBtn:hover {
    background: #a5a5a5;
    color: #fff;
}

/* 手机搜索弹出层 */
.searchMask {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    background: rgba(0, 0, 0, 0.5);
    z-index: 100;
    display: none;
}

/* 登录弹出遮罩层 */
.mask {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    background: rgba(0, 0, 0, 0.5);
    z-index: 1001;
    display: none;
}


/* 非会员弹窗提示 */
.fairnormalUser {
    width: 45%;
    position: fixed;
    z-index: 1010;
    top: 36%;
    left: 30%;
    text-align: left;
    background: #fff;
    /* height: 280px; */
    overflow: hidden;
    border-radius: 8px;
    display: none;
}
.fairnormalUser h2 {
    padding: 15px 20px;
    font-size: 1.2rem;
    letter-spacing: 1px;
    border-bottom: 1px solid #ddd;
    color: #333;
}
.closebtn {
    float: right;
    font-size: 1.5rem;
    color: #adadad;
    transform: rotateZ(90deg);
}
.normalTipbox {
    padding: 30px 20px 60px;
    text-align: center;
}
.normalTipbox p {
    margin: 20px auto 50px;
    font-size: 1.12rem;
    color: #636363;
    text-align: center;
    line-height: 24px;
}
.beuser {
    display: inline-block;
    margin-bottom: 20px;
    height: 40px;
    line-height: 40px;
    background: #ff7d37;
    color: #fff;
    padding: 0 12px;
    border-radius: 6px;
    font-size: 15px;
    text-align: center;
}

.userbox .userimg {
	width: 45px;
	height: 45px;
	overflow: hidden;
	vertical-align: top;
	border-radius: 50%;
	margin-top: 6px;
}
.userborder{
	border: 3px solid #fec11c;
    border-radius: 50%;
}
.userimg .princeimg {
	width: 30px;
    height: 30px;
    border: none;
    margin-left: -22px;
    margin-top: -15px;
}

/************** 轮播共用样式 **************/
.carousel-indicators {margin-bottom: -8px;}
.carousel-indicators li { background-color: #ffffff;width: 7px;height: 7px;}
.carousel-indicators .active { background-color: #d90d0e;width: 8px;height: 8px;border: 1px solid #d90d0e;}
.carousel { top: 70px;padding-bottom: 65px;}

/******************footer共用样式********************/
.footer { border-top: 1px solid #E4DFDF;padding-top: 20px;margin-top: 15px;margin-bottom: 20px;}
.container_follow { overflow: initial;}
.follow_bottom h4 { color: #848484;font-size: 15px;}
.copyright { display: inline;height: 0;float: none;background-color: transparent;font-size: 14px;color: #848484;margin-left: 5px;}
