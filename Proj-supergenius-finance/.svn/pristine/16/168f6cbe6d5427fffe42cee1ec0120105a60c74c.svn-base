.blockgroup {
	width: 99%;
	background: #fff;
	padding: 0 19px;
	padding-bottom: 20px;
	padding-top: 5px;
	margin-top: 28px;
	overflow: hidden;
	border-bottom: 1px solid #c6c6c6;
	box-shadow: 1px 0 1px 0 #c6c6c6;
}

.mb {
	border-left: 3px solid #1d71b6;
	height: 20px;
	margin: 10px auto 20px;
	color: #1d71b6;
	text-align: left;
	font-weight: bold;
	font-size: 17px;
	line-height: 20px;
	background: url(/imgs/default/titbg.png) no-repeat left center;
}

.articleimg {
	width: 17%;
	overflow: hidden;
}

.articleright {
	display: inline-block;
	margin-left: 2%;
	width: 79%;
	vertical-align: top;
}

.articlelink {
	display: inline-block;
	line-height: 24px;
	font-size: 1.2rem;
	font-weight: bold;
	color: #333;
	max-height: 26px;
	overflow: hidden;
}

.newestcontent {
	margin-top: 5px;
	max-height: 75px;
	overflow: hidden;
	font-size: 1.04rem;
	color: #666666;
	line-height: 24px;
}

#lunbo, #bannerBox {
	position: relative;
	margin-top: 25px;
	width: 100%;
	height: 400px;
	overflow: hidden;
}

.banner li {
	position: absolute;
	left: 0;
	height: 400px;
	width: 100%;
}

.banneritem {
	display: block;
	width: 100%;
	height: 100%;
	border-radius: 8px;
}

.banneritem1 {
	background: #000 url(/imgs/default/banner2.png) no-repeat center center;
	background-size: 100% 100%;
}

.banneritem2 {
	background: #000 url(/imgs/default/banner1.png) no-repeat center center;
	background-size: 100% 100%;
}

.banneritem3 {
	background: #000 url(/imgs/default/banner2.png) no-repeat center center;
	background-size: 100% 100%;
}

.banneritem4 {
	background: #000 url(/imgs/default/banner1.png) no-repeat center center;
	background-size: 100% 100%;
}

.swiper-pagination-bullet {
	width: 10px !important;
	height: 10px !important;
	border-radius: inherit !important;
}

.showArticle {
	width: 99%;
	margin: 20px auto;
	padding: 10px 2px;
	height: 380px;
	background: #fff;
	box-shadow: 0 1px 1px 0 #c5c5c5;
}

.showArticle li {
	padding-left: 0;
	margin-bottom: 15px;
}

.showArticle li img {
	width: 100%;
	height: 166px;
	overflow: hidden;
}

.showArticle li .description {
	height: 50px;
	line-height: 24px;
	font-size: 1rem;
	color: #fefefe;
}

.articleList {
	margin-top: 30px;
	background: #fff;
	box-shadow: 0 1px 1px 0 #d0d0d0;
	width: 99%;
	padding: 20px 15px 15px;
	border-bottom: 1px solid #d4d4d4;
}

.newArt {
	height: 25px;
	line-height: 22px;
	margin: 0 auto 30px;
	padding-left: 10px;
	color: #1d71b6;
	font-size: 1.25rem;
	border-left: 5px solid #1d71b6;
}

.tabBox li {
	position: relative;
	margin-bottom: 20px;
	border-bottom: 1px solid #e3e3e3;
	padding-bottom: 20px;
}

.tabitemimg {
	display: inline-block;
	width: 34%;
	vertical-align: top;
	overflow: hidden;
}

.tabitemimg img {
	width: 100%;
	height: 160px;
	overflow: hidden;
}

.tabitemRt {
	display: inline-block;
	width: 62%;
	margin-left: 2%;
	text-align: left;
}

.articleTitle {
	display: block;
	margin-bottom: 10px;
	font-size: 1.25rem;
	color: #333;
	line-height: 24px;
}

.descripe {
	margin: 0 auto 30px;
	color: #666666;
	font-size: 1rem;
	line-height: 22px;
	max-height: 68px;
	overflow: hidden;
}

.timepiece {
	position: absolute;
	bottom: 15px;
	width: 60%;
	min-height: 20px;
	overflow: hidden;
	color: #666;
	font-size: 0.76rem;
}

.time {
	color: #666;
}

.operateBox {
	display: inline-block;
	text-align: right;
	float: right;
	margin-right: 2%;
	min-height: 20px;
	width: 60%;
}

.littleicon {
	margin-left: 6%;
	font-size: .8rem;
	color: #666;
	vertical-align: middle;
	min-height: 20px;
}

.littleicon img {
	padding-right: 2px;
	width: 24px;
	height: 20px;
	overflow: hidden;
	vertical-align: middle;
}

.tabBox li:hover .tabitemimg img {
	transform: scale(1.2);
	-webkit-transform: scale(1.2);
	-o-transform: scale(1.2);
	-moz-transform: scale(1.2);
	transition: transform 0.8s;
	-webkit-transition: transform 0.8s;
	-o-transition: transform 0.8s;
	-moz-transition: transform 0.8s;
	-ms-transition: transform 0.8s;
}

.tabBox li:hover .articleTitle {
	color: #1d71b6;
}

.huiyuan, .questionnaire {
	display: block;
	width: 100%;
	height: auto;
	overflow: hidden;
}

.huiyuan img, .questionnaire img {
	max-width: 99%;
	border: 1px solid #dcdcdc;
	/* padding: 5px 0; */
}

.huiyuan img:hover, .questionnaire img:hover {
	transform: scale(0.96);
	box-shadow: 0 1px 2px 0 #9a9a9a;
	transition: transform 0.3s;
}

.rightItem h4 {
	margin-bottom: 15px;
	font-size: 1.1rem;
	color: #1d71b6;
}

.rightItem i {
	display: inline-block;
	margin-right: 8px;
	width: 5px;
	height: 18px;
	background: #1d71b6;
	vertical-align: middle;
}

.tags li {
	display: inline-block;
	margin: 2px 2% 15px 3px;
	font-size: 0.96rem;
	padding: 10px;
	border: 1px solid #d8d8d8;
	border-radius: 20px;
	color: #333;
	overflow: hidden;
}

.tags li a {
	color: #333;
}

.tags li:hover {
	border: 1px solid #1D71b6;
	color: #1D71b6;
}

.tags li:hover a {
	color: #1D71b6;
}

.itemBlock {
	padding: 20px 10px;
	width: 99%;
	margin-left: 0;
	box-shadow: 0 1px 1px 0 #b5b5b5;
	background: #fff;
}

.description {
	position: absolute;
	bottom: 0;
	width: 94%;
	height: 42px;
	background: rgba(131, 131, 131, 0.8);
	font-size: 0.9rem;
	line-height: 20px;
	color: #fff;
	padding: 0 3%;
	overflow: hidden;
}

.hotArticle li {
	margin-bottom: 20px;
	position: relative;
}

.top1 {
	width: 100%;
	height: 100%;
	overflow: hidden;
}

.hoticon {
	position: absolute;
	top: 0;
	left: 0;
	display: inline-block;
	width: 75px;
	height: 28px;
	line-height: 28px;
	text-align: center;
	font-size: 1.1rem;
	font-weight: bold;
	color: #fff;
}

.redicon {
	background: #ff0000;
}

.yellowicon {
	background: #5cd403;
}

.greenicon {
	background: #1d71b6;
}

.top1 img {
	width: 100%;
	height: 200px;
	overflow: hidden;
}

.top1 .description {
	width: 99.9%;
	height: 50px;
	bottom: 3px;
	padding: 5px 3%;
	font-size: 1rem;
	line-height: 22px;
}

.numtag {
	position: absolute;
	left: 0;
	top: 0;
	display: inline-block;
	width: 20px;
	height: 20px;
	background: #44cb5f;
	text-align: center;
	line-height: 20px;
	font-size: 1rem;
	color: #fff;
}

.four {
	background: #ff6600;
}

.five {
	background: #70e1ff;
}

.hotArticle .title {
	display: inline-block;
	width: 65%;
	margin-left: 1%;
	color: #363636;
	font-size: 1rem;
	line-height: 20px;
}

.hotArticle li a {
	color: #363636;
	font-size: 1rem;
	line-height: 20px;
}

.hotArticle li a:hover {
	color: #1D71b6;
}

.hotArticle .title:hover {
	color: #1D71b6;
}

.hotArticle .flashtime {
	display: block;
	margin-left: 0;
	margin-top: 8px;
	font-size: 0.8rem;
	color: #999;
}

.tabBox {
	margin-top: 15px;
	margin-left: 20px;
    margin-right: 20px;
}

.nomore {
	margin: 40px 0;
	text-align: center;
	font-size: .9rem;
	color: #999;
}

.line {
	display: inline-block;
	width: 35%;
	height: 1px;
	background: #acacac;
	vertical-align: middle;
}

.titlebottom {
	position: absolute;
	bottom: 0;
	padding-left: 5%;
	padding-right: 5%;
	height: 60px;
	background: rgba(127, 126, 126, 0.8);
	width: 100%;
	line-height: 32px;
	text-align: left;
	color: #fff;
	font-size: 1.2rem;
	overflow: hidden;
}

.fourimg {
	width: 30%;
	overflow: hidden;
}

.title .flashtime {
	display: block;
	margin-left: 0;
	margin-top: 5px;
	font-size: 0.8rem;
	color: #999;
}

/* 个人中心 */
.userLeft {
	padding: 20px 0 0;
	border: 1px solid #ddd;
	text-align: center;
	border-radius: 8px;
}

.userInfo li {
	font-size: 1.2rem;
	line-height: 30px;
	margin-bottom: 20px;
}

.userInfo a {
	font-size: 1.1rem;
	color: #333;
}

.userInfo a:hover {
	font-weight: bold;
	color: #f58a08;
}

.userInfo a img {
	width: 30px;
	height: 100%;
	overflow: hidden;
	padding-right: 5px;
	vertical-align: middle;
}

.curguide a {
	font-weight: bold;
	color: #1d71b6;
}

.arrow {
	display: none;
}

.logout {
	margin: 50% 0 0;
	display: block;
	font-size: 1.1rem;
	color: #666;
	padding: 8px 10px;
	background: #ebebeb;
	text-align: center;
}

.logout:hover {
	color: #de0000;
}

.userRight {
	margin-top: 28px;
	margin-bottom: 10px;
	border: 1px solid #ddd;
	padding: 20px 0;
	min-height: 500px;
	border-radius: 8px;
}

.editInfo, .saveInfo {
	width: 90%;
}

.baseinfo {
	margin: 10px auto 10px;
	font-size: 1.16rem;
	color: #1d71b6;
	padding-left: 8px;
	border-left: 5px solid #1d71b6;
}

.edit, .save {
	float: right;
	margin-top: 5px;
	font-size: 0.9rem;
	color: #6f6f6f;
}

.edit:hover, .save:hover {
	color: rgb(255, 153, 51);
}

.baseinfoList {
	margin: 40px auto;
}

.baseinfoList li {
	margin-bottom: 20px;
}

.labelTips {
	display: inline-block;
	width: 15%;
	font-size: 1rem;
	color: #949494;
	vertical-align: top;
	line-height: 20px;
}

.userBaseRt {
	display: inline-block;
	width: 80%;
	vertical-align: middle;
	color: #333;
	font-size: 1rem;
	line-height: 20px;
}

.userBaseRt img {
	width: 45px;
	height: 45px;
	overflow: hidden;
	border-radius: 50%;
}

.userBaseRt input {
	border: 1px solid #b0b0b0;
	width: 40%;
	height: 25px;
	padding: 0 10px;
	line-height: 20px;
	font-size: 0.9rem;
	color: #333;
}

.userBaseRt input[type="radio"] {
	width: auto;
	height: auto;
	vertical-align: middle;
}

.userBaseRt label {
	margin-right: 6%;
}

.userBaseRt textarea {
	border: 1px solid #b0b0b0;
	width: 80%;
	height: 100px;
	resize: none;
	padding: 10px 15px;
	font-size: 0.96rem;
	color: #333;
	line-height: 20px;
}
/* 消息通知 */
.userNews, .myCollect {
	width: 90%;
}

.date {
	margin-bottom: 40px;
}

.date h5 {
	margin: 20px 0 20px 10px;
	font-size: 14px;
	font-weight: bold;
	color: #666;
}

.clean {
	float: right;
	color: #A1A1A1;
	font-size: 0.9rem;
}

.clean:hover {
	color: #d51010;
}

.newslist li {
	margin-bottom: 15px;
	max-height: 20px;
	overflow: hidden;
	padding-left: 25px;
	line-height: 20px;
}

.newslist i {
	margin-right: 5px;
	font-size: 12px;
	color: #FF0000;
	font-style: italic;
}

.newstip {
	font-size: 0.9rem;
	color: #999;
	display: inline-block;
	max-width: 30%;
	vertical-align: top;
}

.form {
	display: inline-block;
	margin-left: 1%;
	max-width: 54%;
	height: 20px;
	overflow: hidden;
	line-height: 20px;
	vertical-align: top;
	font-size: 0.9rem;
	color: #434343;
	text-decoration: none;
}

.form:hover {
	color: #1d71b6;
}

.delete {
	float: right;
	/* margin-right: 15px; */
}

.delete img {
	width: 18px;
	height: 18px;
	overflow: hidden;
}

.IsEmpty {
	width: 100%;
	height: 100%;
	text-align: center;
	font-size: 18px;
	color: #b3b3b3;
	line-height: 500px
}

/* 我的收藏 */
.usercollect {
	padding-left: 15px;
	margin: 20px auto;
}

.bdshare-button-style0-16 a, .bdshare-button-style0-16 .bds_more {
	margin: 6px 6px 6px 0 !important;
}
/* 我的文章 */
.artTab {
	margin: 15px 20px 30px;
	line-height: 35px;
	border-bottom: 1px solid #ddd;
	overflow: hidden;
}

.artTab li {
	display: inline-block;
	margin-right: 5%;
	font-size: 1.1rem;
	color: #333;
	padding: 0 2%;
}

.artTab .curart {
	color: #1d71b6;
	border-bottom: 3px solid #1d71b6;
	padding-bottom: 2px;
}

.artTab li:hover {
	padding-bottom: 2px;
	color: #1d71b6;
	border-bottom: 3px solid #1d71b6;
	cursor: pointer;
}

.myCollect .articleTitle {
	font-size: 1.2rem;
}

.reddot {
	display: inline-block;
	width: 6px;
	height: 6px;
	background: red;
	border-radius: 50%;
	vertical-align: top;
	margin-top: 5px;
	position: absolute;
}

.userArticleLeft {
	margin-top: 28px;
	margin-bottom: 10px;
	padding: 20px 0;
	min-height: 500px;
	border-radius: 8px;
	margin-left: 4%;
	background: #fff;
	box-shadow: 1px 1px 1px 0 #c6c6c6;
}

.userArticleRight {
	padding: 20px 0 0;
	text-align: center;
	border-radius: 8px;
	margin-left: 0;
	background: #fff;
	box-shadow: 1px 1px 1px 0 #c6c6c6;
}

.pubTime {
	font-size: 0.82rem;
	color: #666;
}

.pubTime img {
	width: 18px;
	height: auto;
	overflow: hidden;
	margin-right: 5px;
	vertical-align: top;
}

@media screen and (min-width: 768px) and (max-width: 920px) {
    .articleTitle {
        font-size: 1.1rem;
        max-height: 48px;
        overflow: hidden;
    }
    .descripe {
        display: none;
    }
    .timepiece {
        bottom: 12px;
    }
    .littleicon img {
        width: 20px;
        height: 16px;
    }
    #lunbo {
        height: 260px;
    }
}

@media screen and (max-width: 767px) {
	.containerBox {
		width: 90%;
		min-height: auto;
		overflow: hidden;
	}
	.mainBox {
		display: none;
	}
	.phoneManibox {
		display: block;
		margin-top: 20px;
	}
	.contentLeft {
		width: 100%;
	}
	.contentRight {
		display: none;
	}
	#lunbo {
		margin-top: 0;
		height: 185px;
	}
	.lunboindex {
		margin-top: 40px !important;
	}
	.titlebottom {
		height: 40px;
		font-size: 1rem;
	}
	#controller {
		bottom: 0;
	}
	#controller li {
		width: 10px;
		height: 10px;
	}
	.tabitemimg img {
		max-width: 100%;
		overflow: hidden;
	}
	.articleTitle {
		font-size: 0.98rem;
		line-height: 20px;
		max-height: 42px;
		overflow: hidden;
	}
	.timepiece {
		bottom: 13px;
	}
	.infoArticles .timepiece {
		margin: 0 auto;
	}
	.operateBox {
		width: 45px;
		height: 20px;
		overflow: hidden;
	}
	.tabBoxplace li {
		margin-bottom: 15px;
	}
	.line {
		width: 30%;
	}
	.consult {
		display: none;
	}

	/* 个人基本资料 */
	.userLeft {
		display: block;
		width: 100%;
		border: none;
		margin-top: 10px;
		margin-left: 0;
		padding: 0;
	}
	.userInfo {
		text-align: left;
	}
	.userInfo li {
		border-bottom: 1px solid #ddd;
		padding-bottom: 5px;
	}
	.userInfo a {
		display: block;
		font-size: 1rem;
	}
	.userInfo a:hover {
		color: #ff7500;
	}
	.arrow {
		display: inline-block;
		float: right;
		margin-top: 5px;
	}
	.logout:hover {
		color: #686868;
		background: navajowhite;
	}
	.userRight {
		display: none;
	}
	/* 我的文章 */
	.artTab {
		margin: 0;
	}
	.artTab li {
		margin-right: 0;
		padding: 0 1%;
		font-size: 0.95rem;
	}
	.myCollect .articleTitle {
		font-size: 0.96rem;
	}

	/* 手机端 */
	.userCenterLeft {
		display: none;
	}
	.userCenterRight {
		display: block;
		border: none;
		width: 100%;
		margin-top: 10px;
	}
	.editInfo, .saveInfo {
		width: 100%;
	}
	.userBaseRt input {
		width: 80%;
	}
	.userBaseRt textarea {
		height: 60px;
	}
	.userNews, .myCollect {
		width: 100%;
	}
	.newslist li {
		padding-left: 10px;
	}
	.usercollect {
		padding-left: 0;
	}
	.userArticleLeft {
		margin-top: 0;
		border: none;
		padding: 0;
		min-height: 400px;
		margin-left: 0;
		background: none;
		box-shadow: none;
	}
	.showArticle {
		width: 100%;
		margin: 20px auto;
		padding: 10px 0;
		height: auto;
		background: none;
		box-shadow: none;
	}
	.showArticle li img {
		height: 115px;
	}
	.articleList {
		margin-top: 0;
		background: none;
		box-shadow: none;
		width: 100%;
		padding: 20px 0 15px;
		clear: both;
		border-bottom: none;
	}
	.newArt {
		margin: 0 auto 20px;
	}
	.tabBox li {
		margin-bottom: 15px;
	}
	.descripe {
		display: none;
	}
	.line {
		width: 30%;
	}
	.showArticle li .description {
		width: 92%;
	}
	.userArticleRights {
		display: block;
		width: 100%;
		margin-left: 0;
	}
	.userArticleLefts {
		display: none;
	}
	.newslist i {
		position: absolute;
		margin-top: -4px;
		margin-left: -20px;
	}
}
