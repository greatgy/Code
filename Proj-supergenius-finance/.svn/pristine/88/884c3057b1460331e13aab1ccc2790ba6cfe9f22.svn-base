			<ul class="hotSubslist ">
				[#list beanlist as item]
					[#assign url="${base}/his/article/${item.user.oid}" /]
					<li>
						<div class="hotSubsuser">
							<a href="${url}" target="_blank"><img [#if item.user.avatar?? && item.user.avatar != ""] src="${userimg}${item.user.avatar}" [#else] src="${baseimg}/imgs/web/noavatar.png" [/#if] class="subsuser" /></a>
							<a href="${url}" class="hotUsername" target="_blank">${item.user.showname}</a>
							<span><i>${item.bean.besubscribecount}</i>人订阅</span>
						</div>
						<p class="hotSubdetail">作者简介：[#if item.user.summary == ""] 暂无简介 [#else][@f.TrimSubstring content="${item.user.summary}" mylength=35 /][/#if]</p>
						<div class="source">
							<img src="${baseimg}/imgs/default/voiceimg.png" />
								[#if item.finance??]
									[#assign link=""]
									[#if "${item.finance.firstTypeName}" == "debate"]
										[#assign link="${base}/debate/article/${item.finance.uid}"]
									[#else]
										[#assign link="${base}/${item.finance.firstTypeName}/${item.finance.oid}"]
									[/#if]
									<a href="${link}" class="from" target="_blank">
									[@f.TrimSubstring content="${item.finance.title}" mylength=40 /]
								[#elseif item.topic??]
									<a href="${base}/${item.typename}/${item.topic.oid}" class="from" target="_blank">
									[@f.TrimSubstring content="${item.topic.title}" mylength=40 /]
								[#else]
									<a href="javascript:void(0);" class="from">
									暂无最新文章...
								[/#if]
							</a>
						</div>
					</li>
				[/#list]
			</ul>
		</div>
	</div>
[/#if]