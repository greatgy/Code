	<script type="text/javascript">
		$(function(){
			/*取消订阅*/
			$(".cancelSubs").click(function(){
				$(this).prev().fadeIn("1000","linear",function(){
					$(this).fadeOut("1000","linear");
					$(this).parents("li").remove();
				});
			});
			/*添加订阅*/
			$(".addSubs").click(function(){
				$(this).prev().fadeIn("1000","linear",function(){
					$(this).fadeOut("600","linear");
					$(this).next().find("img").attr("src","${baseimg}/imgs/default/cancelsubs.png");
				});
			});
		});
