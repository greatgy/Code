												<div class="bluebar"></div>
											</div>
											<div class="numberbox">
												<span class="rednum">370</span>
												<span class="bluenum">630</span>
											</div>
										</div>
										<a href="javascript:;" class="redside bluevote">
											<img src="${baseimg}/imgs/default/bluezan.png" />
										</a>
									</div>
								</div>
							</li>
						</ul>
					</div>
				</div>
			</div>
		</div>
	</div>
	[#include "/WEB-INF/FtlLib/Pages/Edit.ftl" /]
	<script type="text/javascript">
		$(function(){
			/*鼠标悬浮显示提示*/
			$(".collected").mouseover(function(){
				$(this).parent().find("span").css("display","block");
			});
			$(".collected").mouseout(function(){
				$(this).parent().find("span").css("display","none");
			});
			/*取消收藏效果*/
			$(".collected").click(function(){
				$(this).find(".heart").attr("src","${baseimg}/imgs/default/notcollect.png");
				$(this).prev().fadeOut("800","linear",function(){
					$(this).parents("li").remove();
				});
			});
			/*论战投票*/
			$(".redvote").click(function(){
				$(this).find("img").attr("src","${baseimg}/imgs/default/redzaned.png");
				$(this).parent().find(".bluevote img").attr("src","${baseimg}/imgs/default/grayzan.png");
				var redNum = $(this).next().find(".rednum",".rprogress");
				var nowNum = parseInt(redNum.html()) + 1;
				redNum.html(nowNum);
			});
			$(".bluevote").click(function(){
				$(this).find("img").attr("src","${baseimg}/imgs/default/bluezaned.png");
				$(this).parent().find(".redvote img").attr("src","${baseimg}/imgs/default/grayzan.png");
				var blueNum = $(this).prev().find(".bluenum",".bprogress");
				var nowNum = parseInt(blueNum.html()) + 1;
				blueNum.html(nowNum);
			});

			/*收藏各项切换显示*/
			$(".collectnav li").click(function(){
				$(this).siblings().removeClass("curSelect");
				$(this).addClass("curSelect");
				var index = $(this).index();
				var showIndex = index + 1;
				$("#collect" + showIndex).show();
				$("#collect" + showIndex).siblings().hide();
			});
		});
	</script>
</body>
</html>
