/* CSS Document */
body {
	background: #f0f0f0;
}
.main {
	width: 1000px;
	margin: 0 auto;
	padding-bottom: 20px;
	background: #fff;
	margin-bottom: 40px;
	margin-top: 20px;
	box-shadow: 1px 1px 1px 0 #D9D9D9;
	overflow: hidden;
}
.contro_banner{
	position: relative;
	width: 1000px;
	margin-top: 30px;
}
.contro_banner .bgimg {
	display: block;
	width: 963px;
	height: 493px;
	overflow: hidden;
	margin: 0 auto;
	border: 1px solid #d3d3d3;
}
.bgimg img {
	width: 100%;
	height: 100%;
	overflow: hidden;
}
.contro_banner .lz_one_01{
	height: 65px;
	line-height: 65px;
	width: 963px;
	background-color: rgba(0,0,0,0.4);
	color: #fff;
	font-weight: bold;
	font-size: 20px;
	text-align: center;
	position: absolute;
	bottom: 0;
	left: 18px;
	text-align: left;
	overflow: hidden;
}
.lz_one_01 a {
	color: #fff;
	padding-left: 25px;
	display: block;
	width: 100%;
	height: 65px;
	overflow: hidden;
}
.contro_icon{ 
	position: absolute;
	top: 15px;
	left: 0;
	background: url(/imgs/default/part01.png) no-repeat left center;
	font-size: 18px;
	width: 175px;
	height: 57px;
	line-height: 45px;
	color: #fff;
}
/*内容部分开始*/
.contro_cont{ 
	width: 965px; 
	height: auto;
}
.contro_vs{ 
	background: url(../../../imgs/default/b_vs.png) no-repeat left center; 
	width: 998px;
	height: 63px; 
	margin-top: 20px;
	margin-bottom: 40px; 
	position: relative;
	margin-left: -17px;
}
.contro_red {
	font-size: 16px;
	position: absolute;
	top:20px;
	left:10%;
	color: #fff;
	width: 280px;
	overflow: hidden;
	text-align: center;
	height: 38px;
	text-overflow: ellipsis;
	white-space: nowrap;
	line-height: 38px;
}
.contro_bule{
	font-size: 16px;
	position: absolute; 
	top:20px; 
	right:10%;
	color: #fff;
	width: 280px;
	overflow: hidden;
	text-align: center;
	height: 38px;
	text-overflow: ellipsis;
	white-space: nowrap;
	line-height: 38px;
}
.tp_midd{ 
	width: 965px;
	height: 230px;
	overflow: hidden;
	margin-bottom: 20px;
}
.tp_img{position:relative; width:1000px; height:215px;overflow:hidden; }
.tp_img h4{font-size:30px; color:#344353; position:absolute;top:160px;left:475px;}
.tp_img .tp_lt{position:absolute; top:75px; left:110px;}
.tp_red{ position: absolute; top:125px; left:50px; font-size: 35px; color: #fff;  display:block; text-align:center;background:url(../../../imgs/default/lunzh2_le.png); width:154px; height:96px; line-height:96px; }
.tp_img .tp_rt{position:absolute; top:75px; right:110px;}
.tp_bule{position: absolute;top:125px; right:50px; color: #fff;font-size: 35px; display:block; text-align:center;background:url(../../../imgs/default/lunzh2_ri.png); width:154px; height:96px; line-height:96px;  }
.tp_midd .tp_tiao{ margin-top:5px; width:1000px; height:auto;}
.tp_midd .tp_tiao .tiao_lt{width:300px; height:20px; background-color:#b92727; margin-left:50px; float:left;}
.tp_midd .tp_tiao .tiao_rt{width:595px; height:20px;background-color:#0966c8;float:left; margin-left:5px;}


.newuser{
	width: 965px;
	overflow: hidden;
}
.newuser .mb {
	padding-left: 10px;
}
.mtb_10 { 
	margin-top: 10px;
	margin-bottom: 40px;
	margin: 0 auto;
	display: inline-block;
	width: 465px;
	vertical-align: top;
	text-align: left;
	margin-left: 10px;
	overflow: hidden;
}
.rside , .bside {
	font-size: 17px;
	height: 35px;
	line-height: 20px;
}
.rside {
	color: #d90000;	
}
.bside {
	color: #0077d9;
}
.user {
	width: 100%;
	height: 65px;
	overflow: hidden;
}
.user img {
	width: 50px;
	height: 50px;
	border: 2px solid #1d71b6;
	margin-right: 10px;
	border-radius: 5px;
}
.bluebox {
	text-align: right;
}
.options {
	background: url(/imgs/default/arrowbg.png) no-repeat bottom center;
	position: relative;
	height: 80px;
	margin-top: 45px;
}

.contro_l , .contro_r { 
	font-size: 17px;
	width: 155px;
	display: inline-block;
	text-align: center;
	line-height: 40px;
	height: 40px;
	text-align: left;
	position: absolute;
	top: 20px;
}
.contro_m{
	width: 229px;
	display: inline-block;
	text-align: center;
	height: 87px;
	background: url(/imgs/default/vsm.png) no-repeat center center;
	margin-top: -30px;
}
.contro_l {
	color: #d90000;
	left: 20px;
}
.contro_r{
	color: #0966c8;
	right: 0;
	left: auto;
}
.commentbox {
	height: 120px;
	background: #e8e8e8;
	overflow: hidden;
	margin-top: 15px;
}
.contro_text { 
	width: 910px;
	height: 42px;
	padding: 8px 10px 0;
	margin-top: 20px;
	font-size: 15px;
	color: #535353;
	line-height: 18px;
	font-family: "microsoft yahei";
	border-radius: 5px;
}
.contro_ared , .contro_abule { 
	font-size: 15px;
	display: inline-block;
	text-align: center;
	height: 28px;
	width: 85px;
	line-height: 28px;
	color: #fff;
	border-radius: 5px;
	box-shadow: 1px 1px 1px 0 #686868;
	margin-right: 16px;
	margin-top: 6px;
}
.contro_ared {
	background: #d90000;
}
.contro_abule{ 
	background: #0966c8;
}
.judgebtn {
	font-size: 15px;
	display: inline-block;
	text-align: center;
	height: 28px;
	width: 85px;
	line-height: 28px;
	color: #fff;
	margin-left: 10px;
	border-radius: 5px;
	box-shadow: 1px 1px 1px 0 #588E84;
	margin-right: 5px;
	margin-top: 6px;
	background: #1d71b6;
	letter-spacing: 2px;
}
.judgebtn:hover {
	background: #7ecef4; 
}
.txtbottom {
	text-align: left;
}
.idtip {
	font-size: 15px;
	display: inline-block;
	margin-left: 25px;
	color: #666;
}
.idtip input {
	width: 73px;
	height: 24px;
	margin-top: 6px;
	padding-left: 5px;
	border: none;
	color: #333;
	font-size: 13px;
	font-family: "microsoft yahei";
	overflow: hidden;
}
/*新闻部分*/
.contro_cont .controbox {
	text-align: left;
	overflow: hidden;
	margin-bottom: 15px;
}
.conleftbox , .conrightbox {
	width: 49.5%;
	display: inline-block;
	text-align: left;
	vertical-align: top;
}
.contro_left { 
	width: 465px;
	height: 160px;
	display: inline-block;
	vertical-align: top;
	padding-right: 12px;
	border-right: 1px #CCC solid;
	position: relative;
}
.controversy{
	width: 450px;
	height: auto;
	border-bottom:1px #cfd1d2 dashed;
	overflow: hidden;
}
.mid_right{
	width: 465px;
	height: 160px;
	display: inline-block;
	vertical-align: top;
	padding-left: 12px;
	border-left: 1px #CCC solid;
	margin-left: -5.4px;
	position: relative;
}
.con_rt_circle1 {
	position:absolute;
	top:90px; 
	right:-5px; 
	z-index:1;
}
.mid_right .con_rt_circle1 {
	left: -6px;
	right: auto;
}
.controversy .contro_title {
	font-weight: normal;
	font-size: 19px;
	font-weight:bold;
	color: #344353; 
	text-align: left;
	margin:6px auto;
}
.contro_cont_1 { 
	width: 450px; 
	font-weight: normal;
}
.controversy .contro_cont_1 img { 
	float: left;
	margin-bottom: 10px;
	width: 145px;
	height: 110px;
	overflow: hidden;
}
.contro_p1{color: #a6a6a6; font-size: 12px; text-align: left; margin-bottom: 8px; margin-left: 8px; margin-left: 160px;}
.contro_a{float: right; color: #1b548d; font-size: 12px; position:relative; top:10px;}
.contro_mid{ margin-left:40px; }
/*评论*/
.comments {
	width: 965px;
	margin: 20px auto;
	padding-bottom: 10px;
	overflow: hidden;
}
.commentitem {
	margin-bottom: 15px;
}
.nameimg {
	width: 40px;
	height: 40px;
	overflow: hidden;
	border: 2px solid #1d71b6;
	border-radius: 4px;
	vertical-align: top;
}
.commentred .nameimg {
	margin-right: 5px;
}
.commentblue .nameimg {
	margin-left: 5px;
}
.commentright {
	display: inline-block;
	width: 865px;
	vertical-align: top;
	border-bottom: 1px dotted #ddd;
	padding-bottom: 10px;
}
.name {
	font-size: 14px;
	font-weight: bold;
	color: #1d71b6;
}
.name:hover {
	color: #15c2e7;
}
.time {
	font-size: 13px;
	color: #535353;
	margin: 0 10px;
}
.commentright p {
	font-size: 15px;
	color: #333;
	line-height: 22px;
	padding: 6px;
	margin-top: 5px;
	margin-bottom: 5px;
	min-height: 44px;
	word-break: break-all;
}
.commentright .reditem {
	background: #fcebeb;
}
.commentright .blueitem {
	background: #ebf4fc;
	text-align: right;
}
.reply a {
	font-size: 13px;
	color: #535353;
}
.reply img {
	width: 16px;
	height: 19px;
	overflow: hidden;
	vertical-align: bottom;
}
.reply .openmore {
	float: right;
	margin-top: 3px;
	color: #74B5EB;
	margin-right: 3px;
}
.reply .openmore:hover {
	color: #0966c8;
}
.reply .openimg {
	width: 14px;
	height: 14px;
	overflow: hidden;
	margin-left: 3px;
} 
.reply .replylik {
	font-size: 14px;
	color: #535353;
	margin-left: 12px;
}
.reply .replylik:hover {
	color: #15c2e7;
}
.replybox {
	width: 100%;
}
.comment_textarea {
	width: 97%;
}
.suport {
	width: 100%;
	height: 40px;
	overflow: hidden;
}
.commentred {
	text-align: left;
}
.commentblue {
	text-align: right;
}
.secondcomment {
	margin-top: 15px;
}
.secondcomment .secondright {
	display: inline-block;
	width: 805px;
	margin-left: 5px;
}
.secondrtop .answer {
	margin: 0 5px;
	font-size: 14px;
	color: #535353;
}
.secondright .gray {
	background: #e8e8e8;
	text-align: left;
}
.commentblue .reply .openmore {
	float: left;
	margin-left: 3px;
}






.contro_pl{width: 1000px; overflow:hidden;height: auto;border-bottom: 1px solid #eaedef; color: #666666;font-size: 14px; font-family: "宋体";  margin-top: 30px;}
.contro_pl span a{color: #666666;}
.contro_pl span a:hover{color: #1d71b6;}
.contr_pic_t{ margin: 5px; margin-top: 0px;}

.contro_red_name{color:#b92727;}
.contro_nei{margin-top: 10px; display: inline-block;font-size:18px;font-family: "微软雅黑"; }
.contro_red_name .contro_nei{text-align:left;}
.contro_pnei{ overflow:hidden; padding:20px 40px; background: #f4f4f4 ; width: 886px; height: auto; border: 1px #e4e4e4 solid;border-top:none; margin-bottom: 20px;}
.contro_bule_name{color: #0966c8;}
.contro_bule_name .contro_nei{text-align: right;}
.contro_bule_hui{ text-align: right;}

.contro_borer{ margin-left:16px;margin-bottom:0;}
.text_left{ text-align: left;}
.text_right{text-align: right;}
.contro_borer2{ margin-right:18px; }




/*弹出框公共样式开始*/
.eject{ display:none;border:1px solid #cdcdcd;box-shadow:3px 3px 5px #f3f3f3;font-family: "微软雅黑";font-weight:bold;color:#7f7f7f;}
.eject>h3{position:relative;height:45px;line-height:45px;padding-left:70px;text-align:left;font-size:16px;border-bottom:1px solid #f2f2f2;background:url(../../../imgs/default/login.jpg) 30px 9px #f7f7f7 no-repeat;}
.eject>h3 span{position:absolute;right:16px;top:16px;}
/*弹出框公共样式结束*/
/*已投票弹出框样式开始*/
.vote_box{width: 455px;height: 295px;font-weight:normal; position:fixed; top:20%; left:32%; background: #fff;z-index: 1000}
.vote_box>h3{background:none;}
.vote_success{height:50px;line-height:50px;padding-left:110px;text-align:left;}
.vote_already{background:url(../../../imgs/default/vote01.png) 60px 5px no-repeat;}
.vote_con{width: 340px;margin-top:10px;font-size:14px;border-top:1px solid #eee;}
.vote_con p{height:50px;line-height:50px;text-align:left;position:relative;}
.vote_con p a{color:#dc1d15;}
.vote_text{width: 320px;height: 50px;padding:10px;font-size:14px;color:#333;border:1px solid #c9c9c9;border-radius:5px;}
.vote_sub{position:absolute;top:10px;right:10px;width: 70px;height: 25px;line-height:25px;text-align:center;display: block;color:#fff !important;background:#f52d25;border-radius:3px;}
.vote_ok{background:url(../../../imgs/default/vote02.png) 60px 5px no-repeat;}
#close1{ position:relative; bottom:15px;}
#close2{ position:relative; bottom:15px;}
/*已投票弹出框样式结束*/