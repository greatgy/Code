# Set root logger level to DEBUG and its only appender to A1.
log4j.rootLogger=off
log4j.rootLogger=ERROR,errorfile
log4j.logger.logfile=off,file
log4j.logger.com.supergenius=DEBUG,Console
log4j.logger.com.genius.timer.Handler=debug,Console,timerfile
log4j.logger.com.genius.global.utility=ERROR,Console
log4j.logger.com.genius.global.utility.EmailUtil=debug,Console,emailfile
log4j.logger.com.genius.global.utility.SerialUtil=DEBUG,Console,serialfile
log4j.logger.com.genius.core.spring=debug,Console
log4j.logger.com.genius.core.interceptors=debug,Console
log4j.logger.com.genius.core.cache.impl.CacheFacade=debug,Console

# A1 is set to be a ConsoleAppender.
log4j.appender.A1=org.apache.log4j.ConsoleAppender
# A1 uses PatternLayout.
log4j.appender.A1.layout=org.apache.log4j.PatternLayout
#log4j.appender.A1.layout.ConversionPattern=%-4r [%t] %-5p %c %x - %m%n
#log4j.appender.A1.layout.ConversionPattern=%d{yyyy-MM-dd HH:mm:ss,SSS} %5p - %m%n
log4j.appender.A1.layout.ConversionPattern=[%d{HH:mm:ss:SSS}][%C-%M] -%m%n
#log4j.appender.A1.layout.ConversionPattern=%d %5p - %m%n

#\u8f93\u51fa\u5230\u6587\u4ef6(\u8fd9\u91cc\u9ed8\u8ba4\u4e3a\u8ffd\u52a0\u65b9\u5f0f)
log4j.appender.F1=org.apache.log4j.FileAppender
log4j.appender.F1.File=Log4J.log
log4j.appender.F1.layout=org.apache.log4j.TTCCLayout
#log4j.appender.F1.File=c:/Log4JDemlo02.htm
#log4j.appender.F1.layout=org.apache.log4j.HTMLLayout

log4j.appender.file=org.apache.log4j.DailyRollingFileAppender
log4j.appender.file.File=root.log
log4j.appender.file.DatePattern='.'yyyy-MM-dd'.log'
log4j.appender.file.layout=org.apache.log4j.PatternLayout
log4j.appender.file.layout.ConversionPattern=%d %5p [%t] %c %l - %m%n

log4j.appender.errorfile=com.genius.server.base.exception.ExceptionFileAppender
log4j.appender.errorfile.File=error.log
log4j.appender.errorfile.Threshold=ERROR
log4j.appender.errorfile.layout=org.apache.log4j.PatternLayout
log4j.appender.errorfile.layout.ConversionPattern= [finance.supergenius.cn] %d %5p [%t] %c %l - %m%n

log4j.appender.comfile=org.apache.log4j.DailyRollingFileAppender
log4j.appender.comfile.File=com.log
log4j.appender.comfile.DatePattern='.'yyyy-MM-dd'.log'
log4j.appender.comfile.layout=org.apache.log4j.PatternLayout
log4j.appender.comfile.layout.ConversionPattern=%d %5p [%t] %c %l - %m%n

log4j.appender.webfile=org.apache.log4j.DailyRollingFileAppender
log4j.appender.webfile.File=web.log
log4j.appender.webfile.DatePattern='.'yyyy-MM-dd'.log'
log4j.appender.webfile.layout=org.apache.log4j.PatternLayout
log4j.appender.webfile.layout.ConversionPattern=%d %5p [%t] %c %l - %m%n

log4j.appender.emailfile=org.apache.log4j.DailyRollingFileAppender
log4j.appender.emailfile.File=email.log
log4j.appender.emailfile.DatePattern='.'yyyy-MM-dd'.log'
log4j.appender.emailfile.layout=org.apache.log4j.PatternLayout
log4j.appender.emailfile.layout.ConversionPattern=%d %5p [%t] %c(%F:%L) - %m%n

log4j.appender.serialfile=org.apache.log4j.DailyRollingFileAppender
log4j.appender.serialfile.File=serial.log
log4j.appender.serialfile.DatePattern='.'yyyy-MM-dd'.log'
log4j.appender.serialfile.layout=org.apache.log4j.PatternLayout
log4j.appender.serialfile.layout.ConversionPattern=%d %5p [%t] %c(%F:%L) - %m%n

log4j.appender.amqpfile=org.apache.log4j.DailyRollingFileAppender
log4j.appender.amqpfile.File=amqp.log
log4j.appender.amqpfile.DatePattern='.'yyyy-MM-dd'.log'
log4j.appender.amqpfile.layout=org.apache.log4j.PatternLayout
log4j.appender.amqpfile.layout.ConversionPattern=%d %5p [%t] %c(%F:%L) - %m%n

log4j.appender.timerfile=org.apache.log4j.DailyRollingFileAppender
log4j.appender.timerfile.File=timer.log
log4j.appender.timerfile.DatePattern='.'yyyy-MM-dd'.log'
log4j.appender.timerfile.layout=org.apache.log4j.PatternLayout
log4j.appender.timerfile.layout.ConversionPattern=%d %5p [%t] %c(%F:%L) - %m%n

log4j.appender.apifile=org.apache.log4j.DailyRollingFileAppender
log4j.appender.apifile.File=api.log
log4j.appender.apifile.DatePattern='.'yyyy-MM-dd'.log'
log4j.appender.apifile.layout=org.apache.log4j.PatternLayout
log4j.appender.apifile.layout.ConversionPattern=%d %5p [%t] %c(%F:%L) - %m%n

