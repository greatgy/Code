    <script type="text/javascript">
	var debateuid = "${bean.uid}";
	var hasvote = -1;// 用来在页面级别存储已经投过票，防止多次请求后台
	var isred ;
	(function() {
		$(document).ready(function() {
			var balancePanel = new Components.BalancePanel($('.tp_midd'), {
				left : ${(bean.countred)?c},
				right : ${(bean.countblue)?c}
			});
		});
	})();
	function alertvote(isred){
		if(isred){
			alert("你已经投票给红方");
		}else{
			alert("你已经投票给蓝方");
		}
		handleHasvote(isred);
	}
	
	function alertisred(isred){
	}
	
	function handleHasvote(isred){
		if(isred){
			hasvote = 1;
		}else{
			hasvote = 0;
		}
		//$(".has_vote_box").css("display", "block");
	}
	
	$(function($) {
		// 为评论按钮注册点击事件
		$(".submit_comment").bind("click", function(){
			submitCommentHandler(this);
		});  
		
	});
	
	var params = {
		content: "",
		fromuid: "",
		channel: ""
	}
	
	var commentdefault = {
		maxlength: 300
	}
	
	var me = {
		showname: "${me.showname!'游客'}",
		imglittle: "${me.imglittle}",
		oid:"${me.oid}",
		defaultImg: ""
	}
	
	function beforeSubmitHandler(obj){
		params = {};
		var vote = $(obj).data("vote");
		if(vote == "red"){
			isred = true;
		}else{
			isred = false;
		}
		var commenttext = $(obj).siblings(".comment_textarea").val();
		params.isred = isred;
		params.fromuid = '${bean.uid}';
		params.channel = "debate";
		params.content = commenttext;
		var touid = $(obj).parent().parent().data("uid");
		if(typeof touid != "undefined"){
			params.touid = touid;
		}
		var topuid = $(obj).parent().parent().data("topuid");
		if(typeof topuid != "undefined"){
			params.topuid = topuid;
		}
		var touseruid = $(obj).parent().parent().data("touseruid");
		if(typeof touseruid != "undefined"){
			params.touseruid = touseruid;
		}
		
		var tousername = $(obj).parent().parent().data("tousername");
		if(typeof tousername != "undefined"){
			params.tousername = tousername;
		}
	    var fromusername = $("input[name='fromusername']").last().val();
        if(fromusername != ""){
            params.fromusername = fromusername;
        }
	}
	
	function afterSubmitHandler(result, obj){
		if(result.success){
			$(".comment_textarea").val("")
			if(typeof params.touid != "undefined"){
				addListComment(obj, result.bean);
			}else{
				$(".comments").html(getCommentHtml(result.bean, obj) + $(".comments").html());
			}
			$(".commentform").remove();
			registerPraise();
		}else{
			alert("添加失败");
		}
	}
	
	// 对评论的回复
	function addListComment(obj, comment){
		var content = "";
		// 还没有回复
		if($(obj).parent().parent().siblings(".contro_pnei_z").length == 0 && $(obj).parent().parent().attr("class").indexOf("reply_detail") == -1){
			content += '<div class="contro_pnei_z">';
		  	content += '<div class="contro_borer">';
		  	if(params.isred){
		  		content += '<img src="${baseimg}/imgs/default/lzh_02_1.jpg">';
		  	}else{
		  		content += '<img src="${baseimg}/imgs/default/lzh_02_2.jpg">';
		  	}
		  	content += '</div>';
		  	content += '<div class="contro_pnei">';
			content += '<div class="left contro_red_hui comment_detail">';
			content += getdetail(comment);
			content += '</div></div></div>';
			$(obj).parent().parent().parent().append(content.toString());					
		}else{// 已经有回复
			if($(obj).parent().parent().attr("class").indexOf("comment_detail") != -1){
				// 直接回复的评论
				content += $(obj).parent().parent().siblings(".contro_pnei_z").children(".contro_pnei").html() + getdetail(comment);
				$(obj).parent().parent().siblings(".contro_pnei_z").children(".contro_pnei").html(content);
			}else{
				// 对评论的回复的回复
				content += $(obj).parent().parent().parent().html() + getdetail(comment);
				$(obj).parent().parent().parent().html(content);				
			}		
		}
	}
	
	function getdetail(comment){
		var topuid = "";
		if(typeof params.topuid == "undefined"){
			topuid = comment.touid;
		}else{
			topuid = params.topuid;
		}
		var content = "";
		content += '<div class="reply_detail" data-uid="'+ comment.uid +'" data-topuid="'+ topuid +'" data-touseruid="'+ params.touseruid +'" data-tousername="'+ comment.fromusername +'" >';
		if(params.isred){
		    content += '<span class="contro_red_name">'+comment.fromusername+'&nbsp;回复&nbsp;'+comment.tousername+'<span>&nbsp;&nbsp;刚刚</span></span><br/>';
			content += '<span class="contro_nei">'+comment.content+'</span><br />';
			content += '<span class="reply"><a href="javascript:void(0)" id="btnprizecomments_${(me.oid)!"0"}_'+comment.uid+'" data-counter=".prizecounter">'
				    + '<img src="${baseimg}/imgs/default/heart_02.png"/><span class="prizecounter">(0)</span></a></span>';
		}else{
			content += '<span class="contro_bule_name">' + comment.fromusername+'&nbsp;回复&nbsp;'+comment.tousername+'<span>&nbsp;&nbsp;刚刚</span></span><br/>';						    
		  	content += '<span class="contro_nei">'+comment.content+'</span><br />';
			content += '<span class="reply"><a href="javascript:void(0)" id="btnprizecomments_${(me.oid)!"0"}_'+comment.uid+'" data-counter=".prizecounter">'
				    + '<img src="${baseimg}/imgs/default/heart_02.png"/><span class="prizecounter">(0)</span></a></span>';
		}
		content += '<span class="reply"><a href="javascript:void(0)" onclick="reply(this);">回复</a></span>';						  	
		content += '</div>';
		return content;
	}
	
	//对论战的评论
	function getCommentHtml(bean, obj){
		 var result = "";
		 if(isred){
		 	result += "<div class='contro_pl text_left'>";
		 	result += '<div class="comment_detail" data-uid="'+ bean.uid +'" data-touseruid="' + bean.fromuseruid +'" data-tousername="'+bean.fromusername+'">';
		 	if(me.imglittle == ""){
		 		result += '<img class="contr_pic_t left" src="${baseimg}/imgs/web/noavatar.png" />';
		 	}else{
		 		result += '<img class="contr_pic_t left" src="${userimg}'+ me.imglittle + '">';
		 	}
	  		result += "<span class='contro_red_name'>"+bean.fromusername+" </span>";
	  		result += "<span>刚刚</span>";
	  		result += "<br /><span class='contro_nei'>"+params.content+"</span><br />";
	  		result += '<span class="reply"><a href="javascript:void(0)" id="btnprizecomments_${(me.oid)!"0"}_'+bean.uid+'" data-counter=".prizecounter">'
				    + '<img src="${baseimg}/imgs/default/heart_02.png"/><span class="prizecounter">(0)</span></a></span>';
	  	}else{
		 	result += "<div class='contro_pl text_right'>";
		 	result += '<div class="comment_detail" data-uid="'+ bean.uid +'" data-touseruid="' + bean.fromuseruid +'" data-tousername="'+bean.fromusername+'">';
	 		if(me.imglittle == ""){
		 		result += '<img class="contr_pic_t right" src="${baseimg}/imgs/web/noavatar.png" />';
		 	}else{
		 		result += '<img class="contr_pic_t right" src="${userimg}'+ me.imglittle + '">';
		 	}
	  		result += "<span class='contro_blue_name'>"+bean.fromusername+" </span>";
	  		result += "<span>刚刚</span>";
	  		result += "<br /><span class='contro_nei'>"+params.content+"</span><br />";
	  		result += '<span class="reply"><a href="javascript:void(0)" id="btnprizecomments_${(me.oid)!"0"}_'+bean.uid+'" data-counter=".prizecounter">'
				    + '<img src="${baseimg}/imgs/default/heart_02.png"/><span class="prizecounter">(0)</span></a></span>';
	  	}
  		result += "<span class='reply'><a href='javascript:void(0)' onclick='reply(this);'>回复</a></span><br/>"
  		result += "</div></div>";
  		return result;	
	}
	
	function submitCommentHandler(obj){
		beforeSubmitHandler(obj);
		if(params.content == ""){
			alert("评论内容不能为空");
			return;
		}
		if(params.content.length >= commentdefault.maxlength){
			alert("评论内容最多" + commentdefault.maxlength +　"个字");
			return;
		}
		$.post("${base}/ajax/comment/{0}_{1}".format(params.channel, params.fromuid), params,function(data){
			afterSubmitHandler(data, obj);
		});
	}
	
	function reply(obj){
		if($(".commentform").length != 0) {
			if(!confirm("您确定要放弃现在的评论吗？")){
				$(".commentform")[0].focus();
				return;
			}else{
				$(".commentform").remove();
			}
		}
		$(obj).parent().parent().append($("#commentform").clone().addClass("commentform"));
		$(".comment_textarea", $(".commentform")).focus();
		$(".comment_textarea", $(".commentform")).bind("blur", function(event){
			if($(this).val() == ""){
				$(this).parent().remove();
			}
		});
		$(".submit_comment").bind("click", function(){
			submitCommentHandler(this);
		});
	}
	
	// 对评论的回复的加载更多
	function loadlist(obj){
		var replys = $(obj).siblings(".hd");
		for(var i=replys.length;i>=replys.length-5;i--){
			if(i>=0){
				$(replys[i]).removeClass("hd");
			}
		}
		replys = $(obj).siblings(".hd");
		if(replys.length == 0) {
			$(obj).addClass("hd");
		}
	}
	
	function registerPraise(){
		$(".reply a[id^='btnprize']").each(function(){$(this).prize()});
	}
	
    </script>
