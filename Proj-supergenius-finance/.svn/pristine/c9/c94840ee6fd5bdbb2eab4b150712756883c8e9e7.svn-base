	<script type="text/javascript">
		$(function(){
			 <!-- 搜索框获得焦点，选项出现--> 
			$("#txtsearch").focus(function(){
				$(".searchlist").show();
			});
			$("#txtsearch").blur(function(){
				$(".searchlist").hide();
			});
			 <!-- 切换导航选中状态--> 
			$(".sidenav li a").click(function(){
				$(this).parent().siblings().find("a").removeClass("activelink");
				$(this).addClass("activelink");
			});
			<!-- 滚动条滚动，导航固定位置--> 
			$(window).scroll(function(){
				var scrollTop = $(window).scrollTop();
				if(scrollTop > 42) {
					$(".header_tou .top").hide();
					$(".header_tou .top_02").css({position:"fixed",top:"0"});
					$(".sidenav").css("top","59px");
				} else {
					$(".header_tou .top").show();
					$(".header_tou .top_02").css({position:"static"});
					$(".sidenav").css("top","96px");
				}	
			});

			/* 一级导航新消息部分点击切换 */
			$(".newsnum").click(function(){
				$(this).parent().find(".newsection").toggleClass("hd");
			});
			/* 点击删除可以忽略消息 */
			$(".del").click(function(){
				$(this).parent().remove();
			});
			
		});

