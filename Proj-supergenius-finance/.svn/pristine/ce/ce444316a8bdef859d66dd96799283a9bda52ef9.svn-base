                [#if item.fromUser??]
                    <a href="${basesns}/p/${item.fromUser.oid}/home" target="_blank">
                [#else]
                <a href="javascript:void(0);">
                [/#if]
                    ${item.fromusername}
                </a>&nbsp;&nbsp;${item.createtimeStr!''}</h3>
				<p class="com_con">${item.content?html!''}</p>
				<p class="com_reply">
					<span class="time"></span>
					<span class="reply">
						<a href="javascript:void(0)" id="btnprizecomments_${(me.oid)!"0"}_${item.uid}" data-counter=".prizecounter">
							<img src="${baseimg}/imgs/default/heart_02.png" data-isprize="${item.isprize!'false'}"/><span class="prizecounter">(${item.prizecount!'0'})</span>
						</a>
					</span>
					<a href="javascript:void(0);" onclick="reply(this);" data-toname="${item.fromusername}">回复</a>
				</p>
			</div>
			<div class="clear"></div>
		</div>
	[/#list]
[/#if]
