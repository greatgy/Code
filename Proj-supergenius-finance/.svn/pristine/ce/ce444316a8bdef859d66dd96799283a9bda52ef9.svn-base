[#if channel == "finance"]
	[#list list?reverse as item]
	<div class="com_list">
		[#if item.fromUser??]
            <a href="${basesns}/p/${item.fromUser.oid}/home" target="_blank">
        [#else]
            <a href="javascript:void(0);">
        [/#if]
			<img width="45px" height="45px" class="left" [#if "${item.fromUser.avatar}" == ""]src="${baseimg}/imgs/web/noavatar.png"[#else]src="${userimg}${item.fromUser.avatar}"[/#if] />
		</a>
		<div class="com_info right" data-uid="${item.uid}" data-touseruid="${item.fromuseruid}"  data-tousername="${item.fromusername}" >
			<h3>
			[#if item.fromUser??]
                <a href="${basesns}/p/${item.fromUser.oid}/home" target="_blank">
            [#else]
                <a href="javascript:void(0);">
            [/#if]
            ${item.fromusername}
            </a>
                                    ：${item.content!''}</h3>
			<div class="com_con"> </div>
			<div class="com_reply">
				<span style="float:left;">${item.createtimeStr}</span>
				<span class="reply">
					<a href="javascript:void(0)" id="btnprizecomments_${(me.oid)!"0"}_${item.uid}" data-counter=".prizecounter">
						<img src="${baseimg}/imgs/default/heart_02.png" data-isprize="${item.isprize!'false'}"/><span class="prizecounter">(${item.prizecount!'0'})</span>
					</a>
				</span>
				<a href="javascript:void(0);" class="reply" onclick="reply(this)">回复</a>
			</div>
		</div>
		<div class="clear"></div>
		<div class="com_hide right">
		[#if item.replys?size>0]
			[#if item.replys?size > 3]
			<a class="com_but" href="javascript:void(0);" class="showmore" onclick="showmore(this)"><span >展开剩余${item.replys?size-3}条评论↓</span></a>
			[/#if]
			[#list item.replys as item2]
			[#if item2_index < item.replys?size-3]
				<div class="com_list com_listInner hd">
			[#else]
				<div class="com_list com_listInner">
			[/#if]
				[#if item.fromUser??]
                    <a href="${basesns}/p/${item2.fromUser.oid}/home" target="_blank">
                [#else]
                <a href="javascript:void(0);">
                [/#if]
					<img width="45px" height="45px" class="left" [#if "${item2.fromUser.avatar}" == ""]src="${baseimg}/imgs/web/noavatar.png"[#else]src="${userimg}${item2.fromUser.avatar}"[/#if]>
				</a>
				<div class="com_info right com_short" data-topuid="${item.uid}" data-uid="${item2.uid}" data-touseruid="${item2.fromuseruid}"  data-tousername="${item2.fromusername}"  >
					<h3>
						[#if item.fromUser??]
                            <a href="${basesns}/p/${item2.fromUser.oid}/home" target="_blank">
                        [#else]
                        <a href="javascript:void(0);">
                        [/#if]${item2.fromusername}</a>
							回复
						[#if item.fromUser??]
                            <a href="${basesns}/p/${item2.toUser.oid}/home" target="_blank">
                        [#else]
                        <a href="javascript:void(0);">
                        [/#if]${item2.tousername}</a>
                                                                        ：<span class="com_con">${item2.content!''}</span></h3>
					<p class="com_reply">
						<span style="float:left;">${item2.createtimeStr}</span>
						<span class="reply">
							<a href="javascript:void(0)" id="btnprizecomments_${(me.oid)!"0"}_${item.uid}" data-counter=".prizecounter">
								<img src="${baseimg}/imgs/default/heart_02.png" data-isprize="${item.isprize!'false'}"/><span class="prizecounter">(${item.prizecount!'0'})</span>
							</a>
						</span>
						<a href="javascript:void(0);" class="reply" onclick="reply(this)">回复</a>
					</p>
				</div>
			</div>
			[/#list]
		[/#if]
		</div>
		<div class="clear"></div>
	</div>
	[/#list]
[/#if]

[#if channel == "topic"]
	[#list list as item]
		[#if item.fromuseruid != me.uid]
