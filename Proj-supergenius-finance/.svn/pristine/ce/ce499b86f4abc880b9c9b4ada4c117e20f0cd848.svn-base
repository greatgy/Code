	<script type="text/javascript">
		$(function(){
			<!-- 文章状态切换-->
			$(".tablist li").click(function(){
				var curIndex = $(this).index();
				$(this).siblings().removeClass("curtab");
				$(this).addClass("curtab");
				var curTpclists = $("#topicbox" + parseInt(curIndex + 1));
				curTpclists.show();
				curTpclists.siblings().hide();
			});
			<!-- 话题状态切换-->
			$(".topictab a").click(function(){
				$(this).siblings().removeClass("curtopic");
				$(this).addClass("curtopic");
			});
		});
	</script>
