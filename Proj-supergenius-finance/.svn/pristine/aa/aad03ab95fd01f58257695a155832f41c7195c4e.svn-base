body { min-width: 1100px;}
/* 底部bottom开始 */
.bottom{width: 100%;height: 220px;background-color: #323030;clear:both;font-family: "Microsoft Yahei";}
.bottom_content { width: 800px;padding-top: 45px;}
.bottom_top { border-bottom: 1px dashed #fff;padding-bottom: 15px;}
.bottom_top img { margin-right: 60px;}
.bottom p{text-align:center;font-family: "Microsoft Yahei";}
.bot_01{color: #fff;margin-top: 15px;}
.bot_01 a{color: #fff;font-size: 13px;margin: 16px;font-family: "Microsoft Yahei";}
.bot_01 a:hover { color: #ff0000;}
.bot_02{color: #9c9b9b;font-size: 13px;margin-top: 10px;margin-bottom: 15px;}
.spare { color: #fff;vertical-align: middle;font-size: 13px;font-family: "Microsoft Yahei";}
.spare img { width: 35px;height: 35px;overflow: hidden;vertical-align: middle;margin-left: 5px;}
.wx_code { display: none; position: relative;width: 200px;height: 200px;top: -240px;left: 80px;background: url("../../imgs/default/wx_bg.png") no-repeat;}
.app_img { width: 150px;height: 150px;overflow: hidden;position: absolute;top: 15px;left: 25px;}
#bctop{width:60px;height:60px;background:url("../../imgs/default/bctop.png") no-repeat;cursor:pointer; position:fixed;bottom:20%;right:10%;clear:both;display:none;}

/*面包屑begin*/
.crumbs_p{ width: 1000px; margin: 0px auto; }
.crumbs{ margin-top:20px;}
.crumbs img{margin-right: 5px;width: 16px;height: 16px;vertical-align: middle;}
.crumbs .crumbs_arrow{ color:#999999; font-size:14px;}
.crumbs a{ font-size: 16px;color: #999999;vertical-align: middle;}
.crumbs a:hover { color: #0BA0A9;}
.crumbs .crumbtitle { font-size: 15px;color: #999999;vertical-align: middle;}
/*面包屑over*/
#file{  position: absolute; margin-left: -120px;margin-top: 3px; opacity: 0}

/*右侧排行榜*/
.list_hide{ display:none;text-align:left;font-size:12px;font-family:"宋体";color:#727374;letter-spacing: 1px;line-height:20px; min-height:60px;}

.db_link{ text-align:right;float:right;margin-right:40px; position:relative;}

/*话题排行榜样式*/
.tp_07_right .tp_07_two .twomain h2{ color:#344353; font-size:14px;text-align: left; margin-left: 20px;}
.tp_07_right .tp_07_two .twomain h2 a{color:#344353;}
.tp_07_right .tp_07_two .twomain h2 a:hover{color:#1d71b6;}
.tp_07_right .tp_07_two .twomain p{color:#777; margin-left: 20px;font-family:"宋体"; margin-top: 7px; font-size:12px;text-align: left; display: block; }
.tp_07_right .tp_07_two .twomain p em{font-style: normal;cursor:pointer; }
.tp_07_right .tp_07_two .twomain p em:hover{color:#3698dc;}
.dhk{position: absolute; right: 20px;top:7px;width:36px; height: 32px;line-height: 28px; color:#fff;font-size:12px; background:url(../../imgs/default/msg_icon01.png) top left no-repeat; }


/*轮播面板*/
.broadcast {position: relative; width: 100%; height: 100%;}
.broadcast .broadcast-center {display:block;width:100%; height: 77%;}
.broadcast .broadcast-center > img {width: 100%; height: 100%;}
.broadcast .broadcast-previous {position: absolute; left: 0px; top:32%; width: 70px; height: 80px;opacity: .5;filter: alpha(opacity=50);}
.broadcast .broadcast-next { position: absolute; right: 0px; top:32%; width: 70px; height: 80px;opacity: .5;filter: alpha(opacity=50);}
.broadcast .broadcast-previous img , .broadcast .broadcast-next img { width: 50px;height: 60px;overflow: hidden;}
.broadcast-titlecover {background: #000000;position: absolute;top: 65%;width: 100%;height: 55px;opacity: .5;}
.broadcast-link {position: absolute; top: 65%; left: 5%;}
.broadcast-link a {color: #FFF; font-size: 18px;display:block;width:600px;height: 55px;line-height: 55px;overflow:hidden;text-align:center;}
.broadcast-link a:hover {text-decoration: underline;}
.broadcast-bottom {width: 100%; height: 24%;}
.broadcast-bottom div {position: relative; float: left; width: 170px; border: 1px solid #000; height: 100%;}
.broadcast-bottom img {width: 100%; height: 100%;}
.broadcast-bottom div[index] {overflow: hidden;}
.broadcast-bottom .broadcast-cover {position: absolute; top: 0px; left: 0px; width: 100%; height: 100%; background: rgba(58,160,230,.9); filter: progid:DXImageTransform.Microsoft.gradient(enabled='true', startColorstr='#903aa0e6', endColorstr='#903aa0e6')\9; transition: top .3s ease-out;}
.broadcast-bottom .broadcast-cover span {position: absolute; top: 0px; left: 0px; padding: 20px 25px 0px; font-size: 16px; color: #FFF; text-align:left;}
/*轮播面板结束*/

/*天平面板*/
.balance {position: relative; width: 100%; height: 100%;}
.balance h4 {font-size: 30px; color: #344353; position: relative; margin: auto; top: 160px; width:300px;}
.balance .number-area {display: block;font-size: 18px;text-align: center;cursor: pointer;margin-top: 3px;}
.balance .left-data {position: absolute;left: -65px;width: 80px;height: 85px;transition: top .3s ease-out;z-index: 1000;color: #fff;text-align: center;}
.balance .left-arrow {margin: auto;}
.balance .left-number {color: #b92727;text-align: center;}
.balance .right-data {position: absolute;top: 75px;right: -60px;width: 80px;height: 85px;transition: top .3s ease-out;z-index: 1000;text-align: center;}
.balance .right-arrow {margin: auto;}
.left-arrow img , .right-arrow img { width: 55px;height: 55px;overflow: hidden;}
.balance .right-number {color: #0966c8;text-align: center;}
.balance .balance-lever {position: absolute;height: 20px;top: 135px;width: 820px;left: 60px;margin: 0 auto;transition: transform .3s ease-out;padding: 0 10px;}
.balance .left-lever {display: inline-block;width: 50%;height: 100%;background: #b92727;transition: width .3s ease-out;float: left;border-radius: 6px;}
.balance .right-lever {display: inline-block;width: 50%;height: 100%;background: #0966c8;transition: width .3s ease-out;border-radius: 6px;}
.balance .balance-pivot {position: relative;top: 155px;margin: auto;width: 95px;height: 52px;overflow: hidden;}
/*天平面板结束*/

