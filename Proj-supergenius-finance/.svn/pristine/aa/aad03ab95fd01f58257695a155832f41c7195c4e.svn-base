/******************导航js效果********************/
.box-nav {position: absolute;top:-20px;right:-20px;height:35px;width: 850px;padding:10px;margin: 0px auto;overflow: hidden;font-size:14px;float:right;}
.box-nav li {float:left;padding-left:31px;padding-bottom:10px; }

.box-nav li:last-child{ background:none;}
.box-nav li.back {border-bottom: 6px solid #1d71b6;height:30px;z-index: 8;position:absolute;}
.box-nav li a {color:#344353; font-weight:bold;outline: none;text-align: center;top: 7px;text-transform: uppercase;letter-spacing: 0;z-index: 10;display: block;float: left;height: 30px;position: relative;overflow: hidden;margin: auto 4px;}
.box-nav li a:hover{color:#1d71b6;border: none;}
/*.box-nav li a:visited{color:#344353;border: none;}
.box-nav li a:hover, .box-nav li a:active, .box-nav li a:visited {border: none;} */
.box-nav li img{ position: relative; left:18px; top:4px;}
