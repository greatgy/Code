@charset "utf-8";
/* CSS Document */
*{ margin:0 auto;}
html { -webkit-text-size-adjust:none; }
body{margin:0; padding:0; font-family:"微软雅黑"; text-align:center;}
ul,ul li{ margin:0; padding:0; list-style-type:none;}
.left{ float:left;}
.right{ float:right;}
.clear{ clear:both;}
a{text-decoration:none;}
img{border: 0; }
.hd{display: none;}
#wrapper { background-color: #f0f0f0;}
/******************以上为公共样式********************/

.header_tou .top{width:100%; height:37px; background-color:#4e4d4d;}
.header_tou .top .top_main1{width:1023px;}
.header_tou .top .top_left{list-style:none; float:left; }
.header_tou .top .top_left li{float:left;}
.header_tou .top .top_left li a{font-size:12px; color:#b8b8b8; display:block; margin:10px 15px;}
.header_tou .top .top_left li a:hover{color:#fff;}
/* .header_tou .top .top_left li a:visited{color:#b8b8b8;} */
.top .top_left .tl_act{color:#FFF!important;}
.top .top_right{list-style:none; float:right;}
.top_right li{float:left;}
.top_right li a{font-size:12px; color:#ccc; display:block; margin:10px 14px;}
.top_right .tr_act{width:55px; height:39px; background-color:#d93126;}
.top_right li a:hover { color: #fff;}
.top_right li .newsnum {
	position: relative;
	display: inline-block;
}
.top_right li .newsnum:hover {
	text-decoration: underline;
}
.newsnotice .number {
	color: red;
}
.newsnum .rdot {
	display: block;
	width: 6px;
	height: 6px;
	background: red;
	border-radius: 50%;
	position: absolute;
	top: 0;
	right: -4px;
	overflow: hidden;
}
.newsnotice .newsection {
	position: absolute;
	background: #fff;
	z-index: 10;
	width: 235px;
	margin-top: 15px;
	margin-left: -75px;
	border: 1px solid #E2E2E2;
}
.newsection .trangle {
	height: 24px;
	margin-left: 58%;
	position: absolute;
	display: block;
	top: -23px;
}
.trangle em , .trangle i {
	width: 0;
	height: 0;
	font-size: 0;
	line-height: 0;
	display: block;
	position: absolute;
	border: 12px solid transparent;
	border-style: dashed dashed solid dashed;
}
.trangle em {
	border-bottom-color: #CACACA;
	top: -1px;
}
.trangle i {
	border-bottom-color: #fff;
	top: 0;
}
.newsection .datalist {
	background: #fff;
}
.newsection .datalist li {
	height: 40px;
	text-align: left;
	width: 215px;
	line-height: 40px;
	padding: 0 10px;
	border-bottom: 1px solid #ddd;
	color: #535353;
	overflow: hidden;
}
.newsection .datalist li:last-child {
	border-bottom: none;
}
.datalist li a {
	display: inline-block;
	margin: 0;
	color: #535353;
}
.datalist li .del {
	display: inline-block;
	width: 20px;
	height: 20px;
	overflow: hidden;
	text-align: center;
	line-height: 20px;
	float: right;
	margin-top: 10px;
}
.datalist .del img {
	width: 20px;
	height: 20px;
	overflow: hidden;
}
.newsection .datalist li:hover {
	background: #F1F1F1;
}
.newsection .datalist li:hover a {
	color: #A6A6A6;
}
.datalist pre {
	color: #535353;
	font-size: 12px;
	text-align: left;
	padding: 10px;
	font-family: "Microsoft Yahei";
	white-space: normal;
	overflow: hidden;
}
.datalist pre p {
	line-height: 20px;
	margin-bottom: 5px;
}
.datalist pre .authorname {
	display: block;
	text-align: right;
}
.datalist .nonews {
	font-size: 12px;
	color: #AEAEAE;
	margin-bottom: 10px;
}

.header_tou .top_02{
	width: 100%;
	height: 58px;
	text-align: left;
	background: #fff;
	z-index: 900;
	box-shadow: 1px 0 2px #646e83;
	margin-bottom: 2px;
}
.top_02 .logoicon {
	width: 150px;
	height: 58px;
	overflow: hidden;
	padding-left: 30px;
}
.top_02 .logoTitle {
	display: inline-block;
	height: 58px;
	line-height: 52px;
	padding-left: 20px;
	vertical-align: top;
	font-size: 24px;
	color: #375771;
	font-size: 21.54px;
	font-family: "隶书";
	font-style: italic;
	color: #1d71b6;
}
.logoTitle .bgtitle {
	display: inline-block;
	width: 98px;
	height: 30px;
	background: #1d71b6;
	vertical-align: middle;
	font-size: 21.5px;
	font-family: "JLinXin";
	font-weight: bolder;
	font-style: normal;
	color: #fff;
	line-height: 30px;
	text-align: center;
	overflow: hidden;
}
.top_02 .searchUser {
	display: inline-block;
	float: right;
	position: absolute;
	right: 0;
	height: 58px;
	width: 440px;
	line-height: 60px;
	padding-left: 5px;
}

#searchform {
	display: inline-block;
	vertical-align: top;
	padding-top: 0;
}
#txtsearch {
	width: 202px;
	height: 28px;
	border-radius: 8px;
	background: #e4e6eb;
	border: none;
	box-shadow: inset 1px 1px 1px #C5C6C6;
	line-height: 20px;
	font-size: 14px;
	padding-left: 10px;
	color: #959596;
	font-family: "Microsoft Yahei";
	overflow: hidden;
	padding-right: 30px;
}
#txtsearch:focus{
	box-shadow: inset 0 0 4px #3091E1;
	outline: none;
}
#searchform .searchimg {
	display: inline-block;
	width: 30px;
	height: 30px;
	background: url(/imgs/default/search.png) no-repeat center center;
	margin-left: -33px;
	vertical-align: middle;
	box-sizing: content-box;
	overflow: hidden;
}
.actsearch {
	background: url(/imgs/default/bsearch.png) no-repeat center center;
}

.searchUser .contribute {
	width: 85px;
	height: 30px;
	display: inline-block;
	background: #1d71b6;
	text-align: center;
	line-height: 30px;
	border-radius: 6px;
	font-size: 16px;
	color: #fff;
	letter-spacing: 3px;
	margin: 0 15px;
	overflow: hidden;
	vertical-align: middle;
	margin-top: -2px;
}
.searchUser .contribute:hover {
	background: #7ecef4;
}
.searchUser .searchlist {
	width: 230px;
	height: 129px;
	border: 1px solid #dbdbdb;
	position: absolute;
	top: 45px;
	left: 10px;
	z-index: 10;
	background: #fff;
	text-align: left;
	display: none;
}
.searchlist li {
	height: 32px;
	line-height: 32px;
	color: #767676;
	background: #fff url(/imgs/default/searchlistimg.png) no-repeat 10px center;
	padding-left: 30px;
	font-size: 12px;
	overflow: hidden;
}
.searchlist li:hover {
	background: #e5e5e5 url(/imgs/default/searchlistimg.png) no-repeat 10px center;
	cursor: pointer;
}
.searchlist .slistli01, .searchlist .slistli01:hover {
	background-position: 10px 6px;
}
.searchlist .slistli02, .searchlist .slistli02:hover  {
	background-position: 10px -25px;
}
.searchlist .slistli03, .searchlist .slistli03:hover  {
	background-position: 10px -55px;
}
.searchlist .slistli04, .searchlist .slistli04:hover  {
	background-position: 10px -84px;
}
.searchlist span {
	font-size: 14px;
	color: #6b6b6b;
}
.searchlist span i {
	font-style: normal;
	color: #267BC1;
}
.searchlist li section {
	max-width: 120px;
	display: inline-block;
	overflow: hidden;
	height: 32px;
	vertical-align: top;
}

.searchUser .userbox {
	width: 55px;
	height: 55px;
	display: inline-block;
	float: right;
	margin-right: 15px;
}
.yesuser , .notuser {
	width: 55px;
	height: 55px;
	position: relative;
	vertical-align: top; 
}
.userbox .userimg {
	width: 45px;
	height: 45px;
	overflow: hidden;
	vertical-align: top;
	border-radius: 50%;
	margin-top: 6px;
}
.yesuser .reddot {
	width: 8px;
	height: 8px;
	border-radius: 100%;
	display: block;
	background: #ff0000;
	position: absolute;
	top: 11px;
	right: 4px;
	overflow: hidden;
}
.userselfbox {
	width: 135px;
	position: absolute;
	z-index: 11;
	right: -2px;
	padding-top: 4px;
	margin-top: -8px;
	overflow: hidden;
	display: none;
}
.userselfbox .trangle {
	width: 0px;
	height: 0px;
	border-style: solid;
	border-width: 0px 12px 16px 12px;
	border-color: transparent transparent #fff transparent;
	margin-left: 70%;
}
.userselfcontent {
	width: 135px;
	height: 140px;
	background: #fff;
	border-left: 1px solid #dcdcdc;
	border-right: 1px solid #dcdcdc;
	border-bottom: 1px solid #dcdcdc;
	overflow: hidden;
	padding-bottom: 5px;
}
.userselfcontent .btncontainer {
	font-size: 14px;
	line-height: 20px;
	margin: 15px auto 5px;
	text-align: center;
}
.btncontainer a {
	width: 50px;
	height: 20px;
	display: inline-block;
	border: 1px solid #a8a8a8;
	text-align: center;
	line-height: 20px;
	color: #6a6a6a;
	overflow: hidden;
}
.btncontainer a:hover {
	border: 1px solid #86b3d8;
	color: #719cca;
}
.btncontainer .contribute {
	margin-right: 8px;
}
.userselfcontent .userights {
	font-size: 14px;
	color: #333333;
	text-align: center;
	overflow: hidden;
	padding-bottom: 5px;
	margin-top: 10px;
}
.userights li {
	height: 32px;
	width: 133px;
	line-height: 32px;
	position: relative;
	text-align: left;
}
.userights li a {
	font-size: 14px;
	color: #333333;
	display: block;
	padding-left: 25px;
}
.userights li a:hover {
	color: #1d71b6;
	border: 1px solid #9ABDD9;
	cursor: pointer;
}


.userights li span {
	display: inline-block;
	width: 25px;
	height: 14px;
	background: #ff0000;
	font-size: 10px;
	color: #fff;
	line-height: 14px;
	border-radius: 5px;
	text-align: center;
	position: absolute;
	top: 9px;
	right: 20px;
	overflow: hidden;
}
.yesuser:hover .userselfbox{
	display: block;
}
.sidenav {
	position: fixed;
	z-index: 900;
	left: 0;
	top: 97px;
	width: 11%;
	box-shadow: 1px 1px 3px #c8c8c8;
	font-size: 18px;
	text-align: left;
	background: #1d71b6;
	height: 100%;
}
.sidenav li {
	height: 55px;
	line-height: 55px;
	overflow: hidden;
	border-bottom: 1px solid #6d97ba;
}
.sidenav li a {
	display: block;
	background: #1d71b6 url(/imgs/default/sidebarimg.png) no-repeat 15px center;
	color: #fff;
	padding-left: 55px;
}
.sidenav li a:hover {
	color: #fff;
	background: #8cc9fb url(/imgs/default/sidebarimg.png) no-repeat 15px center;
}
.sidenav .sidea01 , .sidenav .sidea01:hover {
	background-position: 15px 12px !important;
}
.sidenav .sidea02 , .sidenav .sidea02:hover {
	background-position: 15px -42px !important;
}
.sidenav .sidea03 , .sidenav .sidea03:hover {
	background-position: 15px -92px !important;
}
.sidenav .sidea04 , .sidenav .sidea04:hover {
	background-position: 15px -145px !important;
}
.sidenav .sidea05 , .sidenav .sidea05:hover {
	background-position: 15px -200px !important;
}
.sidenav .sidea06 , .sidenav .sidea06:hover {
	background-position: 15px -252px !important;
}
.sidenav .sidea07 , .sidenav .sidea07:hover {
	background-position: 15px -308px !important;
}
.sidenav .sidea08 , .sidenav .sidea08:hover {
	background-position: 15px -360px !important;
}
.sidenav .sidea09 , .sidenav .sidea09:hover {
	background-position: 15px -415px !important;
}
.sidenav .sidea10 , .sidenav .sidea10:hover {
	background-position: 15px -470px !important;
}
.sidenav .sidea11 , .sidenav .sidea11:hover {
	background-position: 15px -522px !important;
}
.sidenav .sidea12 , .sidenav .sidea12:hover {
	background-position: 15px -576px !important;
}
.sidenav .sidea13 , .sidenav .sidea13:hover {
	background-position: 15px -628px !important;
}
.sidenav .sidea14 , .sidenav .sidea14:hover {
	background-position: 15px -92px !important;
	color: #ffee00;
}
.sidenav li .activelink {
	color: #1d71b6;
	background: #f0f0f0 url(/imgs/default/bluesidebarimg.png) no-repeat 15px center;
}
.centerbox {
	width: 90%;
	margin-right: 0;
}
@media screen and (max-width: 1200px) {
	.sidenav {
		width: 4.7%;
		left: -5px;
	}
	.sidenav:hover {
		width: 14%;
		left: 0;
	}
	.centerbox {
		width: 96%;
	}
}
@media screen and (max-width: 1300px) and (min-width: 1201px) {
	.sidenav {
		width: 4%;
		left: -5px;
	}
	.centerbox {
		width: 95%;
	}
	.sidenav:hover {
		width: 12%;
		left: 0;
	}
}
@media screen and (max-height: 780px) {
