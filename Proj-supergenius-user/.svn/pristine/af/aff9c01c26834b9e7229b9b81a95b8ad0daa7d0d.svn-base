<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>com.supergenius</groupId>
	<artifactId>proj-supergenius-user</artifactId>
	<packaging>war</packaging>
	<version>1.0.0-SNAPSHOT</version>
	<name>Genius Maven Webapp</name>
	<url>http://maven.apache.org</url>
	<properties>
		<supergenius-xo-groupId>com.supergenius.xo</supergenius-xo-groupId>
		<supergenius-xo-common-version>[3.0.0-SNAPSHOT,)</supergenius-xo-common-version>
		<supergenius-xo-user-version>[3.0.0-SNAPSHOT,)</supergenius-xo-user-version>
		<supergenius-xo-account-version>[3.0.0-SNAPSHOT,)</supergenius-xo-account-version>
		<supergenius-server-groupId>com.supergenius.server</supergenius-server-groupId>
		<supergenius-server-common-version>[3.0.0-SNAPSHOT,)</supergenius-server-common-version>
		<supergenius-server-user-version>[3.0.0-SNAPSHOT,)</supergenius-server-user-version>
		<genius-deps-version>[1.1.0-SNAPSHOT,)</genius-deps-version>
		<genius-deps-groupId>com.genius.deps</genius-deps-groupId>
		<java-version>1.8</java-version>
		<maven-clean-plugin-version>2.5</maven-clean-plugin-version>
		<maven-compiler-plugin-version>2.3.2</maven-compiler-plugin-version>
		<maven-resources-plugin-version>2.4.3</maven-resources-plugin-version>
		<maven-surefire-plugin-version>2.12.1</maven-surefire-plugin-version>
		<maven-war-plugin-version>2.3</maven-war-plugin-version>
		<maven-assembly-plugin-version>2.4</maven-assembly-plugin-version>
		<maven-tomcat7-plugin-version>2.2</maven-tomcat7-plugin-version>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
		<maven.build.timestamp.format>yyyyMMdd</maven.build.timestamp.format>
		<timestamp>${maven.build.timestamp}</timestamp>
	</properties>
	<dependencies>
		<dependency>
			<groupId>${supergenius-xo-groupId}</groupId>
			<artifactId>proj-supergenius-xo-common</artifactId>
			<version>${supergenius-xo-common-version}</version>
		</dependency>
		<dependency>
			<groupId>${supergenius-xo-groupId}</groupId>
			<artifactId>proj-supergenius-xo-user</artifactId>
			<version>${supergenius-xo-user-version}</version>
		</dependency>
		<dependency>
			<groupId>${supergenius-xo-groupId}</groupId>
			<artifactId>proj-supergenius-xo-account</artifactId>
			<version>${supergenius-xo-account-version}</version>
		</dependency>
		<dependency>
			<groupId>${supergenius-server-groupId}</groupId>
			<artifactId>proj-supergenius-server-common</artifactId>
			<version>${supergenius-server-common-version}</version>
		</dependency>
		<dependency>
			<groupId>${supergenius-server-groupId}</groupId>
			<artifactId>proj-supergenius-server-user</artifactId>
			<version>${supergenius-server-user-version}</version>
		</dependency>
		<dependency>
			<groupId>${genius-deps-groupId}</groupId>
			<artifactId>genius-deps-xobaseadmin</artifactId>
			<version>${genius-deps-version}</version>
			<type>pom</type>
		</dependency>
		<dependency>
			<groupId>${genius-deps-groupId}</groupId>
			<artifactId>genius-deps-projbase</artifactId>
			<version>${genius-deps-version}</version>
			<type>pom</type>
		</dependency>
		<dependency>
			<groupId>${genius-deps-groupId}</groupId>
			<artifactId>genius-deps-test</artifactId>
			<version>${genius-deps-version}</version>
			<type>pom</type>
			<scope>test</scope>
		</dependency>
		<dependency>
			<groupId>${genius-deps-groupId}</groupId>
			<artifactId>genius-deps-servlet</artifactId>
			<version>${genius-deps-version}</version>
			<type>pom</type>
			<scope>provided</scope>
		</dependency>
		<!-- CXF -->
		<dependency>
			<groupId>org.codehaus.xfire</groupId>
			<artifactId>xfire-all</artifactId>
			<version>1.2.6</version>
			<exclusions>
				<exclusion>
					<groupId>org.codehaus.xfire</groupId>
					<artifactId>xfire-spring</artifactId>
				</exclusion>
				<exclusion>
					<groupId>org.apache.ws.commons</groupId>
					<artifactId>XmlSchema</artifactId>
				</exclusion>
				<exclusion>
					<groupId>org.codehaus.woodstox</groupId>
					<artifactId>wstx-asl</artifactId>
				</exclusion>
			</exclusions>
		</dependency>
				<!-- Snappy -->
		<dependency>
			<groupId>org.xerial.snappy</groupId>
			<artifactId>snappy-java</artifactId>
			<version>1.1.0-M3</version>
			<scope>test</scope>
		</dependency>
		<dependency>
			<groupId>org.jdom</groupId>
			<artifactId>jdom2</artifactId>
			<version>2.0.5</version>
		</dependency>
		<dependency>
			<groupId>dom4j</groupId>
			<artifactId>dom4j</artifactId>
			<version>1.6.1</version>
		</dependency>
	</dependencies>
	<build>
		<plugins>
			<plugin>
				<artifactId>maven-clean-plugin</artifactId>
				<version>${maven-clean-plugin-version}</version>
				<configuration>
					<filesets>
						<fileset>
							<directory>.</directory>
							<includes>
								<include>*.log</include>
							</includes>
						</fileset>
						<fileset>
							<directory>src/main/webapp</directory>
							<includes>
								<include>files/**</include>
								<include>imgs/user/**</include>
								<include>imgs/webdata/**</include>
							</includes>
							<excludes>
								<exclude>imgs/webdata/product/14061319**</exclude>
								<exclude>imgs/webdata/index/14061619**</exclude>
								<exclude>**/readme</exclude>
							</excludes>
						</fileset>
						<fileset>
							<directory>src/main/webapp/WEB-INF/Data</directory>
							<includes>
								<include>*.del</include>
								<include>**</include>
							</includes>
							<excludes>
								<exclude>index/index.json</exclude>
								<exclude>Templ/**</exclude>
								<exclude>SEO/seo.json</exclude>
								<exclude>**/readme</exclude>
							</excludes>
							<followSymlinks>false</followSymlinks>
						</fileset>
					</filesets>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>${maven-compiler-plugin-version}</version>
				<configuration>
					<encoding>UTF-8</encoding>
					<source>${java-version}</source>
					<target>${java-version}</target>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-resources-plugin</artifactId>
				<version>${maven-resources-plugin-version}</version>
				<configuration>
					<source>${java-version}</source>
					<target>${java-version}</target>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-surefire-plugin</artifactId>
				<version>${maven-surefire-plugin-version}</version>
				<configuration>
					<!-- <skip>true</skip> -->
					<runOrder>alphabetical</runOrder>
					<!-- <includes> -->
					<!-- <include>**/InitTestSuite.java</include> -->
					<!-- </includes> -->
					<threadCount>1</threadCount>
					<excludes>
						<exclude>**/global/**</exclude>
					</excludes>
					<!-- <testFailureIgnore>true</testFailureIgnore> -->
				</configuration>
				<dependencies>
					<dependency>
						<groupId>org.apache.maven.surefire</groupId>
						<artifactId>surefire-junit47</artifactId>
						<version>2.11</version>
					</dependency>
				</dependencies>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-war-plugin</artifactId>
				<version>${maven-war-plugin-version}</version>
				<configuration>
					<archiveClasses>false</archiveClasses>
					<!-- <packagingExcludes>WEB-INF/classes/com/**,WEB-INF/classes/me/**</packagingExcludes> -->
					<!-- <warSourceExcludes>imgs/**,js/**,css/**,ckeditor/**</warSourceExcludes> -->
					<webResources>
						<resource>
							<directory>src/main/webapp</directory>
							<!-- <excludes> -->
							<!-- <exclude>**/js</exclude> -->
							<!-- <exclude>**/css</exclude> -->
							<!-- <exclude>/**</exclude> -->
							<!-- </excludes> -->
						</resource>
					</webResources>
				</configuration>
			</plugin>
			<plugin>
				<artifactId>maven-assembly-plugin</artifactId>
				<version>${maven-assembly-plugin-version}</version>
				<executions>
					<execution>
						<id>makejar</id>
						<phase>package</phase>
						<goals>
							<goal>single</goal>
						</goals>
						<configuration>
							<archive>
								<manifest>
									<mainClass>com.grand.app.RabbitMQMain</mainClass>
								</manifest>
							</archive>
							<skipAssembly>false</skipAssembly>
							<appendAssemblyId>false</appendAssemblyId>
							<outputDirectory>
								${project.build.directory}/${project.build.finalName}/WEB-INF/lib
							</outputDirectory>
							<descriptors>
								<descriptor>src/assemble/jar.xml</descriptor>
							</descriptors>
						</configuration>
					</execution>
					<execution>
						<id>makeweb</id>
						<phase>package</phase>
						<goals>
							<goal>single</goal>
						</goals>
						<configuration>
							<descriptors>
								<descriptor>src/assemble/webtargz.xml</descriptor>
							</descriptors>
						</configuration>
					</execution>
					<execution>
						<id>makestatic</id>
						<phase>package</phase>
						<goals>
							<goal>single</goal>
						</goals>
						<configuration>
							<descriptors>
								<descriptor>src/assemble/static.xml</descriptor>
							</descriptors>
						</configuration>
					</execution>
					<execution>
						<id>makeconfig</id>
						<phase>package</phase>
						<goals>
							<goal>single</goal>
						</goals>
						<configuration>
							<descriptors>
								<descriptor>src/assemble/config.xml</descriptor>
							</descriptors>
						</configuration>
					</execution>
					<execution>
						<id>makewebdata</id>
						<phase>package</phase>
						<goals>
							<goal>single</goal>
						</goals>
						<configuration>
							<descriptors>
								<descriptor>src/assemble/webdata.xml</descriptor>
							</descriptors>
						</configuration>
					</execution>
				</executions>
			</plugin>
			<plugin>
				<groupId>org.apache.tomcat.maven</groupId>
				<artifactId>tomcat7-maven-plugin</artifactId>
				<version>${maven-tomcat7-plugin-version}</version>
				<configuration>
					<server>SuperGeniusServ-user</server>
					<path>/${project.build.finalName}</path>
					<port>9087</port>
					<update>true</update>
					<charset>utf-8</charset>
					<uriEncoding>UTF-8</uriEncoding>
				</configuration>
			</plugin>
		</plugins>
		<finalName>proj-supergenius-user</finalName>
	</build>
</project>
