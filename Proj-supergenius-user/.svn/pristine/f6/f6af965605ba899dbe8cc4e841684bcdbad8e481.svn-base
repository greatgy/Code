body {
	background: #e6e6e6;
}
.container {
	min-height: 680px;
}
.main {
	width: 980px;
	overflow: hidden;
	padding-bottom: 40px;
	margin-bottom: 40px;
	padding-top: 17px;
}
.revenueleft {
	width: 198px;
	height: 345px;
	display: inline-block;
	text-align: left;
	box-shadow: 1px 2px 1px 0 #C2C1C1;
	background: #fff;
	margin-right: 12px;
}
.revenueleft h2 {
	height: 52px;
	line-height: 55px;
	text-align: center;
	font-size: 17px;
	font-weight: bold;
	border-bottom: 3px solid #d1444d;
    color: #d1444d;
	letter-spacing: 1px;
	overflow: hidden;
}
.revenuelist li {
	height: 55px;
	line-height: 55px;
	overflow: hidden;
}
.revenuelist li a {
	width: 100%;
	display: block;
	height: 55px;
	line-height: 55px;
	background: url(../../imgs/default/leftside.png) no-repeat 20px center;
}

.revenuelist li span {
	font-size: 16px;
	color: #797979;
	vertical-align: top;
	margin-left: 60px;
}
.revenuelist .activeli {
	background: #d1444d;
}
.revenuelist .activeli a {
	background: url(../../imgs/default/leftsidew.png) no-repeat 20px center;
}
.revenuelist li .baseInfor , .revenuelist .activeli .baseInfor {
	background-position: 20px 15px;
} 
.revenuelist li .acCharge , .revenuelist .activeli .acCharge {
	background-position: 20px -42px;
} 
.revenuelist li .chargeRecord , .revenuelist .activeli .chargeRecord {
	background-position: 20px -97px;
} 
.revenuelist li .revRecord , .revenuelist .activeli .revRecord {
	background-position: 20px -157px;
} 
.revenuelist li .changePsw , .revenuelist .activeli .changePsw {
	background-position: 20px -214px;
} 
.revenuelist .activeli span {
	color: #fff;
}
.revenuelist li:hover {
	background: #d1444d;
}
.revenuelist li:hover span {
	color: #fff;
}
.revenueright {
	width: 765px;
	display: inline-block;
	text-align: left;
	min-height: 605px;
	background: #fff;
	vertical-align: top;
	box-shadow: 1px 2px 1px 0 #C2C1C1;
}
.revenueright h2 {
	height: 50px;
	line-height: 55px;
	font-size: 17px;
	color: #000;
	padding-left: 38px;
	border-bottom: 1px dotted #E6E6E6;
	letter-spacing: 1px;
}
.revenuerigmain {
	width: 640px;
	margin: 0 auto;
	padding-top: 15px;
	padding-bottom: 10px;
	overflow: hidden;
}
.revenuerigmain li {
	font-size: 15px;
	color: #373737;
	text-align: left;
	line-height: 40px;
}
.revenuerigmain span {
	font-size: 13px;
	/*font-weight: bold;*/
	margin-left: 5px;
}
.revenuerigmain .leftmoney {
	color: #000;
}
.revenuerigmain .totalout {
	color: #000;
}
.revenuerigmain .totalin {
	color: #000;
}
.revenuerigmain .opentime {
	color: #898888;
	font-weight: normal;
	letter-spacing: 1px;
}
.btns {
	width: 640px;
	margin: 15px auto;
}
.btns .recharge {
	display: inline-block;
	padding: 6px 10px;
	background: #c61420;
	border-radius: 5px;
	font-size: 14px;
	color: #fff;
	text-shadow: 0 0 1px #fff;
	margin-right: 10px;
}
.btns .recharge:hover {
	box-shadow: 1px 1px 0 1px #AB0909;
	transform: skew(12deg);
	transition: all ease;
	background: #DC0C0C;
}
.btns .changepw {
 	display: inline-block;
	padding: 6px 10px;
	background: #fff;
	border-radius: 5px;
	font-size: 14px;
	color: #7D7D7D;
	border: 1px solid #BCBCBC;
}
.btns .changepw:hover {
	box-shadow: 1px 1px 0 1px #EDEDED;
	-webkit-transform: skew(-12deg);
	transform: skew(-12deg);
	transition: all ease;
	background: #EAEAEA;
	color: #7B7777;
}

/* 账户充值样式 */
.rechargement {
	width: 640px;
}
.rechargerigtop li {
	color: #717171;
}
.rechargerigtop .nowmoney {
	color: #000;
}
.rechargerigtop li input {
	width: 213px;
	height: 22px;
	border: 1px solid #cdcdcd;
	padding: 5px 10px;
	font-size: 14px;
	color: #9D9B9B;
	overflow: hidden;
	line-height: 20px;
}
.rechargerigtop li input:focus {
	box-shadow: 0 0 2px 0 #129DE3;
}
.rechargerigmid {
	margin-top: 35px;
	margin-bottom: 54px;
}
.rechargerigmid h3 , .rechargerigbot h3 {
	font-size: 16px;
	text-align: left;
	/*color: #ff9570;*/
	letter-spacing: 1px;
	padding-bottom: 20px;
}
.payways {
	margin-top: 20px;
}
.payways li span {
	font-size: 15px;
	color: #737171;
	margin-top: 8px;
	display: inline-block;
}
.payways .paywaysright {
	display: inline-block;
	vertical-align: top;
	margin-left: 40px;
	position: relative;
	max-width: 515px;
	overflow: hidden;
}
.payways .paywayspecial {
	margin-left: 10px;
}
.paywaysright .bank {
	display: inline-block;
	float: left;
	margin-right: 25px;
	margin-bottom: 30px;
}
.bank img {
	width: 126px;
	height: 37px;
	overflow: hidden;
	vertical-align: middle;
}
.pay {
	font-size: 15px;
	color: #fff;
	background: #c61420;
	display: block;
	width: 112px;
	height: 38px;
	line-height: 38px;
	text-align: center;
	border-radius: 8px;
	letter-spacing: 1px;
}
.pay:hover {
	box-shadow: 0 1px 0 1px #9D0B0B;
	opacity: .7;
	background: #DA2222;
}
.rechargerigbot {
	padding-bottom: 80px;
}
.rechargerigbot h3 {
	border-bottom: 1px solid #d2d2d2;
}
.rechargerigbot .payquestions {
	padding-top: 20px;
	text-align: left;
}
.payquestions li {
	font-size: 14px;
	color: #5d5e5c;
	line-height: 22px;
}
.rechargecover {
	width: 350px;
	height: 180px;
	background-color: rgba(255,255,255,0.6);
	border-radius: 8px;
}
.rechargecoverment {
	width: 300px;
	margin: 0 auto;
	background-color: #fff;
	padding: 5px 15px;
	margin-top: 9px;
	border-radius: 8px;
	overflow: hidden;
}
.rechargecoverment h1 {
	text-align: left;
	font-size: 16px;
	color: #F49291;
	margin-top: 20px;
	overflow: hidden;
}
.rechargecoverment h1 img {
	width: 53px;
	height: 53px;
	overflow: hidden;
	vertical-align: middle;
	margin-right: 5px;
}
.rechargecoverment .tipreturn {
	margin: 10px auto;
	font-size: 12px;
	color: #535353;
}
.rechargecoverment .recharbtns {
	margin-top: 20px;
}
.recharbtns a {
	width: 122px;
	height: 34px;
	display: inline-block;
	text-align: center;
	line-height: 34px;
	font-size: 14px;
	border-radius: 5px;
	outline: none;
	overflow: hidden;
}
.recharbtns .finish {
	margin-right: 10px;
	background: #c61420;
	color: #fff;
}
.recharbtns .finish:hover {
	background: #da1523;
	box-shadow: 0 1px 0 1px #BA0C0C;
	opacity: .7;
}
.recharbtns .default {
	border: 1px solid #ddd;
	color: #535353;
	background: #fff;
}
.recharbtns .default:hover {
	background: #918F8F;
	color: #fff;
	box-shadow: 0 1px 0 0 #D5E3E4;
	opacity: .7;
}
/* 充值成功页面样式 */
.recharsuccessmid {
	width: 640px;
	margin-top: 120px;
}
.recharsuccess {
	margin: 0 auto;
	text-align: center;
	color: #ff8252;
	font-size: 17px;
	font-weight: bold;
	letter-spacing: 1px;
	margin-bottom: 40px;
	overflow: hidden;
}
.recharsuccess img {
	width: 76px;
	height: 64px;
	overflow: hidden;
	vertical-align: middle;
	margin-right: 20px;
}
.returnsuccess {
	text-align: center;
	font-size: 16px;
	color: #777777;
	overflow: hidden;
}
.returnsuccess b {
	color: #ff8252;
	font-weight: bold;
	margin: 0 5px;
}
.lookrecards {
	display: inline-block;
	text-align: center;
	font-size: 14px;
	width: 100px;
	height: 35px;
	background: #d1444d;
	line-height: 35px;
	color: #fff;
	margin: 0 auto;
	border-radius: 8px;
	margin-left: 260px;
	margin-top: 40px;
	margin-bottom: 20px;
}
.lookrecards:hover {
	background: #118DF4;
	border: 1px solid #D2D0D0;
	outline: none;
	opacity: .7;
}
/* 充值记录样式 */
.recordsright h2 {
	border-bottom: none;
}
.recordslist {
	width: 725px;
	height: 450px;
	margin: 0 auto;
	display: table;
	overflow: hidden;
	margin-bottom: 35px;
}
.recordslist tr {
	height: 40px;
	line-height: 42px;
	width: 724px;
	line-height: 45px;
	text-align: left;
	overflow: hidden;
	display: block;
}
.recordslist .thtr {
	height: 45px;
	background: #1a9adb!important;
}
.recordslist tr th {
	text-align: center;
	font-size: 18px;
	color: #fff;
	overflow: hidden;
}
.recordslist tr .th01 {
	width: 175px;
}
.recordslist tr .th02 {
	width: 170px;
}
.recordslist tr .th03 {
	width: 135px;
}
.recordslist tr .th04 {
	width: 125px;
}
.recordslist tr .th05 {
	width: 100px;
}
.recordslist tr td {
	text-align: center;
	font-size: 12px;
	color: #737579;
	overflow: hidden;
}
.recordslist tr td.th03 , .recordslist tr td.th04 , .recordslist tr td.th05 {
	font-size: 14px;
}
.recordslist .th06 a {
	color: #3080cf;
	font-size: 14px;
}
.recordslist .th06 a:hover {
	color: #60b9ff;
	text-decoration: underline;
}
.cancle {
	margin-right: 10px;
}

.page {
	width: 100%;
	margin: 0 auto;
	margin-top: 10px;
	padding-bottom: 20px;
}
/* 收支记录样式 */

.revenueslist tr .th01 {
	width: 150px;
	padding-left: 10px;
}
.revenueslist tr .th02 {
	width: 170px;
	padding-left: 5px;
}
.revenueslist tr .th03 {
	width: 110px;
}
.revenueslist tr .th04 {
	width: 115px;
}
.revenueslist tr .th05 {
	width: 45px;
}
.revenueslist tr .th06 {
	width: 80px;
	text-align: center;
}
.revenueslist tr .th07 {
	width: 42px;
	text-align: center;
	padding-right: 8px;
}
.revenueslist tr td.th04 , .revenueslist tr td.th05 {
	font-size: 12px;
	font-weight: bold;
	color: #737579;
}
.revenueslist tr td.out {
	color: #e60012;
}
.revenueslist tr td.recive {
	color: #43a102;
}

/* 修改支付密码样式 */
.revenueright .changecontent {
	width: 685px;
	text-align: center;
	padding-top: 30px;
	margin-bottom: 20px;
	padding-bottom: 10px;
	overflow: hidden;
}
.changecontent .changepw {
	width: 540px;
	margin: 0 auto;
	padding-top: 10px;
	text-align: left;
	overflow: hidden;
}
.changepw li {
	margin-bottom: 30px;
}
.changepw span {
	display: inline-block;
	width: 75px;
	text-align: left;
	font-size: 15px;
	color: #676767;
	overflow: hidden;
}
.changepw input {
	width: 230px;
	height: 22px;
	display: inline-block;
	border: 1px solid #cdcdcd;
	padding: 5px 10px;
	margin-left: 5px;
	font-size: 14px;
	color: #4F4F4F;
	line-height: 20px;
	overflow: hidden;
}
.changepw input:focus {
	box-shadow: 0 0 2px 0 #129DE3;
}
.changepw .checknote {
	height: 36px;
	line-height: 35px;
	width: 198px;
	display: inline-block;
	float: right;
	vertical-align: middle;
	overflow: hidden;
	font-size: 12px;
	color: #685f55;
	/* display: none; */
}
.checknote img {
	width: 16px;
	height: 16px;
	overflow: hidden;
	padding-left: 20px;
	vertical-align: middle;
	margin-right: 2px;
}
.safe {
	width: 370px;
	overflow: hidden;
	margin-bottom: 10px;
	margin-top: -20px;
	/* display: none; */
}
.safe span {
	width: 65px;
	height: 18px;
	text-align: center;
	background: #ede2d5;
	font-size: 12px;
	color: #6e6c6b;
	line-height: 17px;
	margin-right: 5px;
}
.safe .saferange {
	background-color: #ddcbb6;
}
.submit {
	width: 100px;
	height: 38px;
	background: #c61420;
	color: #fff;
	text-align: center;
	line-height: 38px;
	font-size: 17px;
	letter-spacing: 8px;
	border-radius: 6px;
	margin-left: -120px;
	overflow: hidden;
}
.submit:hover {
	background: #DC0C0C;
	box-shadow: 0 1px 2px 0 #AB0909;
	cursor: pointer;
}
.recordslist tr:nth-of-type(even) {
    background: #dceef8!important;
}
.draw {
    display: inline-block;
    float: right;
    margin-top: 5px;
    border: 1px solid #ddd;
    border-radius: 6px;
    font-size: 14px;
    color: #666;
    padding: 0 8px;
    height: 30px;
    line-height: 30px;
    text-align: center;
}
.draw:hover {
    background: #00c76f;
    color: #fff;
}
.balance {
	margin: 40px 0 20px;
	font-size: 14px;
	color: #313131;
}
.allmoney {
	color: #e9691c;
	font-size: 16px;
}
.checknote {
    display: inline-block;
}
.rechargerigtop li {
	margin-bottom: 15px;
}
.rechargerigtop li i {
	margin-right: 5px;
	color: #d1444d;
}
.nextbtn {
    display: block;
    margin-top: 30px;
    width: 110px;
    height: 38px;
    background: #c61420;
    color: #fff;
    text-align: center;
    line-height: 38px;
    font-size: 16px;
    border-radius: 6px;
    margin-left: 190px;
    overflow: hidden;
}
.nextbtn:hover {
    background: #DC0C0C;
    box-shadow: 0 1px 2px 0 #AB0909;
    cursor: pointer;
    outline: none;
}
.setmid {
    text-align: center;
    margin-top: 80px;
    padding-bottom: 20px;
    padding: 40px 0 20px;
    overflow: hidden;
}
.setmid img {
    width: 78px;
    height: 76px;
    overflow: hidden;
}
.setmid .tip {
    overflow: hidden;
    font-size: 16px;
    color: #595959;
    margin: 30px 0;
}