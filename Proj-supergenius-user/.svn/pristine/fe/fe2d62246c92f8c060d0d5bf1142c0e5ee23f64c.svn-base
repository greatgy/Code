body {
	background: #e6e6e6;
}
.container {
	margin-bottom: 104px;
	min-height: 600px;
}
.myorders {
	width: 980px;
	min-height: 500px;
	margin: 0 auto;
	background: #fff;
	margin-top: 17px;
	box-shadow: 1px 1px 0 1px #BFBFBF;
}
.myorders h2 {
	width: 940px;
	height: 50px;
	line-height: 60px;
	font-size: 17px;
	color: #000;
	text-align: left;
	padding-left: 20px;
	overflow: hidden;
}
.orderlist {
	width: 942px;
	min-height: 328px;
	text-align: left;
	margin: 0 auto;
	padding-bottom: 30px;
}
.orderlist .orderlisttit{
	width: 925px;
	height: 48px;
	overflow: hidden;
	padding: 0 0 0 10px;
	background: #d1444d;
	line-height: 50px;
}
.orderlisttit .partitem {
	display: inline-block;
	overflow: hidden;
	font-size: 16px;
	color: #fff;
	float: left;
}
.orderlist .part01 {
	width: 170px;
	padding-left: 15px;
}
.orderlist .part02 {
	width: 300px;
}
.orderlist .part03 {
	width: 90px;
}
.orderlist .part04 {
	width: 115px;
}
.orderlist .part05 {
	width: 140px;
}
.orderlist .part06 {
	width: 75px;
}
.orderlist .part07 {
	width: 100px;
}
.part03 select , .part06 select {
	width: 49px;
	font-size: 12px;
	text-align: left;
	overflow: hidden;
}
.orderlist li {
	height: 40px;
	line-height: 40px;
	width: 925px;
	padding: 0 0 0 10px;
	margin: 0 auto;
	overflow: hidden;
}
.orderlist .orderlitem {
	display: inline-block;
	overflow: hidden;
	font-size: 12px;
	color: #313131;
	float: left;
}
.orderlist .orderlitem.part01 {
	
}
.orderlitem a {
	color: #2e7fce;
	font-size: 12px;
	margin-right: 20px;
}
.orderlitem a:hover {
	color: #36abeb;
	text-decoration: underline;
}
.orderlitem .delete:hover {
	color: #f50c0c;
}
.page {
	width: 100%;
	height: 20px;
	padding-bottom: 40px;
	margin: 0 auto;
}
