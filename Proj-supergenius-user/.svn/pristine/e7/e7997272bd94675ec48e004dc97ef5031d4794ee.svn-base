.wraper {
	width: 100%;
	background: url(../../imgs/default/loginbg.png)no-repeat left 37px;
	background-size: 100% 100%;
	min-height: 948px;
	position: relative;
	overflow: hidden;
}
.bottom {
	position: absolute;
	bottom: 5%;
	background: none;
}
.bottom .bot_01 {
	color: #6C6464;
}
.bot_01 a {
	color: #595555;
}
.bot_02 {
	color: #6D6C6C;
}
.clouds {
	position: absolute;
	left: -1000px;
	top: 0;
}
.main {
	max-width: 1000px;
	margin: 0 auto;
	margin-bottom: 20px;
	margin-top: 80px;
	text-align: left;
}
.maincontent {
	width: 875px;
}
.toptext {
	width: 270px;
	height: 78px;
	overflow: hidden;
	margin-bottom: 20px;
}
.logincenter {
	width: 872px;
	height: 438px;
	background: rgba(255,255,255,0.52);
	border-radius: 12px;
	position: relative;
}
.centercontainer {
	width: 856px;
	height: 420px;
	background: rgba(255,255,255,.6);
	border-radius: 12px;
	position: absolute;
	top: 8px;
	left: 8px;
	overflow: hidden;
}
.centerleft {
	display: inline-block;
	width: 430px;
	vertical-align: middle;
	margin-top: 20px;
	padding-left: 10px;
	padding-bottom: 10px;
	overflow: hidden;
}
.centerleft h2 {
	height: 65px;
	line-height: 60px;
	font-size: 19px;
	color: #313131;
	overflow: hidden;
}
.centerleft h2 img {
	width: 165px;
	height: 60px;
	overflow: hidden;
	vertical-align: middle;
	margin-right: 20px;
}
.registerlink {
	width: 315px;
	text-align: right;
	font-size: 15px;
	color: #707073;
	margin-top: 15px;
	overflow: hidden;
}
.centerleft .warninginform {
	margin: 10px 0;
}
form.inputs .loginbut {
	background: #c61420;
	width: 308px;
	height: 38px;
	text-align: center;
	display: block;
	line-height: 38px;
	border-radius: 7px;
	font-size: 16px;
	letter-spacing: 5px;
	color: #fff;
}
form.inputs .loginbut:hover {
	background: #B70C18;
	box-shadow: 1px 1px 0 1px #A60303;
}
.centerright {
	display: inline-block;
	width: 410px;
	height: 305px;
	border-left: 1px solid #fff;
	vertical-align: middle;
	position: relative;
	overflow: hidden;
	padding-bottom: 10px;
	margin-top: 30px;
}
.bird {
	width: 70px;
	height: 50px;
	overflow: hidden;
	position: absolute;
	bottom: 5px;
	left: 20px;
	
}
.rightcenter {
	width: 230px;
	font-size: 2rem;
	color: #24c5b5;
	overflow: hidden;
	position: absolute;
	top: 150px;
	right: -230px;
	overflow: hidden;
}
.leftcenter {
	width: 230px;
	font-size: 16px;
	font-weight: bolder;
	text-transform: uppercase;
	text-shadow: 1px 1px 0 #BDBDC6;
	color: #181864;
	position: absolute;
	top: 160px;
	left: 100px;
	left: -230px;
}
form.inputs input:-webkit-autofill, form.inputs input:-webkit-autofill:hover, form.inputs input:-webkit-autofill:active, form.inputs input:-webkit-autofill:focus {
	-webkit-box-shadow: 0 0 0 1000px white inset;
}
.otherLink {
    width: 72%;
    margin: 0 auto;
    text-align: center;
    margin-top: 20px;
    overflow: hidden;
}
.otherLink img {
	width: 50px;
    height: 50px;
    overflow: hidden;
    border-radius: 50%;
    margin: 0 12% 0 2%;
}
.centerleftgreen h2 img {
	margin-right: 0;
}
.memberLogin {
    background: #ca0e0d;
    padding: 4px 10px;
    font-size: 15px;
    color: #fefcfc;
    border-radius: 5px;
}
form.inputs .logingreen {
	background: #0dc06e;
}
form.inputs .logingreen:hover {
    background: #10dd7f;
    box-shadow: 1px 1px 0 1px #0dce75;
}