.wraper {
	background: #f2f2f2;
}
.container {
	min-height: 720px;
}
.main {
	width: 980px;
	padding-top: 17px;
}
.maintop {
	height: 280px;
	background: #fff;
	box-shadow: 1px 1px 0 1px #D2D2D2;
	margin-bottom: 15px;
	border-left: 1px solid #ECECEC;
}
.maintop h2 , .blockitem h2 {
	height: 42px;
	line-height: 45px;
	text-align: left;
	background: #fafafa;
	border-bottom: 1px dotted #dfdfdf;
	padding-left: 33px;
	font-size: 18px;
	color: #000000;
	overflow: hidden;
}
.maintop h2 a {
	display: inline-block;
	float: right;
	margin-right: 35px;
	color: #c61420;
	font-size: 16px;
}
.maintop h2 .modals {
	display: inline-block;
	color: #c61420;
	margin-left: 645px;
	margin-right: 0;
	padding: 4px 10px;
	font-size: 16px;
	border: 1px solid #c61420;
	border-radius: 6px;
	text-align: center;
	line-height: 20px;
	float: none;
}
.maintop h2 a:hover {
	-webkit-transform: scale(1.1);
	-moz-transform: scale(1.1);
	-o-transform: scale(1.1);
	-ms-transform: scale(1.1);
	transform: scale(1.1); 
}
.maintopleft {
	display: inline-block;
	width: 488px;
	float: left;
	padding-top: 18px;
	border-right: 1px dotted #e7e7e7;
	padding-right: 5px;
	overflow: hidden;
}
.maintoplftop {
	width: 487px;
	height: 165px;
	text-align: left;
	border-bottom: 1px dotted #e7e7e7;
	padding-right: 5px;
	padding-top: 5px;
}
.imgcontainer {
	display: inline-block;
	width: 115px;
	margin-left: 16px;
	vertical-align: top;
}
.imgcontainer img {
	width: 110px;
	height: auto;
	border-radius: 50%;
	overflow: hidden;
}
.imgcontainer span {
	margin-top: 5px;
	font-size: 18px;
	color: #000000;
	text-align: center;
	display: block;
	letter-spacing: 1px;
}
.persondetail {
	width: 347px;
	display: inline-block;
	overflow: hidden;
}
.memberstatus span {
	display: inline-block;
	padding: 6px 12px;
	background: #e0e0e0;
	border-radius: 12px;
	font-size: 14px;
	color: #535353;
}
.personlist {
	margin-left: 5px;
	overflow: hidden;
}
.personlist li {
	margin-top: 23px;
	height: 17px;
    overflow: hidden;
	font-size: 16px;
	color: #3f3f3f;
}
.personlist .membernum {
	color: #000000;
	font-weight: bold;
}
.personlist .email {
	color: #000000;
	font-weight: bold;
}
.maintoplfbottom {
	height: 48px;
	line-height: 48px;
	text-align: left;
	overflow: hidden;
}
.maintoplfbottom a {
	font-size: 16px;
	color: #676767;
	margin-left: 75px;
}
.maintoplfbottom a:hover {
	color: #d4565d;
	font-weight: bold;
}
.maintopright {
	display: inline-block;
	padding-top: 20px;
	width: 455px;
	padding-left: 25px;
	overflow: hidden;
	padding-bottom: 15px;
}
.accountdetail li {
	font-size: 16px;
	color: #565656;
	text-align: left;
	line-height: 40px;
	overflow: hidden;
}
.accountdetail b {
	font-size: 14px;
	margin-left: 5px;
}
.accountdetail .b01 {
	font-weight: bold;
	color: #000000;
} 
.accountdetail .b02 {
	font-weight: bold;
	color: #e60012;
} 
.accountdetail .b03 {
	color: #43a102;
	font-weight: bold;
} 
.accountdetail .b04 {
	color: #8a8a8a;
} 
.btns {
	text-align: left;
	margin-top: 15px;
}
.btns .recharge {
	display: inline-block;
	background: #c61420;
	border-radius: 5px;
	font-size: 14px;
	color: #fff;
	text-shadow: 0 0 1px #fff;
	margin-right: 10px;
	width: 80px;
	height: 28px;
	line-height: 28px;
	text-align: center;
}
.btns .recharge:hover {
	box-shadow: 1px 1px 0 1px #AB0909;
	transform: skew(12deg);
	transition: all ease;
}
.btns .changepw {
 	display: inline-block;
	padding: 6px 10px;
	background: #fff;
	border-radius: 5px;
	font-size: 14px;
	color: #c61420;
    border: 1px solid #c61420;
}
.btns .changepw:hover {
	-webkit-transform: skew(-12deg);
	-moz-transform: skew(-12deg);
	-o-transform: skew(-12deg);
	-ms-transform: skew(-12deg);
	transform: skew(-12deg);
	transition: all ease;
}
.mainbottom {
	text-align: left;
	position: relative;
	height: 850px;
	margin-bottom: 60px;
}
.mainbottom .blockitem {
	width: 482px;
	height: 268px;
	display: inline-block;
	background: #fff;
	margin-bottom: 25px;
	box-shadow: 1px 1px 0 1px #D2D2D2;
	float: left;
	margin-right: 8px;
	border-left: 1px solid #ECECEC;
}
.blockitem h2 {
	padding-left: 20px;
}
.blockitem h2 a {
	color: #000000;
}
.blockitem h2 a:hover {
	color: #858585;
}
.blockitem .libottom {
	width: 450px;
	overflow: hidden;
	padding-bottom: 20px;
	text-align: left;
}
.libottom p {
	margin-top: 40px;
	margin-left: 30px;
	font-size: 15px;
	color: #6f6f6f;
	line-height: 20px;
}
.libottom .baselink {
	padding: 4px 8px;
	border: 1px solid #d4565d;
    color: #d4565d;
	font-size: 14px;
	border-radius: 6px;
	margin-right: 10px;
}
.libottom .baselink:hover {
	background: #d1444d;
    color: #fff;
}
.libottom p .baselink {
	margin-left: 10px;
}
.libottom .leftimg {
	width: 188px;
	height: 100%;
	margin-top: 15px;
	vertical-align: top;
}
.libottom .p1 {
	display: inline-block;
	width: 250px;
	margin: 0;
	margin-top: 15px;
	font-size: 14px;
	color: #8F8F8F;
	height: 160px;
	margin-left: 5px;
	margin-bottom: 12px;
	overflow: hidden;
}
.blockitem h3 {
	height: 35px;
	line-height: 36px;
	border-bottom: 1px dotted #e7e7e7;
	padding-left: 21px;
	overflow: hidden;
}
.blockitem h3 span {
	font-size: 15px;
	color: #797979;
	margin-right: 65px;
}
.blockitem .number {
	margin-left: 8px;
	font-size: 15px;
	font-weight: bold;
	color: #e82431;
}
.blockitem .number:hover {
	color: #35CEBE;
}
.libottom .moralist {
	padding-top: 15px;
	padding-bottom: 20px;
}
.moralist li {
	display: inline-block;
	position: relative;
	margin-right: 10px;
	overflow: hidden;
}
.moralist img {
	width: 98px;
	height: 115px;
	overflow: hidden;
}
.moralist h4 {
	background: url(../../imgs/default/moralbg.png) no-repeat left center;
	width: 95px;
	height: 21px;
	overflow: hidden;
	text-align: center;
	line-height: 18px;
	color: #fff;
	font-size: 14px;
	position: absolute;
	bottom: 10px;
	left: -1px;
}
.libottom .judgemid {
	margin-bottom: 20px;
	overflow: hidden;
	height: 134px;
}
/* .judgemid .leftimg {
	height: 118px;
} */
.libottom .judgelist {
	display: inline-block;
	width: 250px;
	height: 115px;
	margin-top: 17px;
	margin-left: 5px;
	overflow: hidden;
}
.judgelist li {
	height: 20px;
    overflow: hidden;
	line-height: 20px;
	margin-bottom: 10px;
}
.judgelist li a {
	font-size: 14px;
	color: #686868;
}
.judgelist li a:hover {
	color: #6f8abb;
	text-decoration: underline;
}
.judgelist .dot {
	display: inline-block;
    width: 5px;
    height: 5px;
    background-color: red;
    border-radius: 50%;
    vertical-align: middle;
    margin-right: 5px;
    overflow: hidden;
}
.libottom .mergelist {
	margin-top: 15px;
	text-align: left;
	padding-bottom: 16px;
}
.mergelist li {
	display: inline-block;
	width: 98px;
	overflow: hidden;
	margin-right: 10px;
}
.mergelist li img {
	width: 98px;
	height: 95px;
	overflow: hidden;
}
.mergelist .mergetitle {
	text-align: center;
	display: inline-block;
	width: 98px;
	margin-top: 8px;
	font-size: 15px;
	color: #676767;
	overflow: hidden;
}
.mergelist li:hover .mergetitle{
	color: #d1444d;
	font-weight: bold;
	text-decoration: underline;
}
.blockitem .havenote span {
	margin-right: 30px;
}

.modalcontainer {
	width: 460px;
	min-height: 432px;
	border-radius: 15px;
	background-color: rgba(255,255,255,.6);
}
.modalmain {
	background: #fff;
	width: 440px;
	min-height: 410px;
	border-radius: 15px;
	overflow: hidden;
	margin: 0 auto;
	margin-top: 12px;
	margin-bottom: 12px;
}
.modalblocks {
	
}
.modalblocks li {
	height: 75px;
	line-height: 75px;
	border-bottom: 1px dotted #E1E1E1;
	overflow: hidden;
	outline: none;
}
.blockscontent {
	text-align: left;
	width: 420px;
	height: 75px;
	margin: 0 auto;
	overflow: hidden;
}
.blockscontent .radiobox {
	display: inline-block;
	width: 22px;
	height: 22px;
	border: 1px solid #378ad3;
	border-radius: 50%;
	background: #fff none left center;
	outline: none;
	vertical-align: top;
	margin-top: 32px;
	overflow: hidden;
	margin-left: 10px;
}
.blockscontent .radiobox:hover {
	cursor: pointer;
}
.blockscontent .blocksmid {
	display: inline-block;
	height: 75px;
	line-height: 70px;
	overflow: hidden;
	margin-left: 10px;
}
.blocksmid h2 {
	font-size: 20px;
	color: #498bd2;
	height: 25px;
}
.blocksmid span {
	font-size: 14px;
	color: #787676;
}
.blockscontent .turnbutton {
	display: inline-block;
	width: 21px;
	height: 45px;
	float: right;
	margin: 15px 0;
	position: relative;
	margin-right: 5px;
}
.turnbutton a {
	display: inline-block;
	width: 21px;
	height: 15px;
	background: url(../../imgs/default/pagebtn.png) no-repeat center center;
	position: absolute;
}
.turnbutton .forhead {
	top: 0;
	background-position: 0 3px;
}
.turnbutton .nexthead {
	bottom: 0;
	background-position: 0 -30px;
}
.turnbutton a:hover {
	border: 1px solid #C8E1FE;
}
.blockscontent .checkedspan {
	background: #1576cb url(../../imgs/default/checkrig.png) no-repeat center center;
}
.helpnote {
	font-size: 14px;
	color: #6C6C6C;
	margin-top: 20px;
	margin-bottom: 20px;
	letter-spacing: 1px;
	height: 20px;
	line-height: 20px;
	overflow: hidden;
}
.modalmain .checkbtns {
	padding-bottom: 20px;
}
.checkbtns button {
	width: 98px;
	height: 32px;
	border-radius: 5px;
	text-align: center;
	line-height: 32px;
	outline: none;
	font-size: 15px;
	letter-spacing: 5px;
	overflow: hidden;
	margin-right: 50px;
}
.checkbtns .cancle {
	color: #313131;
	background: #fff;
	border: 1px solid #555555;
}
.checkbtns .cancle:hover {
	color: #fff;
	background: #959595;
	opacity: .8;
	cursor: pointer;
}
.checkbtns .save {
	color: #fff;
	background: #c61420;
	border: 1px solid #c61420;
}
.checkbtns .save:hover {
	background: #8C0606;
	opacity: .8;
	cursor: pointer;
}
.position0 {
	position: absolute;
	left: 0;
	top: 0;
}
.position1 {
	position: absolute;
	right: 0;
	top: 0;
}
.position2 {
	position: absolute;
	left: 0;
	top: 285px;
}
.position3 {
	position: absolute;
	right: 0;
	top: 285px;
}
.position4 {
	position: absolute;
	left: 0;
	top: 570px;
}
.position5 {
	position: absolute;
	right: 0;
	top: 570px;
}
.bemember {
    margin: 30px;
    display: inline-block;
    height: 40px;
    line-height: 40px;
    background: #c2020f;
    color: #fff;
    padding: 0 10px;
    border-radius: 6px;
    font-size: 15px;
    text-align: center;
}
.memberimg img {
	width: 100px;
    height: 100px;
    border-radius: 50%;
    overflow: hidden;
    border: 3px solid #fec11c;
    text-align: center;
}
.memberimg {
	margin-top: 10px;
}
.memberimg .princeimg {
	width: auto;
    height: auto;
    border: none;
    position: absolute;
    margin-left: -3%;
    margin-top: -2.6%;
}
.workimg {
	width: 188px;
    text-align: center;
    display: inline-block;
    margin-right: 2%;
    overflow: hidden;
}
.workimg .leftimg {
	height: 98px;
}
.workimg span {
	display: inline-block;
    margin: 5px 0 0;
    font-size: 0.9rem;
    color: #333;
}
.beforemember .bemember {
	background: #0dc06f;
}
.beforemember h2 .modals {
	color: #666666;
	border: 1px solid #c2c2c2;
}
.beforemember h2 a:hover {
	border: 1px solid #0dc06e;
    color: #0dc06e;
}
.beforemember .btns .recharge {
	background: #0dc06f;
}
.beforemember .btns .changepw {
	color: #666;
    border: 1px solid #c2c2c2;
}
.beforemember .btns .recharge:hover {
    box-shadow: 1px 1px 0 1px #0b9b5a;
}
.beforemember .btns .changepw:hover {
	border: 1px solid #0dc06e;
	color: #0dc06e;
}
.beforeColor .libottom .baselink {
	border: 1px solid #0dc06d;
    color: #333;
}
.beforeColor .libottom .baselink:hover {
	background: #0dc06d;
	color: #fff;
}
.beforeColor .mergelist li:hover .mergetitle {
	color: #0dc06e;
}
.beforeColor .libottom .mergelist {
	padding-bottom: 20px;
}
.beforemember .maintoplfbottom a:hover {
	color: #0dc06f;
}
.deleteModel{
	margin-left: 280px;
    font-size: 13px;
    color: #797979;
}