.wraper {
	width: 100%;
	background: #dce9f7 url(../../imgs/default/findpwbg.png) no-repeat center 30px;
	background-size: 100%;
}
.container {
	width: 1000px;
	min-height: 688px;
}
.main {
	width: 880px;
	padding-top: 70px;
}
.topcommon {
	text-align: left;
	height: 40px;
	margin-bottom: 48px;
	padding-bottom: 2px;
	overflow: hidden;
}
.topcommon img {
	width: 112px;
	height: 40px;
	overflow: hidden;
	vertical-align: bottom;
}
.topcommon h2 {
	display: inline-block;
	font-size: 21px;
	color: #fff;
	margin-left: 15px;
	vertical-align: bottom;
	letter-spacing: 1px;
	overflow: hidden;
}
.midcommon {
	background: #faf8f5;
	border: 1px solid #c7c7c6;
	overflow: hidden;
	width: 935px;
	min-height: 560px;
	border-radius: 15px;
	margin-bottom: 80px;
}
.mainmid {
	width: 900px;
	margin-top: 40px;
	padding-bottom: 20px;
	text-align: left;
}
.mainmid h2 {
	font-size: 15px;
	color: #373636;
	line-height: 22px;
	overflow: hidden;
	margin-bottom: 15px;
}
.mainmid h2 img {
	width: 25px;
	height: 23px;
	overflow: hidden;
	vertical-align: middle;
	margin-right: 5px;
}
.midtop h2 b {
	color: #b03d3c;
	margin: 0 5px;
}
.vipinfor {
	border-bottom: 1px solid #d2d2d2;
	padding-bottom: 20px;
	margin-top: 30px;
	overflow: hidden;
}
.vipinfor li {
	margin-top: 10px;
}
.vipinfor span {
	width: 80px;
	display: inline-block;
	text-align: left;
	font-size: 14px;
	color: #686766;
	overflow: hidden;
}
.vipnumber {
	color: #ff0000;
	text-align: left;
	font-weight: bold;
	font-size: 13px;
}
.vipmail {
	color: #77ACE8;
	font-size: 14px;
	text-align: left;
}
.midmiddle , .midbottom {
	padding-top: 15px;
}
.midmiddle h2 , .midbottom h2 {
	font-size: 14px;
	color: #575656;
}

.midcontent {
	width: 835px;
}
.links {
	border-bottom: 1px solid #d2d2d2;
	padding-bottom: 20px;
	overflow: hidden;
	position: relative;
}
.links a {
	display: inline-block;
	width: 115px;
	height: 35px;
	line-height: 35px;
	text-align: center;
	font-size: 14px;
	color: #fff;
	overflow: hidden;
	margin-right: 58px;
}
.links .greenbtn {
	background: #66cc33 none;
}
.links .greenbtn:hover {
	background: #fff url(../../imgs/default/side01.png) no-repeat left center;
	color: #66cc33;
}
.links .redbtn {
	background: #ff6633 none;
}
.links .redbtn:hover {
	background: #fff url(../../imgs/default/side02.png) no-repeat left center;
	color: #ff6633;
}
.links .purplebtn {
	background: #5f52a0 none;
}
.links .purplebtn:hover {
	background: #fff url(../../imgs/default/side03.png) no-repeat left center;
	color: #5f52a0;
}
.links .bluebtn {
	background: #009999 none;
}
.links .bluebtn:hover {
	background: #fff url(../../imgs/default/side04.png) no-repeat left center;
	color: #009999;
}
.links .orangebtn {
	background: #ff9900 none;
	margin-right: 0;
	position: absolute;
	right: 14px;
}
.links .orangebtn:hover {
	background: #fff url(../../imgs/default/side05.png) no-repeat left center;
	color: #ff9900;
}
.weblist {
	text-align: left;
	overflow: hidden;
}
.weblist .weblitem {
	display: inline-block;
	width: 195px;
	height: 180px;
	overflow: hidden;
	background: #e5e5e5;
	margin-right: 10px;
	margin-bottom: 25px;
}
.litop {
	display: block;
	width: 195px;
	height: 135px;
	position: relative;
	overflow: hidden;
}
.litop img {
	width: 195px;
	height: 135px;
	overflow: hidden;
}
.litop h3 {
	height: 30px;
	line-height: 30px;
	color: #fff;
	font-size: 17px;
	overflow: hidden;
	text-align: center;
	width: 193px;
	position: absolute;
	bottom: 0;
	left: 2px;
	background: #000 url(../../imgs/default/whitearrow.png) no-repeat 146px 10px;
	background-color: rgba(0,0,0,0.8);
}
.litop:hover h3 {
	color: #e60012;
	background: #000 url(../../imgs/default/redarrow.png) no-repeat 160px 10px;
	transition: all 0.1s linear;
	-webkit-transition: all 0.1s linear;
	-o-transition: all 0.1s linear;
	-moz-transition: all 0.1s linear;
	-ms-transition: all 0.1s linear;
}
.libottom {
	width: 189px;
	margin: 0 auto;
	margin-left: 4px;
	margin-top: 5px;
	overflow: hidden;
}
.libottom span {
	display: block;
	font-size: 12px;
	color: #6f6f6f;
	height: 13px;
	line-height: 13px;
	overflow: hidden;
	margin-bottom: 5px;
}
.webserves a {
	font-size: 14px;
	color: #5986CD;
	display: inline-block;
	width: 60px;
	text-align: left;
	overflow: hidden;
	padding-bottom: 3px;
}
.webserves a:hover {
	color: #37aae9;
	text-decoration: underline;
}

/* 填写账户信息样式 */
.logotop {
	width: 850px;
	margin: 0 auto;
	text-align: left;
	height: 90px;
	line-height: 95px;
	font-size: 18px;
	color: #7d7d7d;
	overflow: hidden;
	border-bottom: 1px solid #d2d2d2;
}
.logotop img {
	width: 165px;
	height: 60px;
	overflow: hidden;
	vertical-align: top;
	margin-right: 20px;
	margin-top: 15px;
}
.registercontainer {
	width: 700px;
	margin: 0 auto;
	text-align: left;
	padding-top: 25px;
	padding-bottom: 30px;
	overflow: hidden;
}
.steps {
	margin-bottom: 40px;
}
.stepitem {
	display: inline-block;
	width: 100px;
	text-align: center;
}
.stepitem span {
	display: block;
	text-align: center;
	font-size: 15px;
	color: #696766;
}
.stepitem .rectbg {
	display: inline-block;
	width: 28px;
	height: 28px;
	text-align: center;
	background: url(../../imgs/default/grayrect.png) no-repeat center center;
	line-height: 28px;
	color: #fff;
	font-size: 14px;
	margin-bottom: 15px;
}
.linebg {
	width: 120px;
	height: 2px;
	background: #c9c9c9;
	display: inline-block;
	vertical-align: top;
	margin-top: 13px;
	margin-left: -28px;
	margin-right: -30px;
	overflow: hidden;
}
.redlinebg {
	background: #d10000;
}
.curstepitem .rectbg {
	background: url(../../imgs/default/redrect.png) no-repeat center center;
}
.registercontmiddle h2 , .registercontbottom h2 {
	font-size: 16px;
	color: #ff8b60;
	text-align: left;
	padding-bottom: 15px;
	border-bottom: 1px solid #d2d2d2;
	overflow: hidden;
}
.labelbase {
	padding-top: 26px;
}
.labelbase li {
	position: relative;
	margin-bottom: 25px;
}
.labelbase label {
	width: 118px;
	height: 18px;
	line-height: 18px;
	display: inline-block;
	text-align: left;
	font-size: 15px;
	color: #787674;
	overflow: hidden;
}
.labelbase i {
	font-size: 15px;
	color: #E34342;
	vertical-align: middle;
}
.labelbase input {
	width: 282px;
	height: 34px;
	border: 1px solid #cdcdcd;
	padding-left: 15px;
	padding-right: 10px;
	line-height: 20px;
	font-size: 14px;
	color: #484545;
	overflow: hidden;
}
.labelbase input:focus {
	box-shadow: 0 0 3px #4BA1FB;
}
.tipnote {
	display: inline-block;
	width: 254px;
	height: 36px;
	line-height: 36px;
	padding-left: 40px;
	text-align: left;
	position: absolute;
	right: -18px;
	color: #646961;
}
.tipright {
	background: url(../../imgs/default/tipbg.png) no-repeat center center;
}
.tipwrong {
	background: url(../../imgs/default/tipnobg.png) no-repeat center center;
}
.tipwrong img {
	width: 15px;
	height: 14px;
	overflow: hidden;
	vertical-align: middle;
	margin-right: 5px;
	margin-top: -4px;
}
#certifycode {
	width: 125px;
	height: 34px;
}
.codeimg {
	width: 90px;
	height: 32px;
	overflow: hidden;
	vertical-align: middle;
	margin-left: 15px;
}
.refresh {
	font-size: 14px;
	margin-left: 17px;
	color: #8AD9C7;
}
.refresh:hover {
	color: #dc6462;
	text-decoration: underline;
}
#checkbox {
	width: inherit;
	height: inherit;
	padding: 0;
	margin-left: 10px;
	vertical-align: top;
}
.labelbase .agreens {
	width: inherit;
	font-size: 13px;
	color: #9D9D9D;
	margin-left: 3px;
	padding-bottom: 3px;
}
.agreement {
	color: #5788C0;
	margin: 0 5px;
}
.agreement:hover {
	color: #65b8ec;
	text-decoration: underline;
}
.formbottom {
	text-align: center;
	padding-top: 5px;
	margin-bottom: 50px;
	overflow: hidden;
}
.submit {
	width: 114px;
	height: 38px;
	border-radius: 5px;
	outline: none;
	line-height: 38px;
	font-size: 15px;
	color: #fff;
	background: #c61420;
	overflow: hidden;
	text-align: center;
	display: inline-block;
}
.submit:hover {
	background: #DC0C0C;
	cursor: pointer;
}
.disabled {
	background: #bdbdbd;
}
.disabled:hover {
	background: #bdbdbd;
	cursor: pointer;
}
.loginto {
	font-size: 13px;
	color: #d42323;
	margin-left: 25px;
	vertical-align: middle;
}
.loginto:hover {
	color: #4BD09C;
}
.registercontbottom {
	padding-bottom: 50px;
}
.rightslist {
	padding-top: 15px;
}
.rightslist li {
	font-size: 13px;
	color: #787674;
	line-height: 20px;
	margin-bottom: 5px;
	text-align: left;
	overflow: hidden;
}
/* 注册第二步验证注册邮箱样式 */
.goemail {
	margin-bottom: 90px;
}
.mailtip {
	font-size: 18px;
