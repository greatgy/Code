	overflow: hidden;
}
.inputs li input:focus {
	box-shadow: 0 0 3px #4BA1FB;
}
.inputs .tips {
	width: 190px;
	height: 30px;
	display: inline-block;
	font-size: 12px;
	color: #6C6C6C;
	line-height: 30px;
	float: right;
	position: absolute;
	right: 0;
	background: url(../../imgs/default/notbg.png) no-repeat left center;
	background-size: 100% 100%;
}
.inputs .tipswrong {
	background: url(../../imgs/default/notbg.png) no-repeat left center;
	background-size: 100% 100%;
}
.inputs .tipsright {
	background: url(../../imgs/default/tiprig.png) no-repeat left center;
	background-size: 100% 100%;
}
.tips img {
	width: 14px;
	height: 13px;
	overflow: hidden;
	padding-left: 15px;
	vertical-align: baseline;
	margin-right: 2px;
}
.changeboxcontent .submit {
	display: block;
}

.tipyes {
	background: url(../../imgs/default/tipright.png) no-repeat center center;
}
.tipyes img {
	width: 15px;
	height: 14px;
	overflow: hidden;
	vertical-align: middle;
	margin-right: 5px;
	margin-top: -3px;
}
.baseinfor h4 {
	margin-top: 25px;
	font-size: 14px;
	color: #333030;
	font-family: arial;
}
.baseinfor h4 b {
	color: #d10000;
	font-family: "宋体";
	font-weight: bold;
	font-size: 15px;
}
.names {
	margin: 20px 0;
}
.names input {
	width: 17px;
	height: 17px;
	outline: none;
	border: none;
	overflow: hidden;
	margin-left: 90px;
}
.names input:focus {
	box-shadow: none;
}
.labelbase .special {
	width: inherit;
	margin-left: 8px;
}
.baseinforlist h3 {
	line-height: 18px;
	color: #737272;
	font-size: 15px;
	overflow: hidden;
}
.baseinforbottom {
	margin-top: 45px;
}
.strength {
	margin: -10px auto -10px 0;
    width: 80%;
    text-align: center;
	/* display: none; */
}
.strength span {
	display: inline-block;
	width: 64px;
	height: 16px;
	text-align: center;
	background: #ede2d5;
	line-height: 16px;
	font-size: 12px;
	color: #6e6c6b;
	margin-right: 5px;
}
.strength .curstrength {
	background: #ddcbb6;
}
.nextsubmit {
	width: 136px;
}
/* 缴费提示样式 */
.tipnumbers {
	width: 100%;
	height: 255px;
	overflow: hidden;
	position: relative;
}
.tip1 {
	width: 530px;
	height: 220px;
	overflow: hidden;
	position: absolute;
	z-index: 1;
	left: 40px;
	top: 0;
}
.tip2 {
	z-index: 2;
	left: 70px;
	top: 17px;
}
.tip3 {
	z-index: 3;
	left: 100px;
	top: 34px;
}
.blueblock {
	display: inline-block;
	width: 42px;
	height: 42px;
	background: url(../../imgs/default/bluerect.png) no-repeat center center;
	line-height: 42px;
	text-align: center;
	font-size: 12px;
	font-weight: bold;
	color: #faf8f5;
	position: absolute;
	z-index: 1;
	margin-left: 0;
	overflow: hidden;
}
.tip1container {
	width: 500px;
	height: 194px;
	border: 2px solid #d1444d;
	background: #fff1f2;
	position: relative;
	top: 20px;
	left: 8px;
}
.tip1content {
	width: 400px;
	margin: 0 auto;
	margin-top: 26px;
	text-align: left;
}
.tip1content h3 {
	text-align: center;
	font-size: 18px;
	color: #4c5254;
	margin-bottom: 20px;
}
.tip1content p {
	text-align: left;
	font-size: 14px;
	color: #5d6467;
	line-height: 21px;
}
.tip1content p a {
	color: #B170AE;
}
.tip1content p a:hover {
	color: #15C188;
}
.tip1content .check {
	margin-top: 8px;
    font-size: 12px;
    color: #333;
}
.check input {
	vertical-align: top;
}
.buttons {
	text-align: center;
	padding: 30px 0;
}
.prevpage {
	width: 114px;
	height: 38px;
	border-radius: 5px;
	outline: none;
	line-height: 38px;
	font-size: 15px;
	color: #333333;
	background: #faf8f5;
	border: 1px solid #313131;
	overflow: hidden;
	text-align: center;
	display: inline-block;
	margin-right: 30px;
	letter-spacing: 1px;
}
.prevpage:hover {
	background: #EDEDED;
	color: #5B98B7;
	cursor: pointer;
	border: 1px solid #ddd;
}
/* 付款页面样式 */
.payways {
	margin-bottom: 55px;
}
.wayslist {
	margin-top: 20px;
	text-align: left;
}
.wayslist li {
	margin-bottom: 20px;
	font-size: 15px;
	color: #787674;
	overflow: hidden;
}
.wayslist li b {
	color: #ff8252;
	font-weight: bold;
	font-size: 14px;
}
.wayslist li span {
	display: inline-block;
	width: 105px;
	text-align: left;
	vertical-align: top;
	margin-top: 8px;
	overflow: hidden;
}
.paywaysright {
	display: inline-block;
	width: 545px;
	text-align: left;
	overflow: hidden;
}
.bank {
	display: inline-block;
	margin-right: 15px;
	margin-bottom: 25px;
	overflow: hidden;
}
.bank img {
	vertical-align: middle;
	margin-left: 5px;
	width: 126px;
	height: 37px;
	overflow: hidden;
}
.pay {
	font-size: 15px;
	color: #fff;
	background: #c61420;
	display: block;
	width: 112px;
	height: 38px;
	line-height: 38px;
	text-align: center;
	border-radius: 8px;
	letter-spacing: 1px;
}
.pay:hover {
	box-shadow: 0 1px 0 1px #9D0B0B;
	opacity: .7;
	background: #DA2222;
}
.rechargecover {
	width: 350px;
	height: 180px;
	background-color: rgba(255,255,255,0.6);
	border-radius: 8px;
}
.rechargecoverment {
	width: 300px;
	margin: 0 auto;
	background-color: #fff;
	padding: 6px 15px;
	margin-top: 8px;
	border-radius: 8px;
}
.rechargecoverment h1 {
	text-align: left;
	font-size: 16px;
	color: #F49291;
	margin-top: 20px;
	overflow: hidden;
}
.rechargecoverment h1 img {
	width: 53px;
	height: 53px;
	overflow: hidden;
	vertical-align: middle;
	margin-right: 5px;
}
.rechargecoverment .tipreturn {
	margin: 10px auto;
	font-size: 12px;
	color: #535353;
}
.rechargecoverment .recharbtns {
	margin-top: 20px;
}
.recharbtns a {
	width: 122px;
	height: 34px;
	display: inline-block;
	text-align: center;
	line-height: 34px;
	font-size: 14px;
	border-radius: 5px;
	outline: none;
	overflow: hidden;
}
.recharbtns .finish {
	margin-right: 10px;
	background: #c61420;
	color: #fff;
}
.recharbtns .finish:hover {
	background: #da1523;
	box-shadow: 0 1px 0 1px #BA0C0C;
	opacity: .7;
}
.recharbtns .default {
	border: 1px solid #ddd;
	color: #535353;
	background: #fff;
}
.recharbtns .default:hover {
	background: #918F8F;
	color: #fff;
	box-shadow: 0 1px 0 0 #D5E3E4;
	opacity: .7;
}

