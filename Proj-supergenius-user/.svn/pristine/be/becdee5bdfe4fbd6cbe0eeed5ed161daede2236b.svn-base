	display: block;
	margin-bottom: 10px;
}
.moretest {
	padding-bottom: 45px;
}
.questionlist {
	padding-top: 20px;
	font-size: 15px;
	color: #696766;
	overflow: hidden;
}
.questionlist li {
	margin-bottom: 10px;
}
.questionlist a {
	margin-left: 5px;
	color: #2373BC;
}
.questionlist a:hover {
	color: #6FB9E8;
	text-decoration: underline;
}
.changebox {
	width: 425px;
	height: 210px;
	background: url(../../imgs/default/morebg.png) no-repeat center center;
	overflow: hidden;
	display: none;
}
.changeboxcontent {
	width: 385px;
	height: 152px;
	text-align: left;
	padding: 8px;
	border: 1px dotted #ccc;
