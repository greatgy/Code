<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.genius.xo.baseadmin.dao.AdminLogDao">
	
	<sql id="basecolumns">`uid`,`createtime`</sql>
	<sql id="othercolumns">`dataid`, `adminuid`, `channel`, `operation` , `data` , `desc`</sql>
	<sql id="allcolumns"><include refid="basecolumns"/>, <include refid="othercolumns"/></sql>
	<sql id="table">adminlog</sql>
	<sql id="whereid">where uid = #{uid}</sql>
	<sql id="wheredeletebyuid">where uid = #{uid}</sql>
	<sql id="allfields"><include refid="basecolumns"/>, <include refid="othercolumns"/></sql>
	<sql id="allparams">#{uid},#{createtime},#{dataid},#{adminuid},#{channel},#{operation},#{data},#{desc}</sql>
	<sql id="count">count(*)</sql>
	<sql id="setuid">uid = #{uid}</sql>
	<sql id="setdataid">dataid = #{dataid}</sql>
	<sql id="setadminuid">adminuid = #{adminuid}</sql>
	<sql id="setchannel">channel = #{channel}</sql>
	<sql id="setoperation">operation = #{operation}</sql>
	<sql id="setdata">data = #{data}</sql>
	<sql id="setdesc">`desc` = #{desc}</sql>
	<sql id="orderby">order by createtime</sql>
	<sql id="allsets">set <include refid="setdataid"/>, <include refid="setadminuid"/>, <include refid="setchannel"/>, <include refid="setoperation"/>, <include refid="setdata"/>, <include refid="setdesc"/></sql>
		
	<select id="get" parameterType="string" resultType="adminLog">
		select
		<include refid="allcolumns" />
		from
		<include refid="table"/>
		<include refid="whereid"/>
	</select>
	
	<insert id="insert" parameterType="adminLog">
		insert into
		<include refid="table"/>
		(<include refid="allfields"/>)
		values (
			<include refid="allparams"/>
		)
	</insert>
	
	<delete id="delete" parameterType="string">
		delete from
		<include refid="table"/>
		<include refid="wheredeletebyuid"/>
	</delete>
	
	<select id="getOne" parameterType="hashMap" resultType="adminLog">
		select
		<include refid="allcolumns" />
		from
		<include refid="table"/>
		<where>
			<if test="dataid != null">
				and <include refid="setdataid"/>
			</if>
			<if test="adminuid != null">
				and <include refid="setadminuid"/>
			</if>
			<if test="channel != null">
				and <include refid="setchannel"/>
			</if>
			<if test="operation != null">
				and <include refid="setoperation"/>
			</if>	
			<if test="data != null">
				and <include refid="setdata"/>
			</if>
			<if test="desc != null">
				and <include refid="setdesc"/>
			</if>
		</where>
		LIMIT 0,1
	</select>
	
	<select id="getCount" parameterType="hashMap" resultType="int">
		select
		<include refid="count" />
		from
		<include refid="table"/>
		<where>
			<if test="dataid != null">
				and <include refid="setdataid"/>
			</if>
			<if test="adminuid != null">
				and <include refid="setadminuid"/>
			</if>
			<if test="channel != null">
				and <include refid="setchannel"/>
			</if>
			<if test="operation != null">
				and <include refid="setoperation"/>
			</if>	
			<if test="data != null">
				and <include refid="setdata"/>
			</if>
			<if test="desc != null">
				and <include refid="setdesc"/>
			</if>
		</where>
	</select>
	
	<select id="getList" parameterType="hashMap" resultType="adminLog">
		select
		<include refid="allcolumns" />
		from
		<include refid="table"/>
		<where>
			<if test="dataid != null">
				and <include refid="setdataid"/>
			</if>
			<if test="adminuid != null">
				and <include refid="setadminuid"/>
			</if>
			<if test="channel != null">
				and <include refid="setchannel"/>
			</if>
			<if test="operation != null">
				and <include refid="setoperation"/>
			</if>	
			<if test="data != null">
				and <include refid="setdata"/>
			</if>
			<if test="desc != null">
				and <include refid="setdesc"/>
			</if>
		</where>
		<!-- 处理orderby语句 -->
		<choose>
			<when test="orderBy != null">
				${orderBy}
			</when>
			<otherwise>
				<include refid="orderby"/>
				<if test="ascDesc != null">
					${ascDesc}
				</if>
			</otherwise>
		</choose>
		<!-- 处理limit语句 -->
		<trim prefix="LIMIT" prefixOverrides=",">
			<if test="startIndex != null and pageSize != null">
				${startIndex}
			</if>
			<if test="pageSize != null">
			 , ${pageSize}
			 </if>
		</trim>
	</select>
	
	<update id="update" parameterType="adminLog">
		update
		<include refid="table"/>
		<include refid="allsets"/>
		<include refid="whereid"/>
	</update>
	
</mapper>