	<select id="getInitList" parameterType="hashMap" resultType="contribute">
		select
		<include refid="allcolumns" />
		from
		<include refid="table"/>
		<where>
			useruid="${useruid}" and 
			(
				state=0	or (state=1 and uid in (select contributeuid from finance f where f.status=10 and f.authoruid="${useruid}"))
			)
			<if test="status != null">
				and <include refid="setstatus"/>
			</if>
		</where>
		<!-- 处理orderby语句 -->
		<choose>
			<when test="orderBy != null">
				${orderBy}
			</when>
			<otherwise>
				<include refid="orderby"/>
