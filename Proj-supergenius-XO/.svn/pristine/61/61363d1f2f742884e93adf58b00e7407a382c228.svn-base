			</if>
			<if test="state != null">
				and <include refid="setstate"/>
			</if>
			<if test="states != null">
				and state in
				<foreach item="item" index="index" collection="states" open="(" separator="," close=")">
                 	 #{item}
            	</foreach>
			</if>
			<if test="status != null">
				and <include refid="setstatus"/>
			</if>
			<if test="createtimestart != null">
				and <include refid="setcreatetimestart" />
			</if>
			<if test="createtimeend != null">
				and <include refid="setcreatetimeend" />
			</if>
		</where>
		<!-- 处理orderby语句 -->
		<choose>
			<when test="orderBy != null">
				${orderBy}
			</when>
			<otherwise>
				<include refid="orderby"/>
				<if test="ascDesc != null">
					${ascDesc}
				</if>
			</otherwise>
		</choose>
		<!-- 处理limit语句 -->
		<trim prefix="LIMIT" prefixOverrides=",">
			<if test="startIndex != null and pageSize != null">
