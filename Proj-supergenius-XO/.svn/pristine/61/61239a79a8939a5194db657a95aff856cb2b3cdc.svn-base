package com.supergenius.xo.tpi.entity;

import com.genius.core.base.annotation.Json;
import com.genius.core.base.annotation.Maps;

/**
 * 并购指标
 * 
 * @author ShangJianguo
 */
@Maps(strategy = Maps.dbStrategy)
public class MergerIndicator {

	//股权可转让性
	@Json(strategy = Json.webStrategy)
	private int assignwill;// 实际控制人控制权出让意愿（0-10）
	@Json(strategy = Json.webStrategy)
	private int othersharewill;// 让外部资本参股的意愿0-10
	@Json(strategy = Json.webStrategy)
	private int assignfree;// 股权转让自由度 0-10
	
	//行业层面
	@Json(strategy = Json.webStrategy)
	private int industrylaw;//行业法律政策环境
	@Json(strategy = Json.webStrategy)
	private int industrygrowing;//行业成长前景
	@Json(strategy = Json.webStrategy)
	private int industryperiod;//行业周期性
	@Json(strategy = Json.webStrategy)
	private int industrybarrier;//行业进入壁垒
	@Json(strategy = Json.webStrategy)
	private int industryconcentration;//行业集中度
	@Json(strategy = Json.webStrategy)
	private int industrychain;//产业链环境
	
	//企业层面
	@Json(strategy = Json.webStrategy)
	private int firmstrategy;//战略管理水平
	@Json(strategy = Json.webStrategy)
	private int firmoperations;//运营管理水平
	@Json(strategy = Json.webStrategy)
	private int firmservice;//产品和服务的竞争力
	@Json(strategy = Json.webStrategy)
	private int firmsshare;//核心市场份额排名
		//技术地位
	@Json(strategy = Json.webStrategy)
	private int firmsglobalkill;//在全球行业中的技术地位
	@Json(strategy = Json.webStrategy)
	private int firmschinakill;//在中国行业中的技术地位
	@Json(strategy = Json.webStrategy)
	private int firmbrand;//品牌地位
	@Json(strategy = Json.webStrategy)
	private int firmcoverage;//国际市场覆盖率
		//市场成长性
	@Json(strategy = Json.webStrategy)
	private int firmgrowth;//国际市场成长性
	@Json(strategy = Json.webStrategy)
	private int firmdomestic;//国内市场成长性
	@Json(strategy = Json.webStrategy)
	private int firmfinance;//财务健康指数
	@Json(strategy = Json.webStrategy)
	private int firmsustainable;//可持续发展能力
	
	//并购机会与并购风险
	@Json(strategy = Json.webStrategy)
	private int risk;// 并购风险0-10
	@Json(strategy = Json.webStrategy)
	private int opportunity;// 并购时机 0-10
	
	//并购能力和经验方面的要求
	@Json(strategy = Json.webStrategy)
	private int peermaindex;// 适合同行业并购指数
	@Json(strategy = Json.webStrategy)
	private int investmaindex;// 适合投资者并购指数
	@Json(strategy = Json.webStrategy)
	private int ambitionmaindex;// 适合野心并购指数
	
	public int getAssignwill() {
		return assignwill;
	}
	
	public void setAssignwill(int assignwill) {
		this.assignwill = assignwill;
	}
	
	public int getOthersharewill() {
		return othersharewill;
	}
	
	public void setOthersharewill(int othersharewill) {
		this.othersharewill = othersharewill;
	}
	
	public int getAssignfree() {
		return assignfree;
	}
	
	public void setAssignfree(int assignfree) {
		this.assignfree = assignfree;
	}
	
	public int getIndustrylaw() {
		return industrylaw;
	}
	
	public void setIndustrylaw(int industrylaw) {
		this.industrylaw = industrylaw;
	}
	
	public int getIndustrygrowing() {
		return industrygrowing;
	}
	
	public void setIndustrygrowing(int industrygrowing) {
		this.industrygrowing = industrygrowing;
	}
	
	public int getIndustryperiod() {
		return industryperiod;
	}
	
	public void setIndustryperiod(int industryperiod) {
		this.industryperiod = industryperiod;
	}
	
	public int getIndustrybarrier() {
		return industrybarrier;
	}
	
	public void setIndustrybarrier(int industrybarrier) {
		this.industrybarrier = industrybarrier;
	}
	
	public int getIndustryconcentration() {
		return industryconcentration;
	}
	
	public void setIndustryconcentration(int industryconcentration) {
		this.industryconcentration = industryconcentration;
	}
	
	public int getIndustrychain() {
		return industrychain;
	}
	
	public void setIndustrychain(int industrychain) {
		this.industrychain = industrychain;
	}
	
	public int getFirmstrategy() {
		return firmstrategy;
	}
	
	public void setFirmstrategy(int firmstrategy) {
		this.firmstrategy = firmstrategy;
	}
	
	public int getFirmoperations() {
		return firmoperations;
	}
	
	public void setFirmoperations(int firmoperations) {
		this.firmoperations = firmoperations;
	}
	
	public int getFirmservice() {
		return firmservice;
	}
	
	public void setFirmservice(int firmservice) {
		this.firmservice = firmservice;
	}
	
	public int getFirmsshare() {
		return firmsshare;
	}
	
	public void setFirmsshare(int firmsshare) {
		this.firmsshare = firmsshare;
	}
	
	public int getFirmsglobalkill() {
		return firmsglobalkill;
	}
	
	public void setFirmsglobalkill(int firmsglobalkill) {
		this.firmsglobalkill = firmsglobalkill;
	}
	
	public int getFirmschinakill() {
		return firmschinakill;
	}
	
	public void setFirmschinakill(int firmschinakill) {
		this.firmschinakill = firmschinakill;
	}
	
	public int getFirmbrand() {
		return firmbrand;
	}
	
	public void setFirmbrand(int firmbrand) {
		this.firmbrand = firmbrand;
	}
	
	public int getFirmcoverage() {
		return firmcoverage;
	}
	
	public void setFirmcoverage(int firmcoverage) {
		this.firmcoverage = firmcoverage;
	}
	
	public int getFirmgrowth() {
		return firmgrowth;
	}
	
	public void setFirmgrowth(int firmgrowth) {
		this.firmgrowth = firmgrowth;
	}
	
	public int getFirmdomestic() {
		return firmdomestic;
	}
	
	public void setFirmdomestic(int firmdomestic) {
		this.firmdomestic = firmdomestic;
	}
	
	public int getFirmfinance() {
		return firmfinance;
	}
	
	public void setFirmfinance(int firmfinance) {
		this.firmfinance = firmfinance;
	}
	
	public int getFirmsustainable() {
		return firmsustainable;
	}
	
	public void setFirmsustainable(int firmsustainable) {
		this.firmsustainable = firmsustainable;
	}
	
	public int getRisk() {
		return risk;
	}
	
	public void setRisk(int risk) {
		this.risk = risk;
	}
	
	public int getOpportunity() {
		return opportunity;
	}
	
	public void setOpportunity(int opportunity) {
		this.opportunity = opportunity;
	}
	
	public int getPeermaindex() {
		return peermaindex;
	}
	
	public void setPeermaindex(int peermaindex) {
		this.peermaindex = peermaindex;
	}
	
	public int getInvestmaindex() {
		return investmaindex;
	}
	
	public void setInvestmaindex(int investmaindex) {
		this.investmaindex = investmaindex;
	}
	
	public int getAmbitionmaindex() {
		return ambitionmaindex;
	}
	
	public void setAmbitionmaindex(int ambitionmaindex) {
		this.ambitionmaindex = ambitionmaindex;
	}
	
}
