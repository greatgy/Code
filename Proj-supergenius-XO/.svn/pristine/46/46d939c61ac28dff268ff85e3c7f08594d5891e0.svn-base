<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.supergenius.xo.gupage.dao.PatentDao">

    <sql id="basecolumns">`uid`, `createtime` </sql>
    <sql id="othercolumns">`filepath`, `imglittle`, `imgmedium`, `imgbig`, `imgoriginal`, `kind`, `data`, `type`, `adminuid`, `status` </sql>
    <sql id="allcolumns"><include refid="basecolumns"/>, <include refid="othercolumns"/>, `updatetime`</sql>
    <sql id="table">patent</sql>
    <sql id="count">count(*)</sql>
    <sql id="whereid">where uid = #{uid}</sql>
    <sql id="allfields"><include refid="basecolumns"/>, <include refid="othercolumns"/> </sql>
    <sql id="allparams">#{uid},#{createtime},#{filepath},#{imglittle},#{imgmedium},#{imgbig},#{imgoriginal},#{kind},#{data},#{type},#{adminuid},#{status}</sql>
    <sql id="setfilepath">`filepath` = #{filepath}</sql>
    <sql id="setimglittle">`imglittle` = #{imglittle}</sql>
    <sql id="setimgmedium">`imgmedium` = #{imgmedium}</sql>
    <sql id="setimgbig">`imgbig` = #{imgbig}</sql>
    <sql id="setimgoriginal">`imgoriginal` = #{imgoriginal}</sql>
    <sql id="setkind">`kind` = #{kind}</sql>
    <sql id="setdata">`data` = #{data}</sql>
    <sql id="settype">`type` = #{type}</sql>
    <sql id="setadminuid">`adminuid` = #{adminuid}</sql>
    <sql id="setstatus">`status` = #{status}</sql>
    <sql id="allsets">set
            <include refid="setfilepath"/>,
            <include refid="setimglittle"/>,
            <include refid="setimgmedium"/>,
            <include refid="setimgbig"/>,
            <include refid="setimgoriginal"/>,
            <include refid="setkind"/>,
            <include refid="setdata"/>,
            <include refid="settype"/>,
            <include refid="setadminuid"/>,
            <include refid="setstatus"/>
    </sql>
    <sql id="orderby">order by createtime</sql>

    <select id="get" parameterType="string" resultType="patent">
        select
        <include refid="allcolumns" />
        from
        <include refid="table"/>
        <include refid="whereid"/>
    </select>

    <select id="getOne" parameterType="hashMap" resultType="patent">
        select
        <include refid="allcolumns" />
        from
        <include refid="table"/>
        <where>
            <if test="filepath != null">
                and <include refid="setfilepath"/>
            </if>
            <if test="imglittle != null">
                and <include refid="setimglittle"/>
            </if>
            <if test="imgmedium != null">
                and <include refid="setimgmedium"/>
            </if>
            <if test="imgbig != null">
                and <include refid="setimgbig"/>
            </if>
            <if test="imgoriginal != null">
                and <include refid="setimgoriginal"/>
            </if>
            <if test="kind != null">
                and <include refid="setkind"/>
            </if>
            <if test="data != null">
                and <include refid="setdata"/>
            </if>
            <if test="type != null">
                and <include refid="settype"/>
            </if>
            <if test="adminuid != null">
                and <include refid="setadminuid"/>
            </if>
            <if test="status != null">
                and <include refid="setstatus"/>
            </if>
        </where>
        LIMIT 0,1
    </select>

    <select id="getCount" parameterType="hashMap" resultType="int">
        select
        <include refid="count" />
        from
        <include refid="table"/>
        <where>
            <if test="filepath != null">
                and <include refid="setfilepath"/>
            </if>
            <if test="imglittle != null">
                and <include refid="setimglittle"/>
            </if>
            <if test="imgmedium != null">
                and <include refid="setimgmedium"/>
            </if>
            <if test="imgbig != null">
                and <include refid="setimgbig"/>
            </if>
            <if test="imgoriginal != null">
                and <include refid="setimgoriginal"/>
            </if>
            <if test="kind != null">
                and <include refid="setkind"/>
            </if>
            <if test="data != null">
                and <include refid="setdata"/>
            </if>
            <if test="type != null">
                and <include refid="settype"/>
            </if>
            <if test="adminuid != null">
                and <include refid="setadminuid"/>
            </if>
            <if test="status != null">
                and <include refid="setstatus"/>
            </if>
        </where>
    </select>

    <select id="getList" parameterType="hashMap" resultType="patent">
        select
        <include refid="allcolumns" />
        from
        <include refid="table"/>
        <where>
            <if test="filepath != null">
                and <include refid="setfilepath"/>
            </if>
            <if test="imglittle != null">
                and <include refid="setimglittle"/>
            </if>
            <if test="imgmedium != null">
                and <include refid="setimgmedium"/>
            </if>
            <if test="imgbig != null">
                and <include refid="setimgbig"/>
            </if>
            <if test="imgoriginal != null">
                and <include refid="setimgoriginal"/>
            </if>
            <if test="kind != null">
                and <include refid="setkind"/>
            </if>
            <if test="data != null">
                and <include refid="setdata"/>
            </if>
            <if test="type != null">
                and <include refid="settype"/>
            </if>
            <if test="adminuid != null">
                and <include refid="setadminuid"/>
            </if>
            <if test="status != null">
                and <include refid="setstatus"/>
            </if>
        </where>
        <!-- 处理orderby语句 -->
        <choose>
            <when test="orderBy != null">
                ${orderBy}
            </when>
            <otherwise>
                <include refid="orderby"/>
                <if test="ascDesc != null">
                    ${ascDesc}
                </if>
            </otherwise>
        </choose>
        <!-- 处理limit语句 -->
        <trim prefix="LIMIT" prefixOverrides=",">
            <if test="startIndex != null and pageSize != null">
                ${startIndex}
            </if>
            <if test="pageSize != null">
                , ${pageSize}
            </if>
        </trim>
    </select>

    <insert id="insert" parameterType="patent">
        insert into
        <include refid="table"/>
        (<include refid="allfields"/>)
        values (
        <include refid="allparams"/>
        )
    </insert>

    <update id="update" parameterType="patent">
        update
        <include refid="table"/>
        <include refid="allsets"/>
        <include refid="whereid"/>
    </update>

    <update id="updateFields" parameterType="hashMap">
        update
        <include refid="table"/>
        <set>
            <if test="filepath != null">
                <include refid="setfilepath"/>,
            </if>
            <if test="imglittle != null">
                <include refid="setimglittle"/>,
            </if>
            <if test="imgmedium != null">
                <include refid="setimgmedium"/>,
            </if>
            <if test="imgbig != null">
                <include refid="setimgbig"/>,
            </if>
            <if test="imgoriginal != null">
                <include refid="setimgoriginal"/>,
            </if>
            <if test="kind != null">
                <include refid="setkind"/>,
            </if>
            <if test="data != null">
                <include refid="setdata"/>,
            </if>
            <if test="type != null">
                <include refid="settype"/>,
            </if>
            <if test="adminuid != null">
                <include refid="setadminuid"/>,
            </if>
            <if test="status != null">
                <include refid="setstatus"/>
            </if>
        </set>
        <include refid="whereid"/>
    </update>

    <delete id="delete" parameterType="string">
        delete from
        <include refid="table"/>
        <include refid="whereid"/>
    </delete>

</mapper>

