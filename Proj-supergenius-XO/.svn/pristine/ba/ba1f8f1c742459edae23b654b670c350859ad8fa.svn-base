<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.supergenius.xo.user.dao.OrderGoodsDao">

    <sql id="basecolumns">`uid`, `createtime` </sql>
    <sql id="othercolumns"><include refid="basecolumns"/>, `orderuid`, `useruid`, `sn`, `name`, `type`, `count`, `unitprice`, `totalprice`, `status`, `refuid` </sql>
    <sql id="allcolumns"><include refid="basecolumns"/>, <include refid="othercolumns"/>, `updatetime`</sql>
    <sql id="table">`ordergoods`</sql>
    <sql id="count">count(*)</sql>
    <sql id="whereid">where uid = #{uid}</sql>
    <sql id="allfields"><include refid="othercolumns"/> </sql>
    <sql id="allparams">#{uid},#{createtime},#{orderuid},#{useruid},#{sn},#{name},#{type},#{count},#{unitprice},#{totalprice},#{status},#{refuid}</sql>
    <sql id="allitemparams">#{item.uid},#{item.createtime},#{item.orderuid},#{item.useruid},#{item.sn},#{item.name},#{item.type},#{item.count},#{item.unitprice},#{item.totalprice},#{item.status},#{item.refuid}</sql>
    <sql id="setorderuid">orderuid = #{orderuid}</sql>
    <sql id="orderuidin">orderuid in 
        <foreach item="item" index="index" collection="orderuid" open="(" separator="," close=")">  
  			#{item}  
 		</foreach>  
    </sql>
    <sql id="setuseruid">useruid = #{useruid}</sql>
    <sql id="useruidsin">useruid in 
    	<foreach item="item" index="index" collection="useruids" open="(" separator="," close=")">  
  			#{item}  
 		</foreach>  
    </sql>
    <sql id="setsn">sn = #{sn}</sql>
    <sql id="setsns">sn in (${sns})</sql>
    <sql id="setname">name = #{name}</sql>
    <sql id="settype">type = #{type}</sql>
    <sql id="setcount">count = #{count}</sql>
    <sql id="setunitprice">unitprice = #{unitprice}</sql>
    <sql id="settotalprice">totalprice = #{totalprice}</sql>
    <sql id="setstatus">status = #{status}</sql>
    <sql id="setrefuid">refuid = #{refuid}</sql>
    <sql id="allsets">set
            <include refid="setorderuid"/>,
            <include refid="setuseruid"/>,
            <include refid="setsn"/>,
            <include refid="setname"/>,
            <include refid="settype"/>,
            <include refid="setcount"/>,
            <include refid="setunitprice"/>,
            <include refid="settotalprice"/>,
            <include refid="setstatus"/>,
            <include refid="setrefuid"/>
    </sql>
    <sql id="orderby">order by createtime</sql>

    <select id="get" parameterType="string" resultType="OrderGoods">
        select
        <include refid="allcolumns" />
        from
        <include refid="table"/>
        <include refid="whereid"/>
    </select>

    <select id="getOne" parameterType="hashMap" resultType="OrderGoods">
        select
        <include refid="allcolumns" />
        from
        <include refid="table"/>
        <where>
            <if test="orderuid != null">
                and <include refid="setorderuid"/>
            </if>
            <if test="useruid != null">
                and <include refid="setuseruid"/>
            </if>
            <if test="sn != null">
                and <include refid="setsn"/>
            </if>
            <if test="name != null">
                and <include refid="setname"/>
            </if>
            <if test="type != null">
                and <include refid="settype"/>
            </if>
            <if test="count != null">
                and <include refid="setcount"/>
            </if>
            <if test="unitprice != null">
                and <include refid="setunitprice"/>
            </if>
            <if test="totalprice != null">
                and <include refid="settotalprice"/>
            </if>
            <if test="status != null">
                and <include refid="setstatus"/>
            </if>
            <if test="refuid != null">
                and <include refid="setrefuid"/>
            </if>
        </where>
        LIMIT 0,1
    </select>

    <select id="getCount" parameterType="hashMap" resultType="int">
        select
        <choose>
            <when test="distinct_user != null">
                count(DISTINCT `useruid`)
            </when>
            <otherwise>
		        <include refid="count" />
            </otherwise>
        </choose>
        from
        <include refid="table"/>
        <where>
            <if test="orderuid != null">
                and <include refid="setorderuid"/>
            </if>
            <if test="useruid != null">
                and <include refid="setuseruid"/>
            </if>
            <if test="useruids != null">
                and <include refid="useruidsin"/>
            </if>
            <if test="sn != null">
                and <include refid="setsn"/>
            </if>
            <if test="sn_lk != null">
                and sn like CONCAT('%',#{sn_lk},'%')
            </if>
            <if test="name != null">
                and <include refid="setname"/>
            </if>
            <if test="name_lk != null">
                and name like CONCAT('%',#{name_lk},'%')
            </if>
            <if test="type != null">
                and <include refid="settype"/>
            </if>
            <if test="count != null">
                and <include refid="setcount"/>
            </if>
            <if test="unitprice != null">
                and <include refid="setunitprice"/>
            </if>
            <if test="totalprice != null">
                and <include refid="settotalprice"/>
            </if>
            <if test="totalprice_gt != null">
                and totalprice &gt;= #{totalprice_gt}
            </if>
            <if test="totalprice_lt != null">
                and totalprice &lt;= #{totalprice_lt}
            </if>
            <if test="sns != null">
                and <include refid="setsns"/>
            </if>
            <if test="createtime_gt != null">
                and createtime &gt;= #{createtime_gt}
            </if>
            <if test="createtime_lt != null">
                and createtime &lt;= #{createtime_lt}
            </if>
            <if test="status != null">
                and <include refid="setstatus"/>
            </if>
            <if test="refuid != null">
                and <include refid="setrefuid"/>
            </if>
        </where>
    </select>

    <select id="getList" parameterType="hashMap" resultType="OrderGoods">
        select
        <include refid="allcolumns" />
        from
        <include refid="table"/>
        <where>
            <if test="orderuid != null">
                and <include refid="setorderuid"/>
            </if>
            <if test="useruid != null">
                and <include refid="setuseruid"/>
            </if>
            <if test="useruids != null">
                and <include refid="useruidsin"/>
            </if>
            <if test="sn != null">
                and <include refid="setsn"/>
            </if>
            <if test="sn_lk != null">
                and sn like CONCAT('%',#{sn_lk},'%')
            </if>
            <if test="name != null">
                and <include refid="setname"/>
            </if>
            <if test="name_lk != null">
                and name like CONCAT('%',#{name_lk},'%')
            </if>
            <if test="type != null">
                and <include refid="settype"/>
            </if>
            <if test="count != null">
                and <include refid="setcount"/>
            </if>
            <if test="unitprice != null">
                and <include refid="setunitprice"/>
            </if>
            <if test="totalprice != null">
                and <include refid="settotalprice"/>
            </if>
            <if test="totalprice_gt != null">
                and totalprice &gt;= #{totalprice_gt}
            </if>
            <if test="totalprice_lt != null">
                and totalprice &lt;= #{totalprice_lt}
            </if>
            <if test="sns != null">
                and <include refid="setsns"/>
            </if>
            <if test="createtime_gt != null">
                and createtime &gt;= #{createtime_gt}
            </if>
            <if test="createtime_lt != null">
                and createtime &lt;= #{createtime_lt}
            </if>
            <if test="status != null">
                and <include refid="setstatus"/>
            </if>
            <if test="refuid != null">
                and <include refid="setrefuid"/>
            </if>
        </where>
        <!-- 处理orderby语句 -->
        <choose>
            <when test="orderBy != null">
                ${orderBy}
            </when>
            <otherwise>
                <include refid="orderby"/>
                <if test="ascDesc != null">
                    ${ascDesc}
                </if>
            </otherwise>
        </choose>
        <!-- 处理limit语句 -->
        <trim prefix="LIMIT" prefixOverrides=",">
            <if test="startIndex != null and pageSize != null">
                ${startIndex}
            </if>
            <if test="pageSize != null">
                , ${pageSize}
            </if>
        </trim>
    </select>
    
    <select id="getBuyUserList" parameterType="hashMap" resultType="string">
        select
            DISTINCT(`useruid`)
        from
        <include refid="table"/>
        <where>
            <if test="orderuid != null">
                and <include refid="setorderuid"/>
            </if>
            <if test="useruid != null">
                and <include refid="setuseruid"/>
            </if>
            <if test="useruids != null">
                and <include refid="useruidsin"/>
            </if>
            <if test="sn != null">
                and <include refid="setsn"/>
            </if>
            <if test="sn_lk != null">
                and sn like CONCAT('%',#{sn_lk},'%')
            </if>
            <if test="name != null">
                and <include refid="setname"/>
            </if>
            <if test="name_lk != null">
                and name like CONCAT('%',#{name_lk},'%')
            </if>
            <if test="type != null">
                and <include refid="settype"/>
            </if>
            <if test="count != null">
                and <include refid="setcount"/>
            </if>
            <if test="unitprice != null">
                and <include refid="setunitprice"/>
            </if>
            <if test="totalprice != null">
                and <include refid="settotalprice"/>
            </if>
            <if test="totalprice_gt != null">
                and totalprice &gt;= #{totalprice_gt}
            </if>
            <if test="totalprice_lt != null">
                and totalprice &lt;= #{totalprice_lt}
            </if>
            <if test="sns != null">
                and <include refid="setsns"/>
            </if>
            <if test="createtime_gt != null">
                and createtime &gt;= #{createtime_gt}
            </if>
            <if test="createtime_lt != null">
                and createtime &lt;= #{createtime_lt}
            </if>
            <if test="status != null">
                and <include refid="setstatus"/>
            </if>
            <if test="refuid != null">
                and <include refid="setrefuid"/>
            </if>
        </where>
        <!-- 处理orderby语句 -->
        <choose>
            <when test="orderBy != null">
                ${orderBy}
            </when>
            <otherwise>
                <include refid="orderby"/>
                <if test="ascDesc != null">
                    ${ascDesc}
                </if>
            </otherwise>
        </choose>
        <!-- 处理limit语句 -->
        <trim prefix="LIMIT" prefixOverrides=",">
            <if test="startIndex != null and pageSize != null">
                ${startIndex}
            </if>
            <if test="pageSize != null">
                , ${pageSize}
            </if>
        </trim>
    </select>

    <insert id="insert" parameterType="OrderGoods">
        insert into
        <include refid="table"/>
        (<include refid="allfields"/>)
        values (
        <include refid="allparams"/>
        )
    </insert>
    
    <insert id="insertList" parameterType="list">
        insert into
        <include refid="table"/>
        (<include refid="allfields"/>)
        values
        <foreach item="item" index="index" collection="list" separator=",">
            (<include refid="allitemparams"/>)
        </foreach>
    </insert>
    
    <update id="update" parameterType="OrderGoods">
        update
        <include refid="table"/>
        <include refid="allsets"/>
        <include refid="whereid"/>
    </update>

    <update id="updateFields" parameterType="hashMap">
        update
        <include refid="table"/>
        <set>
            <if test="orderuid != null">
                <include refid="setorderuid"/>,
            </if>
            <if test="useruid != null">
                <include refid="setuseruid"/>,
            </if>
            <if test="sn != null">
                <include refid="setsn"/>,
            </if>
            <if test="name != null">
                <include refid="setname"/>,
            </if>
            <if test="type != null">
                <include refid="settype"/>,
            </if>
            <if test="count != null">
                <include refid="setcount"/>,
            </if>
            <if test="unitprice != null">
                <include refid="setunitprice"/>,
            </if>
            <if test="totalprice != null">
                <include refid="settotalprice"/>,
            </if>
            <if test="status != null">
                <include refid="setstatus"/>
            </if>
            <if test="refuid != null">
                <include refid="setrefuid"/>
            </if>
        </set>
        <include refid="whereid"/>
    </update>
    
    <update id="removeOrder" parameterType="string">
    	update
    	<include refid="table"/>
    	<set>
            <if test="status != null">
                <include refid="setstatus"/>
            </if>
        </set>
        <where>
        	<if test="orderuid != null">
                and <include refid="setorderuid"/>
            </if>
        </where>
    </update>

    <delete id="delete" parameterType="string">
        delete from
        <include refid="table"/>
        <include refid="whereid"/>
    </delete>
</mapper>

