<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.supergenius.xo.life.dao.ComplaintDao">

    <sql id="basecolumns">`uid`, `createtime` </sql>
    <sql id="othercolumns">`fromuseruid`, `fromuid`, `refurl`, `kind`, `refname`, `type`, `desc`, `file`, `result`, `status` </sql>
    <sql id="allcolumns"><include refid="basecolumns"/>, <include refid="othercolumns"/>, `updatetime`</sql>
    <sql id="table">complaint</sql>
    <sql id="count">count(*)</sql>
    <sql id="whereid">where uid = #{uid}</sql>
    <sql id="allfields"><include refid="basecolumns"/>, <include refid="othercolumns"/> </sql>
    <sql id="allparams">#{uid},#{createtime},#{fromuseruid},#{fromuid},#{refurl},#{kind},#{refname},#{type},#{desc},#{file},#{result},#{status}</sql>
    <sql id="setfromuseruid">`fromuseruid` = #{fromuseruid}</sql>
    <sql id="setfromuid">`fromuid` = #{fromuid}</sql>
    <sql id="setrefurl">`refurl` = #{refurl}</sql>
    <sql id="setkind">`kind` = #{kind}</sql>
    <sql id="setrefname">`refname` = #{refname}</sql>
    <sql id="settype">`type` = #{type}</sql>
    <sql id="setdesc">`desc` = #{desc}</sql>
    <sql id="setfile">`file` = #{file}</sql>
    <sql id="setresult">`result` = #{result}</sql>
    <sql id="setstatus">`status` = #{status}</sql>
    <sql id="allsets">set
            <include refid="setfromuseruid"/>,
            <include refid="setfromuid"/>,
            <include refid="setrefurl"/>,
            <include refid="setkind"/>,
            <include refid="setrefname"/>,
            <include refid="settype"/>,
            <include refid="setdesc"/>,
            <include refid="setfile"/>,
            <include refid="setresult"/>,
            <include refid="setstatus"/>
    </sql>
    <sql id="orderby">order by createtime</sql>

    <select id="get" parameterType="string" resultType="complaint">
        select
        <include refid="allcolumns" />
        from
        <include refid="table"/>
        <include refid="whereid"/>
    </select>

    <select id="getOne" parameterType="hashMap" resultType="complaint">
        select
        <include refid="allcolumns" />
        from
        <include refid="table"/>
        <where>
            <if test="fromuseruid != null">
                and <include refid="setfromuseruid"/>
            </if>
            <if test="fromuid != null">
                and <include refid="setfromuid"/>
            </if>
            <if test="refurl != null">
                and <include refid="setrefurl"/>
            </if>
            <if test="kind != null">
                and <include refid="setkind"/>
            </if>
            <if test="refname != null">
                and <include refid="setrefname"/>
            </if>
            <if test="type != null">
                and <include refid="settype"/>
            </if>
            <if test="desc != null">
                and <include refid="setdesc"/>
            </if>
            <if test="file != null">
                and <include refid="setfile"/>
            </if>
            <if test="result != null">
                and <include refid="setresult"/>
            </if>
            <if test="status != null">
                and <include refid="setstatus"/>
            </if>
        </where>
        LIMIT 0,1
    </select>

    <select id="getCount" parameterType="hashMap" resultType="int">
        select
        <include refid="count" />
        from
        <include refid="table"/>
        <where>
            <if test="fromuseruid != null">
                and <include refid="setfromuseruid"/>
            </if>
            <if test="fromuid != null">
                and <include refid="setfromuid"/>
            </if>
            <if test="refurl != null">
                and <include refid="setrefurl"/>
            </if>
            <if test="kind != null">
                and <include refid="setkind"/>
            </if>
            <if test="refname != null">
                and <include refid="setrefname"/>
            </if>
            <if test="type != null">
                and <include refid="settype"/>
            </if>
            <if test="desc != null">
                and <include refid="setdesc"/>
            </if>
            <if test="file != null">
                and <include refid="setfile"/>
            </if>
            <if test="result != null">
                and <include refid="setresult"/>
            </if>
            <if test="status != null">
                and <include refid="setstatus"/>
            </if>
        </where>
    </select>

    <select id="getList" parameterType="hashMap" resultType="complaint">
        select
        <include refid="allcolumns" />
        from
        <include refid="table"/>
        <where>
            <if test="fromuseruid != null">
                and <include refid="setfromuseruid"/>
            </if>
            <if test="fromuid != null">
                and <include refid="setfromuid"/>
            </if>
            <if test="refurl != null">
                and <include refid="setrefurl"/>
            </if>
            <if test="kind != null">
                and <include refid="setkind"/>
            </if>
            <if test="refname != null">
                and <include refid="setrefname"/>
            </if>
            <if test="type != null">
                and <include refid="settype"/>
            </if>
            <if test="desc != null">
                and <include refid="setdesc"/>
            </if>
            <if test="file != null">
                and <include refid="setfile"/>
            </if>
            <if test="result != null">
                and <include refid="setresult"/>
            </if>
            <if test="status != null">
                and <include refid="setstatus"/>
            </if>
        </where>
        <!-- 处理orderby语句 -->
        <choose>
            <when test="orderBy != null">
                ${orderBy}
            </when>
            <otherwise>
                <include refid="orderby"/>
                <if test="ascDesc != null">
                    ${ascDesc}
                </if>
            </otherwise>
        </choose>
        <!-- 处理limit语句 -->
        <trim prefix="LIMIT" prefixOverrides=",">
            <if test="startIndex != null and pageSize != null">
                ${startIndex}
            </if>
            <if test="pageSize != null">
                , ${pageSize}
            </if>
        </trim>
    </select>

    <insert id="insert" parameterType="complaint">
        insert into
        <include refid="table"/>
        (<include refid="allfields"/>)
        values (
        <include refid="allparams"/>
        )
    </insert>

    <update id="update" parameterType="complaint">
        update
        <include refid="table"/>
        <include refid="allsets"/>
        <include refid="whereid"/>
    </update>

    <update id="updateFields" parameterType="hashMap">
        update
        <include refid="table"/>
        <set>
            <if test="fromuseruid != null">
                <include refid="setfromuseruid"/>,
            </if>
            <if test="fromuid != null">
                and <include refid="setfromuid"/>
            </if>
            <if test="refurl != null">
                <include refid="setrefurl"/>,
            </if>
            <if test="kind != null">
                and <include refid="setkind"/>
            </if>
            <if test="refname != null">
                <include refid="setrefname"/>,
            </if>
            <if test="type != null">
                <include refid="settype"/>,
            </if>
            <if test="desc != null">
                <include refid="setdesc"/>,
            </if>
            <if test="file != null">
                <include refid="setfile"/>,
            </if>
            <if test="result != null">
                <include refid="setresult"/>,
            </if>
            <if test="status != null">
                <include refid="setstatus"/>
            </if>
        </set>
        <include refid="whereid"/>
    </update>

    <delete id="delete" parameterType="string">
        delete from
        <include refid="table"/>
        <include refid="whereid"/>
    </delete>

</mapper>

