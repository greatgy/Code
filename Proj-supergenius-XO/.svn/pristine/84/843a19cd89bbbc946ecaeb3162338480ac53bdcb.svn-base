<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.supergenius.xo.manager.dao.PkDateDao">

    <sql id="basecolumns">`uid`, `createtime` </sql>
    <sql id="othercolumns"><include refid="basecolumns"/>, `year`, `toyear`, `month`, `tomonth`, `day`, `today`, `fromhour`, `fromminute`, `tohour`, `tominute`, `datetime`, `count`, `leftcount`, `type`, `status` </sql>
    <sql id="allcolumns"><include refid="basecolumns"/>, <include refid="othercolumns"/>, `updatetime`</sql>
    <sql id="table">pkdate</sql>
    <sql id="count">count(*)</sql>
    <sql id="whereid">where uid = #{uid}</sql>
    <sql id="allfields"><include refid="othercolumns"/> </sql>
    <sql id="allparams">#{uid},#{createtime},#{year},#{toyear},#{month},#{tomonth},#{day},#{today},#{fromhour},#{fromminute},#{tohour},#{tominute},#{datetime},#{count},#{leftcount},#{type},#{status}</sql>
    <sql id="setyear">`year` = #{year}</sql>
    <sql id="setuid">uid = #{uid}</sql>
    <sql id="settoyear">`toyear` = #{toyear}</sql>
    <sql id="setmonth">`month` = #{month}</sql>
    <sql id="settomonth">`tomonth` = #{tomonth}</sql>
    <sql id="setday">`day` = #{day}</sql>
    <sql id="settoday">`today` = #{today}</sql>
    <sql id="setfromhour">`fromhour` = #{fromhour}</sql>
    <sql id="setfromminute">`fromminute` = #{fromminute}</sql>
    <sql id="settohour">`tohour` = #{tohour}</sql>
    <sql id="settominute">`tominute` = #{tominute}</sql>
    <sql id="setdatetime">`datetime` = #{datetime}</sql>
    <sql id="setcount">`count` = #{count}</sql>
    <sql id="setleftcount">`leftcount` = #{leftcount}</sql>
    <sql id="settype">`type` = #{type}</sql>
    <sql id="setstatus">`status` = #{status}</sql>
    <sql id="allsets">set
            <include refid="setyear"/>,
            <include refid="settoyear"/>,
            <include refid="setmonth"/>,
            <include refid="settomonth"/>,
            <include refid="setday"/>,
            <include refid="settoday"/>,
            <include refid="setfromhour"/>,
            <include refid="setfromminute"/>,
            <include refid="settohour"/>,
            <include refid="settominute"/>,
            <include refid="setdatetime"/>,
            <include refid="setcount"/>,
            <include refid="setleftcount"/>,
            <include refid="settype"/>,
            <include refid="setstatus"/>
    </sql>
    <sql id="orderby">order by createtime</sql>

    <select id="get" parameterType="string" resultType="pkdate">
        select
        <include refid="allcolumns" />
        from
        <include refid="table"/>
        <include refid="whereid"/>
    </select>

    <select id="getOne" parameterType="hashMap" resultType="pkdate">
        select
        <include refid="allcolumns" />
        from
        <include refid="table"/>
        <where>
            <if test="uid != null">
                and <include refid="setuid"/>
            </if>         
            <if test="year != null">
                and <include refid="setyear"/>
            </if>
            <if test="toyear != null">
                and <include refid="settoyear"/>
            </if>
            <if test="month != null">
                and <include refid="setmonth"/>
            </if>
            <if test="tomonth != null">
                and <include refid="settomonth"/>
            </if>
            <if test="day != null">
                and <include refid="setday"/>
            </if>
            <if test="today != null">
                and <include refid="settoday"/>
            </if>
            <if test="fromhour != null">
                and <include refid="setfromhour"/>
            </if>
            <if test="fromminute != null">
                and <include refid="setfromminute"/>
            </if>
            <if test="tohour != null">
                and <include refid="settohour"/>
            </if>
            <if test="tominute != null">
                and <include refid="settominute"/>
            </if>
            <if test="datetime != null">
                and <include refid="setdatetime"/>
            </if>
            <if test="count != null">
                and <include refid="setcount"/>
            </if>
            <if test="leftcount != null">
                and <include refid="setleftcount"/>
            </if>
            <if test="type != null">
                and <include refid="settype"/>
            </if>
            <if test="status != null">
                and <include refid="setstatus"/>
            </if>
        </where>
        LIMIT 0,1
    </select>

    <select id="getCount" parameterType="hashMap" resultType="int">
        select
        <include refid="count" />
        from
        <include refid="table"/>
        <where>
            <if test="year != null">
                and <include refid="setyear"/>
            </if>
            <if test="toyear != null">
                and <include refid="settoyear"/>
            </if>
            <if test="month != null">
                and <include refid="setmonth"/>
            </if>
            <if test="tomonth != null">
                and <include refid="settomonth"/>
            </if>
            <if test="day != null">
                and <include refid="setday"/>
            </if>
            <if test="today != null">
                and <include refid="settoday"/>
            </if>
            <if test="fromhour != null">
                and <include refid="setfromhour"/>
            </if>
            <if test="fromminute != null">
                and <include refid="setfromminute"/>
            </if>
            <if test="tohour != null">
                and <include refid="settohour"/>
            </if>
            <if test="tominute != null">
                and <include refid="settominute"/>
            </if>
            <if test="datetime != null">
                and <include refid="setdatetime"/>
            </if>
            <if test="count != null">
                and <include refid="setcount"/>
            </if>
            <if test="leftcount != null">
                and <include refid="setleftcount"/>
            </if>
            <if test="type != null">
                and <include refid="settype"/>
            </if>
            <if test="status != null">
                and <include refid="setstatus"/>
            </if>
        </where>
    </select>

    <select id="getList" parameterType="hashMap" resultType="pkdate">
        select
        <include refid="allcolumns" />
        from
        <include refid="table"/>
        <where>
            <if test="year != null">
                and <include refid="setyear"/>
            </if>
            <if test="toyear != null">
                and <include refid="settoyear"/>
            </if>
            <if test="month != null">
                and <include refid="setmonth"/>
            </if>
            <if test="tomonth != null">
                and <include refid="settomonth"/>
            </if>
            <if test="day != null">
                and <include refid="setday"/>
            </if>
            <if test="today != null">
                and <include refid="settoday"/>
            </if>
            <if test="fromhour != null">
                and <include refid="setfromhour"/>
            </if>
            <if test="fromminute != null">
                and <include refid="setfromminute"/>
            </if>
            <if test="tohour != null">
                and <include refid="settohour"/>
            </if>
            <if test="tominute != null">
                and <include refid="settominute"/>
            </if>
            <if test="datetime != null">
                and <include refid="setdatetime"/>
            </if>
            <if test="count != null">
                and <include refid="setcount"/>
            </if>
            <if test="leftcount != null">
                and <include refid="setleftcount"/>
            </if>
            <if test="type != null">
                and <include refid="settype"/>
            </if>
            <if test="status != null">
                and <include refid="setstatus"/>
            </if>
        </where>
        <!-- 处理orderby语句 -->
        <choose>
            <when test="orderBy != null">
                ${orderBy}
            </when>
            <otherwise>
                <include refid="orderby"/>
                <if test="ascDesc != null">
                    ${ascDesc}
                </if>
            </otherwise>
        </choose>
        <!-- 处理limit语句 -->
        <trim prefix="LIMIT" prefixOverrides=",">
            <if test="startIndex != null and pageSize != null">
                ${startIndex}
            </if>
            <if test="pageSize != null">
                , ${pageSize}
            </if>
        </trim>
    </select>

    <insert id="insert" parameterType="pkdate">
        insert into
        <include refid="table"/>
        (<include refid="allfields"/>)
        values (
        <include refid="allparams"/>
        )
    </insert>

    <update id="update" parameterType="pkdate">
        update
        <include refid="table"/>
        <include refid="allsets"/>
        <include refid="whereid"/>
    </update>

    <update id="updateFields" parameterType="hashMap">
        update
        <include refid="table"/>
        <set>
            <if test="year != null">
                <include refid="setyear"/>,
            </if>
            <if test="toyear != null">
                <include refid="settoyear"/>,
            </if>
            <if test="month != null">
                <include refid="setmonth"/>,
            </if>
            <if test="tomonth != null">
                <include refid="settomonth"/>,
            </if>
            <if test="day != null">
                <include refid="setday"/>,
            </if>
            <if test="today != null">
                <include refid="settoday"/>,
            </if>
            <if test="fromhour != null">
                <include refid="setfromhour"/>,
            </if>
            <if test="fromminute != null">
                <include refid="setfromminute"/>,
            </if>
            <if test="tohour != null">
                <include refid="settohour"/>,
            </if>
            <if test="tominute != null">
                <include refid="settominute"/>,
            </if>
            <if test="datetime != null">
                <include refid="setdatetime"/>,
            </if>
            <if test="count != null">
                <include refid="setcount"/>,
            </if>
            <if test="leftcount != null">
                <include refid="setleftcount"/>,
            </if>
            <if test="type != null">
                <include refid="settype"/>,
            </if>
            <if test="status != null">
                <include refid="setstatus"/>
            </if>
        </set>
        <include refid="whereid"/>
    </update>

    <delete id="delete" parameterType="string">
        delete from
        <include refid="table"/>
        <include refid="whereid"/>
    </delete>

</mapper>

