INSERT INTO `content` (`uid`, `name`, `type`, `data`, `summary`, `content`, `status`, `createtime`, `updatetime`) VALUES 
  ('uid00000000000000000000000000000','职业经理人培训细则','1','{\"uid\":\"myuid00000\"}','1','<!DOCTYPE html>\r\n<html>\r\n<head>\r\n	<meta charset=\"UTF-8\">\r\n	<title>职业经理人培训--学员培训细则</title>\r\n	<link rel=\"stylesheet\" href=\"${basecss}/css/default/training.css\" />\r\n	<style type=\"text/css\">\r\n		.wraper {\r\n			background: url(${baseimg}/imgs/default/detailbg.png) no-repeat left top; \r\n		    background-size: 100% 100%;\r\n		    height: 510px;\r\n		}\r\n			</style>\r\n</head>\r\n<body>\r\n	\r\n		<div class=\"progressBox\">\r\n			<h1 class=\"topTitle\">学员培训细则</h1>\r\n			<div class=\"trainSteps\" id=\"trains\">\r\n				<div class=\"stepitem\">\r\n					<a class=\"bigcircle\" href=\"javascript:;\">\r\n						<span class=\"smallcircle circleimg01\"></span>\r\n					</a>\r\n					<a href=\"javascript:;\" class=\"stepname\">学习建议</a>\r\n				</div>\r\n				<span class=\"line\"></span>\r\n				<div class=\"stepitem\">\r\n					<a class=\"bigcircle\" href=\"javascript:;\">\r\n						<span class=\"smallcircle circleimg02\"></span>\r\n					</a>\r\n					<a href=\"javascript:;\" class=\"stepname\">培训流程</a>\r\n				</div>\r\n				<span class=\"line\"></span>\r\n				<div class=\"stepitem\">\r\n					<a class=\"bigcircle\" href=\"javascript:;\">\r\n						<span class=\"smallcircle circleimg03\"></span>\r\n					</a>\r\n					<a href=\"javascript:;\" class=\"stepname\">晋级细则</a>\r\n				</div>\r\n				<span class=\"line\"></span>\r\n				<div class=\"stepitem\">\r\n					<a class=\"bigcircle\" href=\"javascript:;\">\r\n						<span class=\"smallcircle circleimg04\"></span>\r\n					</a>\r\n					<a href=\"javascript:;\" class=\"stepname\">毕业研究课题答辩细则</a>\r\n				</div>\r\n				<span class=\"line\"></span>\r\n				<div class=\"stepitem\">\r\n					<a class=\"bigcircle\" href=\"javascript:;\">\r\n						<span class=\"smallcircle circleimg05\"></span>\r\n					</a>\r\n					<a href=\"javascript:;\" class=\"stepname\">资费政策</a>\r\n				</div>\r\n				<span class=\"line\"></span>\r\n				<div class=\"stepitem\">\r\n					<a class=\"bigcircle\" href=\"javascript:;\">\r\n						<span class=\"smallcircle circleimg06\"></span>\r\n					</a>\r\n					<a href=\"javascript:;\" class=\"stepname\">专家管理</a>\r\n				</div>\r\n				<span class=\"line\"></span>\r\n				<div class=\"stepitem\">\r\n					<a class=\"bigcircle\" href=\"javascript:;\">\r\n						<span class=\"smallcircle circleimg07\"></span>\r\n					</a>\r\n					<a href=\"javascript:;\" class=\"stepname\">裁判管理</a>\r\n				</div>\r\n			</div>\r\n		</div>\r\n		<div class=\"container\" id=\"container\">\r\n			<div class=\"main\" id=\"main\">\r\n				<!-- 学习建议部分 -->\r\n				<div class=\"maincontent\" id=\"maincontent1\">\r\n 					<h2 class=\"titleblock\">\r\n 						<span class=\"bigcircle\">\r\n							<div class=\"smallcircle circleimg01\"></div>\r\n						</span>\r\n 						<!-- <img src=\"${baseimg}/imgs/default/adviced.png\" class=\"tipimg\" /> -->\r\n 						学习建议\r\n 					</h2>\r\n 					<div class=\"detailContent\">\r\n 						<ul class=\"detailist\" style=\"font-size: 18px;line-height: 2em;color: #4a5670;\">\r\n 							<li style=\"list-style: decimal inside;\">社会人士缴纳会费即成为本网站会员，会员有权自由浏览本网站的公开文字资料和视频，有权申请调阅职业经理人培训过程中的版权视频。</li>\r\n 							<li style=\"list-style: decimal inside;\">会员向本网站递交职业经理人培训学院学员（以下简称学员）申请，本网站将对申请者审核批准，统一分期、分班。会员在申请学员过程中需要确定培训专业。</li>\r\n 							<li style=\"list-style: decimal inside;\">职业经理人培训分六个培训专业，分别是：市场营销管理、整合传播管理、战略企划管理、人力资源管理、生产和物流管理以及财务管理，学员可根据自身情况自行选择培训专业。</li>\r\n 							<li style=\"list-style: decimal inside;\">会员申请成为学员后，为保证顺利参加培训，建议至少向账户中充值人民币1000元。</li>\r\n 							<li style=\"list-style: decimal inside;\">学员可进入超天才职业经理人培训学院，开始参加培训。</li>\r\n 							<li style=\"list-style: decimal inside;\">学员可以通过观看视频进行自学，如观看挑战培训视频了解挑战规则，熟悉挑战培训模式及其相关内容。</li>\r\n 							<li style=\"list-style: decimal inside;\">在自学过程中，如遇到问题可以上报到培训管理中心，中心将统一安排专家进行解答，学员可以以视频会议形式参与专家讲解培训，也可以点播专家讲解培训视频。</li>\r\n 							<li style=\"list-style: decimal inside;\">学员可通过浏览研究课题材料、开题视频和答辩视频自学毕业研究课题答辩的相关内容。</li>\r\n 							<li style=\"list-style: decimal inside;\">学员对以上内容的自学无时间限制。</li>\r\n 							<li style=\"list-style: decimal inside;\">学员经过自学，不断提升能力，当确认已做好充分准备后可以发出挑战申请。</li>\r\n 							<li style=\"list-style: decimal inside;\">学员可以通过观看视频，了解其他学员实力，慎重选择挑战对象。</li>\r\n 							<li style=\"list-style: decimal inside;\">学员逐级挑战晋升达到总监级别，有资格申请RMBA毕业研究课题开题。</li>\r\n 							<li style=\"list-style: decimal inside;\">学员除通过挑战晋级，也可向天财评论投稿公开发表文章，持文章申请评审获得积分，进而逐级晋级到总监级别。</li>\r\n 							<li style=\"list-style: decimal inside;\">学员参阅学习往届学员毕业课题相关报告及视频，在此基础上完成毕业研究课题。</li>\r\n 							<li style=\"list-style: decimal inside;\">总监级别学员毕业研究课题研究报告通过审核，答辩合格后，可获得超天才职业经理人培训学院颁发的RMBA学位证书和徽章（以下简称学位）。</li>\r\n 							<li style=\"list-style: decimal inside;\">学员获得RMBA学位后，有权申请裁判资格和专家资格。</li>\r\n 							<li style=\"list-style: decimal inside;\">学员获得RMBA学位后，可以选择继续接受培训争取SMBA学位和TMBA学位，也可以选择暂停培训（其他学员无法向其发起挑战），日后仍可无条件申请恢复培训。</li>\r\n 							<li style=\"list-style: decimal inside;\">学员取得RMBA学位后，有资格申请SMBA学位毕业研究课题开题，毕业研究课题研究报告通过审核，答辩合格，可获得SMBA学位，并晋级到副总裁级别。其中财务管理专业学员需具备注册会计师证方可获得SMBA学位并晋级到副总裁级别。学员拥有两个及以上不同培训方向SMBA学位方有资格申请TMBA学位毕业研究课题开题，毕业研究课题研究报告经审核通过，答辩合格，可获得TMBA学位，并晋级到总裁级别。</li>\r\n 						</ul>\r\n 					</div>\r\n				</div>\r\n				<!-- 培训流程部分 -->\r\n				<div class=\"maincontent\" id=\"maincontent2\">\r\n 					<h2 class=\"titleblock\">\r\n 						<span class=\"bigcircle\">\r\n							<div class=\"smallcircle circleimg02\"></div>\r\n						</span>\r\n 						<!-- <img src=\"${baseimg}/imgs/default/adviced.png\" class=\"tipimg\" /> -->\r\n 						培训流程\r\n 					</h2>\r\n 					<div class=\"detailContent\">\r\n 						<ul class=\"detailist\" style=\"font-size: 18px;line-height: 2em;color: #4a5670;\">\r\n 							<li style=\"list-style: decimal inside;\">社会人士缴纳会费可成为超天才网会员，会员可以浏览本网站中各类文字和视频，也可以免费申请成为职业经理人培训学院学员（以下简称学员）。会员向本网站递交学员申请，本网站将对申请者审核批准，统一分期、分班。会员成为学员后可进入超天才职业经理人培训学院，开始参加培训。</li>\r\n 							<li style=\"list-style: decimal inside;\">职业经理人培训分六个培训专业，分别是：市场营销管理、整合传播管理、战略企划管理、人力资源管理、生产和物流管理以及财务管理，学员可根据自身情况自行选择培训专业。</li>\r\n 							<li style=\"list-style: decimal inside;\">职业经理人培训形式均为不脱产性质，主要利用晚上、周末、节假日等业余时间实现和完成。</li>\r\n 							<li style=\"list-style: decimal inside;\">由会员申请成为学员后，便开始参加职业经理人培训，请学员自行合理安排时间。每位学员应向本网站提供其自行选择的禁止联系的时间表，以便客服与学员取得联系，例如：某学员自行填写了下列4个禁止客服与之联系的时间段00:00-07:00，09:00-12:00，13:00-17:00，21:00-24:00。</li>\r\n 							<li style=\"list-style: decimal inside;\">职业经理人培训的每个培训专业有五个级别，由低至高依次是：基础学员、经理级学员、总监级学员、副总裁级学员、总裁级学员。</li>\r\n 							<li style=\"list-style: decimal inside;\">职业经理人培训采用线上和线下相结合的培训方式。线上培训包括挑战培训、专家讲解培训及点播培训等。线下培训包括挑战培训、研究课题的开题、中间指导、预答辩、答辩以及一些内容的专题讲座等。培训费用详见《资费政策》。</li>\r\n 							<li style=\"list-style: decimal inside;\">专家讲解培训主要解答学员提出的问题，所有学员均可参加，采取现场授课形式。学员可向培训管理中心提出培训要求，本网站根据学员反馈的问题统一挑选重点需求安排专家解答，学员可免费参加，培训时间为两小时。每次培训提前10天公布解答的问题和时间以便学员加入培训。该视频将被上传至本网站，所有学员均可点播观看。每次专家讲解培训解决过往学员提出的十个热点问题，参与培训的学员如仍有疑问可于培训过后提出，并将问题上报培训管理中心，以便专家进一步解答。</li>\r\n 							<li style=\"list-style: decimal inside;\">点播培训是指学员可以免费或付费点播职业经理人培训学院已有的视频，通常包括挑战培训视频、专家讲解培训视频、研究课题的开题和答辩视频等，免费内容和收费标准详见《资费政策》。</li>\r\n 							<li style=\"list-style: decimal inside;\">挑战培训主要利用网络视频会议系统建立会场，视级别或情况不同，也可有现场挑战。每场挑战的会场观众数设定不超过二十人。学员可通过购买门票进入会场旁听，成为观众，按规定参与会场互动。收费及奖励标准详见《资费政策》。</li>\r\n 							<li style=\"list-style: decimal inside;\">学员参与挑战训练，只能向本培训专业高一级别学员发起挑战，不可越级挑战，不可跨专业挑战。挑战成功或平手即可晋级，学员须逐级晋升。每场挑战结论由该场挑战的裁判委员会做出。结论分为挑战成功、挑战失败和平手，参赛双方的最终得分相差在10%以内，判定为平手，挑战者晋级，被挑战者不失败。挑战规则详情及资费情况请见《晋级细则》与《资费政策》。</li>\r\n 							<li style=\"list-style: decimal inside;\">除挑战模式外，学员可通过向天财评论投稿的方式获得晋级。学员向培训管理中心申请晋级并缴纳晋级费用，申请通过后提交在天财评论上公开发表的文章，申请文章积分评审。每级晋级所需积分不同，满足积分需求后可获得晋级。详情请见《晋级细则》与《资费政策》。</li>\r\n 							<li style=\"list-style: decimal inside;\">职业经理人培训学院将为完成培训、通过毕业研究课题研究报告审核、通过答辩并确认合格的学员颁发相应级别的超天才职业经理人培训学院学位证书和徽章（以下简称学位）。</li>\r\n 							<li style=\"list-style: decimal inside;\"> 职业经理人培训所发学位有三个级别：RMBA、SMBA、TMBA。</li>\r\n 							<li style=\"list-style: decimal inside;\">新学员经过职业经理人培训提供的与实战高度结合的逐级挑战训练或向天财评论投稿发表文章获得一定积分，晋级达到总监级别，可申请毕业研究课题开题，毕业研究课题研究报告通过审核，答辩合格，可获得RMBA学位。</li>\r\n 							<li style=\"list-style: decimal inside;\">毕业研究课题答辩流程为开题→预答辩→答辩，具体流程及规则请见《毕业研究课题答辩细则》。</li>\r\n 							<li style=\"list-style: decimal inside;\">学员在取得RMBA学位后有资格申请进入职业经理人高级培训学院，申请毕业研究课题开题，毕业研究课题研究报告经审核通过，答辩合格，可获得SMBA学位，并晋级到副总裁级别。其中财务管理专业学员需具备注册会计师证方可获得SMBA学位。</li>\r\n 							<li style=\"list-style: decimal inside;\">学员拥有两个及以上不同培训方向SMBA学位方有资格申请进入职业经理人顶级培训学院，申请毕业研究课题开题，毕业研究课题研究报告经审核通过，答辩合格，可获得TMBA学位，并晋级到总裁级别。</li>\r\n 							<li style=\"list-style: decimal inside;\">RMBA、SMBA与TMBA学位证书与徽章由职业经理人培训学院、职业经理人高级培训学院与职业经理人顶级培训学院同时颁发，不单独颁发。</li>\r\n 							<li style=\"list-style: decimal inside;\">挑战、答辩失败的学员不允许上诉，但是可以向本网站举报裁判或专家的不公正或不当行为。裁判在任意连续10场内被查实举报3次将被取消裁判资格。专家被查实举报1次将被降级，若最低级别专家被查实举报则取消专家资格。裁判或专家被举报而被查实的次数、被裁判管理委员会或专家管理委员会判罚情况、查实举报的举报人、挑战场次等信息在本网站实时公布。</li>\r\n 						</ul>\r\n 					</div>\r\n				</div>\r\n				<!-- 挑战细则部分 -->\r\n				<div class=\"maincontent\" id=\"maincontent3\">\r\n 					<h2 class=\"titleblock\">\r\n 						<span class=\"bigcircle\">\r\n							<div class=\"smallcircle circleimg03\"></div>\r\n						</span>\r\n 						<!-- <img src=\"${baseimg}/imgs/default/adviced.png\" class=\"tipimg\" /> -->\r\n 						晋级细则\r\n 					</h2>\r\n 					<div class=\"detailContent\">\r\n 						<div class=\"detailist\" style=\"font-size: 18px;line-height: 2em;color: #4a5670;\">\r\n 							<h4>1）晋级流程</h4>\r\n 							<ul class=\"progressul\">\r\n	 							<li style=\"list-style: decimal inside;\">职业经理人培训的每个培训专业有五个级别，依次是：基础学员、经理级学员、总监级学员、副总裁级学员、总裁级学员。</li>\r\n	 							<li style=\"list-style: decimal inside;\">学员需由最低级基础学员逐级晋级，总监级别以下学员的晋级方式包括挑战和在天财评论投稿两种方式。总监级别以上学员的晋级方式：总监级别学员取得RMBA学位后，申请SMBA学位毕业研究课题开题，毕业研究课题研究报告通过审核，答辩合格，可获得SMBA学位，并晋级到副总裁级别。其中财务管理专业学员只有具备注册会计师证才可获得SMBA学位并晋级到副总裁级别。副总裁级别学员拥有两个及以上不同培训方向SMBA学位方有资格申请TMBA毕业研究课题开题，毕业研究课题研究报告经审核通过，答辩合格，可获得TMBA学位，并晋级到总裁级别。</li>\r\n	 							<li style=\"list-style: decimal inside;\">挑战模式中，学员只能向本培训专业高一级别学员发起挑战，不可越级挑战，不可跨专业挑战。挑战成功或平手即可晋级，学员须逐级晋升。每场挑战结论由该场挑战的裁判委员会做出，结论分为挑战成功、挑战失败和平手。参赛双方的最终得分相差在10%以上，挑战获胜者可获得人民币1000元奖励。参赛双方的最终得分相差在10%以内，判定为平手，挑战者晋级，被挑战者不失败，双方均无奖励。</li>\r\n	 							<li style=\"list-style: decimal inside;\">已经发布的挑战计划不可变更。挑战者或被挑战者在挑战开始后十分钟仍未入场视为缺席，若一方缺席，则视为对方获胜；若双方均缺席则视双方均失败。</li>\r\n	 							<li style=\"list-style: decimal inside;\">挑战者根据网站提示提交挑战申请，选择会员清单中同一培训专业高一级别学员为被挑战者，同时向被挑战者提供未来第八周到十一周内的十个可供选择的挑战时间，并在裁判库中挑选五名以内备选裁判。</li>\r\n	 							<li style=\"list-style: decimal inside;\">被挑战者在收到挑战申请的十日内回复并确定是否接受挑战，十日内未回复视为拒绝挑战，超过十日无法回复。</li>\r\n	 							<li style=\"list-style: decimal inside;\">如果被挑战者确定接受挑战，需在挑战者提供的时间中选择一个并确定为挑战时间，且在裁判库中挑选五名以内备选裁判，则挑战时间正式确定。</li>\r\n	 							<li style=\"list-style: decimal inside;\">如果被挑战者对十个时间均不满意，可以重新选择十个时间回复挑战者，并在裁判库中挑选五名以内备选裁判。挑战者需在收到新时间的五日内回复，在十个时间内选择一个确定为挑战时间。如挑战者不接受新时间，则视为挑战失败，或五日内未回复视为挑战失败，超过五日无法回复。</li>\r\n	 							<li style=\"list-style: decimal inside;\">允许被挑战者拒绝接受挑战，但是每个月必须接受一次挑战（在有学员向其发起挑战的前提下）。在有学员向其发起挑战的前提下，一个自然月内未接受任何挑战按失败一次处理。由学员晋级后的第一个完整自然月开始计算，每个自然月从1号算起。例如，学员在3月15日晋升为经理级别，由4月开始算起。同一级别上作为被挑战者累计五次失败将会被降级，总监级别以上学员不会被降级。总监级别学员在申请毕业研究课题开题之前作为被挑战者累计五次失败将会被降级，申请毕业研究课题开题后将不会被降级。申请毕业研究课题开题的同时需缴纳费用，详情请见《资费政策》。</li>\r\n	 							<li style=\"list-style: decimal inside;\">挑战时间正式确定后，培训管理中心将立即对双方选定的裁判全部发出邀请。裁判在收到邀请后，在五日内回复是否接受邀请，超过五日无法回复。分别从每位学员选择的所有备选裁判中确定一名，选定率先确认出席参加的裁判为本场挑战的裁判。如裁判确实有不可抗拒的原因而无法准时出席，培训管理中心将与学员协商指派裁判。如五日内未有任何裁判确认出席，由培训管理中心向裁判管理委员会申请指派一名裁判。每场挑战有三位裁判，除以上已经确定的两名，第三位由裁判管理委员会指定并出任主裁判。裁判为有偿参与，出席每场挑战均领取报酬。但是如果确认出席的裁判没有在72小时前请假且最终没能参加，则扣一次裁判费，即裁判在下次挑战中不领取报酬。如已确定的裁判缺席，培训管理中心从应急裁判库中指派一名裁判为接替裁判。</li>\r\n	 							<li style=\"list-style: decimal inside;\">挑战分为现场挑战和在线挑战。在线挑战采用视频会议系统进行，学员需要在挑战开始前十分钟内入场，如参赛学员在挑战开始十分钟后仍未入场视为缺席。若一方缺席，则视为对方获胜；若双方均缺席则视双方均失败。现场挑战由培训管理中心与学员线下沟通，确认挑战地点。现场挑战学员与裁判均不可无故缺席，如有特殊情况请提前72小时告知培训管理中心。</li>\r\n	 							<li style=\"list-style: decimal inside;\">参加在线挑战的学员在挑战前应做好硬件准备工作。为保证挑战过程中语音和画面清晰流畅，请在挑战前配备以下硬件设备：配带质量良好的头戴式耳麦；配备LED台灯或面部前方有其它照射光源。</li>\r\n	 							<li style=\"list-style: decimal inside;\">本网站拥有所有挑战视频的知识产权，可将挑战视频上传至本网站供其他学员学习。</li>\r\n	 							<li style=\"list-style: decimal inside;\">若因视频会议系统故障、停电、网站瘫痪等不可抗力而导致挑战无法进行，培训管理中心将协调更改挑战时间。</li>\r\n	 							<li style=\"list-style: decimal inside;\">\r\n	 								除挑战模式外，学员可通过向天财评论投稿的方式获得晋级。学员向培训管理中心提交晋级请求，并交纳晋级费。审核通过后提交在天财评论上公开发表的文章，申请文章积分评审。培训管理中心对文章进行查重、排除抄袭，原创文章根据文章水平审核评分，每篇50-400分不等，非原创文章根据学员推荐文章时写的推荐语评分，每篇1-5分不等。每级晋级所需积分不同，学员为晋级而提交的积分中，其原创文章对应的积分不低于70%，例如基础学员申请晋级为经理级学员需要2000分，其中要求至少有1400分来源于原创文章。\r\n	 								<table class=\"lineimgs\">\r\n	 									<tr>\r\n	 										<td>级别</td>\r\n	 										<td>对应积分</td>\r\n	 									</tr>\r\n	 									<tr>\r\n	 										<td>基础学员→经理</td>\r\n	 										<td>2000分</td>\r\n	 									</tr>\r\n	 									<tr>\r\n	 										<td>经理→总监</td>\r\n	 										<td>3000分</td>\r\n	 									</tr>\r\n	 								</table>\r\n	 							</li>\r\n	 							<li style=\"list-style: decimal inside;\">学员申请评审后，所得积分若不满足晋级要求，其积分保留，可继续撰写文章并再次申请评审直到满足晋级要求。若积分数额超出学员当前级别晋级所需，其超出部分在学员晋级成功后保留，可用于下次晋级。每篇文章只可申请评审一次。例如：某一基础学员向培训管理中心提交晋级请求，并交纳晋级费。同时提交10篇文章申请评审，所得积分1000分，不符合晋级要求，其1000分保留，该10篇文章不可再次申请评审。该学员可再次提交文章评审而无需重复缴纳晋级费用。其第二次申请评审文章10篇，得到积分1500分，与之前积分累计相加共2500分，符合晋级要求。培训管理中心经审核，确认合格，同意晋级请求，扣除2000积分。学员晋级，其剩余500积分保留，可用于下次晋级。晋级费用详情请见《资费政策》。</li>\r\n	 							<li style=\"list-style: decimal inside;\">总监级别以下的学员可自行选择适合的方式晋级，可单一使用挑战或者投稿的方式，也可混合两种方式，但是通过投稿晋级的学员仍需要接受其他学员的挑战（在有学员向其发起挑战的前提下）。关于被挑战者接受或拒绝挑战部分详见第9条。</li>\r\n	 						</ul>\r\n 							<h4>2）挑战题目设置</h4>\r\n	 						<ul class=\"progressul\">\r\n	 							<li style=\"list-style: decimal inside;\">挑战题目共12道，其中4道为裁判委员会提出，挑战者和被挑战者需共同回答，挑战者与被挑战者轮替作为第一回答人，即第1题、第3题为挑战者先回答，第2题、第4题为被挑战者先回答。挑战者和被挑战者为对方各出3道题，共6道。第1题、第3题、第5题为挑战者提问，第2题、第4题、第6题为被挑战者提问。最后2道题目为本场观众提出，分别向挑战者和被挑战者各提问1道题。必须双方学员均回答问题，该环节评分才有效。例如，如果有观众向挑战者提问，而没有观众向被挑战者提问，则挑战者该环节的成绩不计分。</li>\r\n	 							<li style=\"list-style: decimal inside;\">每场挑战裁判委员会所出的题目以真实案例为主，每场挑战3个案例。题目在挑战举行前至少三十天向挑战双方公布。</li>\r\n	 							<li style=\"list-style: decimal inside;\">挑战双方互为对方所出题目应围绕前述题目所涉及企业以及本专业进行，本场挑战中观众所出2道题目，也必须在前述题目涉及企业以及本专业范围内。双方互为对方所出题目和观众所出题目，不可提前透露。为避免所选题目被裁判拒绝，或两位学员出题有重复，请学员准备足够数量的备选题目。若出题学员所出问题连续被裁判否定，而导致没有适合的问题提问，裁判有权指定问题，出题者该题目不得分。</li>\r\n	 						</ul>\r\n 							<h4>3）挑战评分规则</h4>\r\n	 						<ul class=\"progressul\">\r\n								<li style=\"list-style: decimal inside;\">每场挑战有三位裁判，包括一位主裁判，主裁判对整场挑战进行调控。所有题目的评分按一定规则加权加总得出，规则见下文。</li>\r\n	 							<li style=\"list-style: decimal inside;\">学员总分为所回答题目所有分数按照规则累计相加，得分高者获胜。但是如果参赛双方的最终得分相差在10%以内，判定为平手，挑战者晋级，被挑战者不失败，双方均无奖励。</li>\r\n	 							<li style=\"list-style: decimal inside;\">为维护秩序与公平公正，避免裁判与参赛者间出现内幕交易，在公布分数时各裁判要相应公布评判理由，每道题的评语不超过140字。</li>\r\n	 							<li style=\"list-style: decimal inside;\">裁判可对学员的回答进行追问，但是针对一名学员每个问题的回答最多只可追问一次。</li>\r\n	 							<li style=\"list-style: decimal inside;\">挑战者和被挑战者共同回答裁判题的每一道题，裁判根据挑战者和被挑战者的回答比较评分，每位裁判就同一道题给两位学员的评分相加为100分，主裁判权重为1.5，其他裁判为1.0。例如，一位裁判对裁判题第一题的打分标准：挑战者的分数加上被挑战者的分数等于100分。</li>\r\n	 							<li style=\"list-style: decimal inside;\">对手题和观众提问的评判均按百分制打分，主裁判权重为1.5，其他裁判为1.0。</li>\r\n	 							<li style=\"list-style: decimal inside;\">挑战双方互为对方所出题目，其出题质量纳入裁判评分范围。出题者权重为0.4，答题者权重为0.6。\r\n	 								<div class=\"titleContent\">\r\n	 									<h6>环节1：裁判出题</h6>\r\n	 									<table class=\"lineimgs\">\r\n	 										<tr>\r\n	 											<td>裁判出题环节</td>\r\n	 											<td>挑战者</td>\r\n	 											<td>被挑战者</td>\r\n	 										</tr>\r\n	 										<tr>\r\n	 											<td>裁判A</td>\r\n	 											<td>评分x1.0</td>\r\n	 											<td>评分x1.0</td>\r\n	 										</tr>\r\n	 										<tr>\r\n	 											<td>裁判B</td>\r\n	 											<td>评分x1.0</td>\r\n	 											<td>评分x1.0</td>\r\n	 										</tr>\r\n	 										<tr>\r\n	 											<td>主裁判</td>\r\n	 											<td>评分×1.5</td>\r\n	 											<td>评分×1.5</td>\r\n	 										</tr>\r\n	 										<tr>\r\n	 											<td>合计</td>\r\n	 											<td>挑战者该环节总分</td>\r\n	 											<td>被挑战者该环节总分</td>\r\n	 										</tr>\r\n	 									</table>\r\n	 								</div>\r\n	 								<div class=\"titleContent\">\r\n	 									<h6>环节2：挑战者向被挑战者出题</h6>\r\n	 									<table class=\"lineimgs\">\r\n	 										<tr>\r\n	 											<td>裁判出题环节</td>\r\n	 											<td>挑战者</td>\r\n	 											<td>被挑战者</td>\r\n	 										</tr>\r\n	 										<tr>\r\n	 											<td>裁判A</td>\r\n	 											<td>评分x1.0x0.4</td>\r\n	 											<td>评分x1.0x0.6</td>\r\n	 										</tr>\r\n	 										<tr>\r\n	 											<td>裁判B</td>\r\n	 											<td>评分×1.0×0.4</td>\r\n	 											<td>评分×1.0×0.6</td>\r\n	 										</tr>\r\n	 										<tr>\r\n	 											<td>主裁判</td>\r\n	 											<td>评分×1.5×0.4</td>\r\n	 											<td>评分×1.5×0.6</td>\r\n	 										</tr>\r\n	 										<tr>\r\n	 											<td>合计</td>\r\n	 											<td>挑战者该环节总分</td>\r\n	 											<td>被挑战者该环节总分</td>\r\n	 										</tr>\r\n	 									</table>\r\n	 								</div>\r\n	 								<div class=\"titleContent\">\r\n	 									<h6>环节3：被挑战者向挑战者出题</h6>\r\n	 									<table class=\"lineimgs\">\r\n	 										<tr>\r\n	 											<td>裁判出题环节</td>\r\n	 											<td>挑战者</td>\r\n	 											<td>被挑战者</td>\r\n	 										</tr>\r\n	 										<tr>\r\n	 											<td>裁判A</td>\r\n	 											<td>评分×1.0×0.6</td>\r\n	 											<td>评分×1.0×0.4</td>\r\n	 										</tr>\r\n	 										<tr>\r\n	 											<td>裁判B</td>\r\n	 											<td>评分×1.0×0.6</td>\r\n	 											<td>评分×1.0×0.4</td>\r\n	 										</tr>\r\n	 										<tr>\r\n	 											<td>主裁判</td>\r\n	 											<td>评分×1.5×0.6</td>\r\n	 											<td>评分×1.5×0.4</td>\r\n	 										</tr>\r\n	 										<tr>\r\n	 											<td>合计</td>\r\n	 											<td>挑战者该环节总分</td>\r\n	 											<td>被挑战者该环节总分</td>\r\n	 										</tr>\r\n	 									</table>\r\n	 								</div>\r\n	 								<div class=\"titleContent\">\r\n	 									<h6>环节4：观众出题</h6>\r\n	 									<table class=\"lineimgs\">\r\n	 										<tr>\r\n	 											<td>裁判出题环节</td>\r\n	 											<td>挑战者</td>\r\n	 											<td>被挑战者</td>\r\n	 										</tr>\r\n	 										<tr>\r\n	 											<td>裁判A</td>\r\n	 											<td>评分x1.0x0.2</td>\r\n	 											<td>评分x1.0x0.2</td>\r\n	 										</tr>\r\n	 										<tr>\r\n	 											<td>裁判B</td>\r\n	 											<td>评分×1.0×0.2</td>\r\n	 											<td>评分×1.0×0.2</td>\r\n	 										</tr>\r\n	 										<tr>\r\n	 											<td>主裁判</td>\r\n	 											<td>评分×1.5×0.2</td>\r\n	 											<td>评分×1.5×0.2</td>\r\n	 										</tr>\r\n	 										<tr>\r\n	 											<td>合计</td>\r\n	 											<td>挑战者该环节总分</td>\r\n	 											<td>被挑战者该环节总分</td>\r\n	 										</tr>\r\n	 									</table>\r\n	 								</div>\r\n	 								将3个环节挑战者和被挑战者的所有分数分别相加得到其总分，得分高者获胜。但是如果参赛双方的最终得分相差在10%以内，判定为平手，挑战者晋级，被挑战者不失败，双方均无奖励。\r\n	 							</li>\r\n	 						</ul>\r\n	 					</div>\r\n 					</div>\r\n				</div>\r\n				<!-- 毕业研究课题 答辩细则部分 -->\r\n				<div class=\"maincontent\" id=\"maincontent4\">\r\n 					<h2 class=\"titleblock\">\r\n 						<span class=\"bigcircle\">\r\n							<div class=\"smallcircle circleimg04\"></div>\r\n						</span>\r\n 						<!-- <img src=\"${baseimg}/imgs/default/adviced.png\" class=\"tipimg\" /> -->\r\n 						毕业研究课题答辩细则\r\n 					</h2>\r\n 					<div class=\"detailContent\">\r\n 						<ul class=\"detailist\" style=\"font-size: 18px;line-height: 2em;color: #4a5670;\">\r\n 							<li style=\"list-style: decimal inside;\">职业经理人培训所发学位证书和徽章（以下简称学位）有三个级别：RMBA、SMBA、TMBA。其中，RMBA学位为最低级别，TMBA学位为最高级别。</li>\r\n 							<li style=\"list-style: decimal inside;\">毕业研究课题答辩流程为开题→预答辩→答辩。</li>\r\n 							<li style=\"list-style: decimal inside;\">学员若想获得RMBA证书需先晋级到总监级别，再完成毕业研究课题答辩流程。首先申请开题，可自选一家公司作为研究对象，须根据公司经营状况提出10个以上与本培训专业相关的主问题。这些问题必须能够反映公司在该领域（学员培训专业）的全部管理问题。学员将以上主问题整理成正式材料，向培训管理中心申请召开开题论证会，并提供十个可供选择的开题论证会时间。培训管理中心根据学员提供材料确定是否批准其进行开题，如材料尚不合格，管理中心将告知学员其不足之处以备学员补充，如材料合格，将批准学员开题并确定开题论证会时间。培训管理中心负责安排开题委员会，该委员会由3名专家组成。</li>\r\n 							<li style=\"list-style: decimal inside;\">开题论证会上，专家从学员提交的主问题中确认有价值的题目为预答辩的题目，如有必要也可提出新的问题，共计10题以上。开题论证会原则上为现场，如学员无法到场，可以远程视频会议开题。</li>\r\n 							<li style=\"list-style: decimal inside;\">开题论证会结束后，学员需对会上总结的主问题做解答，作答要求具有独创性，体现研究问题的能力。学员另须由每道题提出3个以上子问题，子问题在预答辩之前无需作答，其目的在于表明学员对该问题理解的深度和发现问题的能力。学员将作答内容和子问题整理为正式报告提交到培训管理中心，由培训管理中心安排预答辩委员会，预答辩委员会由3位专家组成。</li>\r\n 							<li style=\"list-style: decimal inside;\">预答辩不安排会议，只需学员上交相关材料即可。预答辩委员会专家审核主问题的答案与子问题，确认作答是否恰当，肯定有价值的子问题，剔除不理想的，追加更深刻的，并提出反馈意见报告。</li>\r\n 							<li style=\"list-style: decimal inside;\">预答辩后，学员对专家确认的子问题作答，并针对每个子问题提出3个以上深化或相关延伸的问题，无需作答，仅用以表明其研究能力和洞察能力。学员经过进一步修改将问题与解答整理为毕业课题研究报告，提交到培训管理中心并提供十个可供选择的答辩时间。培训管理中心将确定是否批准该学员进行答辩，如材料尚不合格，管理中心将告知学员其不足之处以备学员补充，如材料合格，将批准学员答辩。培训管理中心负责安排答辩委员会并确定答辩时间，答辩委员会由3名专家组成。</li>\r\n 							<li style=\"list-style: decimal inside;\">\r\n 								答辩中，答辩委员会专家针对研究报告对学员提出问题，学员需现场回答，专家根据研究报告、学员回答的完整性以及对问题的理解力和洞察力评定通过或不通过答辩，简单多数专家通过记为通过答辩。答辩会须现场进行。\r\n 							</li>\r\n 							<li style=\"list-style: decimal inside;\">答辩通过后，通过答辩的学员根据专家的建议修改研究报告并上交到培训管理中心，研究报告撰写要求详见《毕业研究课题研究报告要求》。未通过的学员需要二次答辩，并征收二次答辩费用。答辩费用及二次答辩费用详见《资费政策》。</li>\r\n 							<li style=\"list-style: decimal inside;\">学员在取得RMBA学位后有资格申请SMBA毕业研究课题开题，其毕业研究课题为开题委员会指定一家大型企业为研究对象，其余与获取RMBA学位答辩流程一致。其中财务管理专业学员需具备注册会计师证方可获得SMBA学位。</li>\r\n 							<li style=\"list-style: decimal inside;\">学员拥有两个及以上不同培训方向SMBA学位方有资格申请TMBA毕业研究课题开题，其毕业研究课题为开题委员会指定一家大型企业为研究对象，需要对该企业进行完整全面的分析，不仅仅是针对某一个专业，而是根据公司经营状况提出10个以上主问题。这些问题必须能够反映公司的全部管理问题，其余与获取RMBA学位答辩流程一致。</li>\r\n 							<li style=\"list-style: decimal inside;\">允许申请RMBA学位的学员在答辩会之前更换题目。在开题论证会之前可免费更换题目，开题论证会结束后、预答辩开始前更换题目需补交答辩费用的20%，即人民币1000元。预答辩后、答辩会前更换题目需补交答辩费用的50%，即人民币2500元。申请SMBA、TMBA学位的学员不可变更题目。</li>\r\n 						</ul>\r\n 					</div>\r\n				</div>\r\n				<!-- 资费政策部分 -->\r\n				<div class=\"maincontent\" id=\"maincontent5\">\r\n 					<h2 class=\"titleblock\">\r\n 						<span class=\"bigcircle\">\r\n							<div class=\"smallcircle circleimg05\"></div>\r\n						</span>\r\n 						<!-- <img src=\"${baseimg}/imgs/default/adviced.png\" class=\"tipimg\" /> -->\r\n 						资费政策\r\n 					</h2>\r\n 					<div class=\"detailContent\">\r\n 						<div class=\"detailist\" style=\"font-size: 18px;line-height: 2em;color: #4a5670;\">\r\n 							<p>资费政策中出现的货币单位均为人民币。</p>\r\n 							<h4>一、会员费</h4>\r\n 							<p>社会人士缴纳会员费后，成为超天才网会员。费用标准为人民币10000元/人。</p>\r\n 							<h4>二、点播视频费用</h4>\r\n 							<p>会员可以免费或付费点播职业经理人培训相关视频，通常包括挑战培训视频、专家讲解培训视频、毕业研究课题的开题视频和答辩视频等。收费视频点播一般收取10-50元不等。学员购买视频后，可播放30日，30日后播放须再次购买。/人。</p>\r\n 							<h4>三、晋级费用</h4>\r\n 							<table class=\"lineimgs\">\r\n								<tr>\r\n									<td>晋升级别</td>\r\n									<td>晋级费用（元/次）</td>\r\n								</tr>\r\n								<tr>\r\n									<td>总监</td>\r\n									<td>8000</td>\r\n								</tr>\r\n								<tr>\r\n									<td>经理</td>\r\n									<td>5000</td>\r\n								</tr>\r\n							</table>\r\n 							<h4>四、毕业研究课题答辩费用</h4>\r\n 							<p>毕业研究课题答辩费用包括开题、预答辩和答辩等，如一次答辩失败，可在补充研究之后申请二次答辩。如学员参加挑战和答辩均一次成功，则取得RMBA学位所需总费用为63000元，取得SMBA学位所需总费用为163000元，取得TMBA学位所需总费用为526000元。</p>\r\n 							<table class=\"lineimgs\">\r\n 								<tr>\r\n 									<td>申请学位</td>\r\n 									<td>答辩费用（元/次）</td>\r\n 									<td>二次答辩费用（元/次）</td>\r\n 								</tr>\r\n 								<tr>\r\n 									<td>TMBA</td>\r\n 									<td>200000</td>\r\n 									<td>100000</td>\r\n 								</tr>\r\n 								<tr>\r\n 									<td>SMBA</td>\r\n 									<td>100000</td>\r\n 									<td>50000</td>\r\n 								</tr>\r\n 								<tr>\r\n 									<td>RMBA</td>\r\n 									<td>50000</td>\r\n 									<td>25000</td>\r\n 								</tr>\r\n 							</table>\r\n 							<h4>五、挑战门票</h4>\r\n 							<p>学员可购买挑战门票，作为观众，以视频会议形式观看挑战，每场挑战门票费用10-100元。若因参加挑战的学员缺席或视频会议系统故障、停电、网站瘫痪等不可抗力而导致挑战无法进行，门票费用全额退还。</p>\r\n 							<h4>六、挑战奖励</h4>\r\n 							<p>在任何级别的挑战中，获胜的一方将得到1000元奖励，本网站不代为办理纳税事宜。若挑战判定为平手，双方均无奖励。</p>\r\n 						</div>\r\n 					</div>\r\n				</div>\r\n				<!-- 专家管理部分 -->\r\n				<div class=\"maincontent\" id=\"maincontent6\">\r\n 					<h2 class=\"titleblock\">\r\n 						<span class=\"bigcircle\">\r\n							<div class=\"smallcircle circleimg06\"></div>\r\n						</span>\r\n 						专家管理\r\n 					</h2>\r\n 					<div class=\"detailContent\">\r\n 						<ul class=\"detailist\" id=\"datailist\" style=\"font-size: 18px;line-height: 2em;color: #4a5670;\">\r\n 							<li style=\"list-style: decimal inside;\">专家资格分为三级，分别是专家、高级专家、特级专家，其中最低一级为专家，可以担任RMBA学位申请的委员会专家，担任SMBA学位申请的委员会专家的最低要求为高级专家，担任TMBA学位申请的委员会专家的最低要求为特级专家。各专业的各级专家可由本网站特别聘任、聘请，也可由会员和学员中产生。</li>\r\n 							<li style=\"list-style: decimal inside;\">裁判分为六个专业，分别是：市场营销管理、整合传播管理、战略企划管理、人力资源管理、生产和物流管理以及财务管理。每位专家只能评审本专业的毕业研究课题答辩。</li>\r\n 							<li style=\"list-style: decimal inside;\">专家资格由本网站专家管理委员会经考核后给予确认并正式授予专家资格证书。以本网站正式发布、确认或授予的凭据为准。</li>\r\n 							<li style=\"list-style: decimal inside;\">申请专家资格者，须填写专家申请表，表中必须填写10个以上可供参考的空闲时间段，考核合格后才能被正式授予专家资格。考核分两部分，一是提交一份关于某个公司的完整研究报告，研究报告的要求与申请RMBA学位所提交的研究报告的要求一致。二是参加本网站组织的面试考核，本网站在申请人填写的10个以上可供参考的空闲时间段中确定一个为现场面试时间。</li>\r\n 							<li style=\"list-style: decimal inside;\">获得RMBA学位的学员申请并通过考核可获得本专业的专家资格，获得SMBA学位的学员申请并通过考核可获得本专业的高级专家资格，具有TMBA学位的学员申请并通过考核可以成为本专业特级专家。</li>\r\n 							<li style=\"list-style: decimal inside;\">申请高级专家资格者，提交的研究报告的要求与申请SMBA学位所提交的研究报告的要求一致，其他流程同申请专家资格。申请特级专家资格者，提交的研究报告的要求与申请TMBA学位所提交的研究报告的要求一致，其他流程同申请专家资格。</li>\r\n 							<li style=\"list-style: decimal inside;\">本网站从外部直接特聘的专家、高级专家和特级专家，受聘者仅限于上市公司或万人以上的大型企业的高管或具备一定社会影响力的知名专家、学者、企业家等社会人士。</li>\r\n 							<li style=\"list-style: decimal inside;\">学员成为专家后，参与评判的答辩数量超过50场，可申请成为本网站雇佣制的专职专家</li>\r\n 							<li style=\"list-style: decimal inside;\">专家如有退出意向，可向培训管理中心申请退出专家队伍，日后仍可无条件申请恢复身份。</li>\r\n 							<li style=\"list-style: decimal inside;\">若专家不能做到勤勉尽责，或者专家管理委员会认为其无法胜任专家工作，本网站有权解聘专家。</li>\r\n 							<li style=\"list-style: decimal inside;\">各场次各级别答辩的专家委员会由3人组成，本网站指定专家委员会主席。</li>\r\n 							<li style=\"list-style: decimal inside;\">如专家被本网站指定出任某学员毕业研究课题答辩，各级专家不得无故缺席已经确认并列入既定计划的开题、预答辩、答辩或二次答辩，如有紧急情况确实无法履行专家职责，应提前120小时电话告知专家管理委员会并请假，以便专家管理委员会安排接替人员。此情况专家无法取得报酬，也无须接受处罚。若专家在开题、预答辩、答辩或二次答辩开始前120小时内请假或临时缺席则需承担相应处罚，处罚内容是必须无偿出任下次毕业研究课题答辩。如已确定的专家无法出席，本网站将尽量从应急专家库中指定一名同级别专家为接替专家。</li>\r\n 							<li style=\"list-style: decimal inside;\">学员可以向培训管理中心举报专家的不公正或不当行为。本网站严谨认真对待举报，只有证据充分、确凿才会判罚专家。专家被查实举报1次将被降级，若最低级专家被查实举报则取消专家资格。专家被查实举报次数、被专家管理委员会判罚次数、查实举报人以及答辩场次等信息在本网站实时公布。</li>\r\n 							<li style=\"list-style: decimal inside;\">各级专家应遵守本网站的各项规章和流程，服从本网站的安排和调度，并有责任对本网站的各项工作与行为提出善意或建设性的意见或建议，对各种违规或舞弊行为进行拒绝、抵制或举报。</li>\r\n 							<li style=\"list-style: decimal inside;\">本网站对各级专家实施动态管理，及时检查、分析专家队伍的现状，及时更新专家人选信息库，以保证有足够的专家人才提供良好的服务。</li>\r\n 							<li style=\"list-style: decimal inside;\">专家只有出席了答辩会才能领取报酬，例如：某位专家出席了开题论证会，但是答辩会请假未出席，则不能领取报酬，但是从应急专家库中指定的接替专家因为出席了答辩会可以领取报酬。</li>\r\n 							<li style=\"list-style: decimal inside;\">专家报酬：\r\n 								<table class=\"lineimgs\">\r\n 									<tr>\r\n 										<td>申请学位</td>\r\n 										<td>专家起始等级</td>\r\n 										<td>专家报酬（元/场）</td>\r\n 										<td>二次答辩专家报酬（元/场）</td>\r\n 									</tr>\r\n 									<tr>\r\n 										<td>TMBA</td>\r\n 										<td>特级专家</td>\r\n 										<td>15000</td>\r\n 										<td>7500</td>\r\n 									</tr>\r\n 									<tr>\r\n 										<td>SMBA</td>\r\n 										<td>高级专家</td>\r\n 										<td>10000</td>\r\n 										<td>5000</td>\r\n 									</tr>\r\n 									<tr>\r\n 										<td>RMBA</td>\r\n 										<td>专家</td>\r\n 										<td>5000</td>\r\n 										<td>2500</td>\r\n 									</tr>\r\n 								</table>\r\n 								其中，专家所得需要照章纳税（劳务收入所得税），本网站不代为办理。因现场开题或现场答辩产生的路费（火车票、经济舱机票）本网站可报销，并且提供住宿。\r\n 							</li>\r\n 						</ul>\r\n 					</div>\r\n				</div>\r\n				<!-- 裁判管理部分 -->\r\n				<div class=\"maincontent\" id=\"maincontent7\">\r\n 					<h2 class=\"titleblock\">\r\n 						<span class=\"bigcircle\">\r\n							<div class=\"smallcircle circleimg07\"></div>\r\n						</span>\r\n 						裁判管理\r\n 					</h2>\r\n 					<div class=\"detailContent\">\r\n 						<ul class=\"detailist\" id=\"datailist\" style=\"font-size: 18px;line-height: 2em;color: #4a5670;\">\r\n 							<li style=\"list-style: decimal inside;\">各培训专业裁决挑战的裁判中，部分由本网站特别聘任、聘请，其余可从已获得RMBA、SMBA或TMBA学位证书和徽章（以下简称学位）的学员中产生。</li>\r\n 							<li style=\"list-style: decimal inside;\">裁判资格由本网站裁判管理委员会经考核后给予确认并正式授予裁判资格证书。以本网站正式发布、确认或授予的凭据为准。</li>\r\n 							<li style=\"list-style: decimal inside;\">裁判分为六个专业，分别是：市场营销管理、整合传播管理、战略企划管理、人力资源管理、生产和物流管理以及财务管理。每位裁判只能评判本专业的挑战。</li>\r\n 							<li style=\"list-style: decimal inside;\">学员获得RMBA、SMBA或TMBA学位后，有资格申请成为该专业的裁判，须填写裁判申请表，表中必须填写10个以上可供参考的空闲时间段，考核合格后才能被正式授予裁判资格。考核分两部分，一是为本网站题库累计贡献50道挑战题目。题目数量以被采用（或被纳入有效题库）为准。二是参加本网站组织的面试考核，本网站在学员填写的10个以上可供参考的空闲时间段中确定一个为面试时间，面试可采用现场或视频会议形式。</li>\r\n 							<li style=\"list-style: decimal inside;\">本网站从外部直接特聘的裁判，受聘者仅限于上市公司或万人以上的大型企业的高管或具备一定社会影响力的知名专家、学者、企业家等社会人士。</li>\r\n 							<li style=\"list-style: decimal inside;\">各场次各级别挑战的裁判委员会均由三人组成，其中挑战双方各自从裁判人选库中选任一位，本网站指定主裁判。主裁判由公司内部的符合资格的正式员工担任或是公司特聘裁判担任。</li>\r\n 							<li style=\"list-style: decimal inside;\">学员成为裁判后，参与评判的挑战数量超过100场，有资格申请成为本网站雇佣制的专职裁判。</li>\r\n 							<li style=\"list-style: decimal inside;\">裁判如有退出意向，可向培训管理中心申请退出裁判队伍，日后仍可无条件申请恢复身份。</li>\r\n 							<li style=\"list-style: decimal inside;\">如裁判被学员选任或被本网站指定出任某场挑战，且该项挑战已经确认并列入既定计划，各级裁判不得无故缺席，如有紧急情况确实无法履行裁判职责，应提前72小时向本网站请假，以便安排接替人员。此情况裁判无法获得报酬，无需承担处罚。挑战开始前72小时内请假或挑战开始10分钟后仍未出现按缺席处理，缺席者需承担相应处罚，处罚内容是必须无偿担任下次裁判。如已确定的裁判缺席，本网站将从应急裁判库中指定一名为接替裁判，接替裁判可获得相应报酬。</li>\r\n 							<li style=\"list-style: decimal inside;\">若裁判不能做到勤勉尽责，或者裁判管理委员会认为其无法胜任裁判工作，本网站有权解聘裁判。</li>\r\n 							<li style=\"list-style: decimal inside;\">挑战前，培训管理中心在挑战举行前至少30天向裁判公布挑战题目，裁判应仔细审阅并研究题目。挑战过程中，每个题目每位裁判最多提问一个问题，但是允许裁判多次点评。主裁判负责监督其他裁判适当提问、控制挑战进程、挑战结束后关闭视频会议系统等。挑战结束后，裁判应对学员的作答给出评语和评分，每道题的评语不超过140字，评分规则详见《晋级细则》。</li>\r\n 							<li style=\"list-style: decimal inside;\">学员可以向培训管理中心举报裁判的不公正或不当行为。本网站严谨认真对待举报，只有证据充分、确凿才会判罚裁判。裁判在任意连续10场内被查实举报3次，将被取消裁判资格。裁判被查实举报次数、被裁判管理委员会判罚情况、查实举报人以及挑战场次等信息在本网站实时公布。</li>\r\n 							<li style=\"list-style: decimal inside;\">各级裁判应遵守本网站的各项规章和流程，服从本网站的安排和调度，并有责任对本网站的各项工作与行为提出善意或建设性的意见或建议，对各种违规或舞弊行为进行拒绝、抵制或举报。</li>\r\n 							<li style=\"list-style: decimal inside;\">本网站对各级裁判实施动态管理，及时检查、分析裁判队伍的现状，及时更新裁判人选信息库，以保证有足够的裁判人才为挑战平台提供良好的服务。</li>\r\n 							<li style=\"list-style: decimal inside;\">	裁判报酬：\r\n 							 	<table class=\"lineimgs\">\r\n 							 		<tr>\r\n 							 			<td>挑战级别</td>\r\n 							 			<td>裁判报酬（元/场）</td>\r\n 							 		</tr>\r\n 							 		<tr>\r\n 							 			<td>总监</td>\r\n 							 			<td>800</td>\r\n 							 		</tr>\r\n 							 		<tr>\r\n 							 			<td>经理</td>\r\n 							 			<td>500</td>\r\n 							 		</tr>\r\n 							 	</table>\r\n 							 	其中，裁判所得需要照章纳税（劳务收入所得税），本网站不代为办理。若因参加挑战的学员缺席而导致挑战无法进行，裁判报酬照常发放。\r\n 							</li>\r\n 						</ul>\r\n 					</div>\r\n				</div>\r\n			</div>\r\n			<!-- 侧边栏导航部分 -->\r\n			<ul class=\"sidebar\" id=\"sidebar\">\r\n				<li class=\"stepitem\">\r\n					<a class=\"bigcircle\" href=\"javascript:;\">\r\n						<div class=\"smallcircle circleimg01\"></div>\r\n					</a>\r\n				</li>\r\n				<li class=\"stepitem\">\r\n					<a class=\"bigcircle\" href=\"javascript:;\">\r\n						<div class=\"smallcircle circleimg02\"></div>\r\n					</a>\r\n				</li>\r\n				<li class=\"stepitem\">\r\n					<a class=\"bigcircle\" href=\"javascript:;\">\r\n						<div class=\"smallcircle circleimg03\"></div>\r\n					</a>\r\n				</li>\r\n				<li class=\"stepitem\">\r\n					<a class=\"bigcircle\" href=\"javascript:;\">\r\n						<div class=\"smallcircle circleimg04\"></div>\r\n					</a>\r\n				</li>\r\n				<li class=\"stepitem\">\r\n					<a class=\"bigcircle\" href=\"javascript:;\">\r\n						<div class=\"smallcircle circleimg05\"></div>\r\n					</a>\r\n				</li>\r\n				<li class=\"stepitem\">\r\n					<a class=\"bigcircle\" href=\"javascript:;\">\r\n						<div class=\"smallcircle circleimg06\"></div>\r\n					</a>\r\n				</li>\r\n				<li class=\"stepitem\">\r\n					<a class=\"bigcircle\" href=\"javascript:;\">\r\n						<div class=\"smallcircle circleimg07\"></div>\r\n					</a>\r\n				</li>\r\n			</ul>\r\n		</div>\r\n		<!--#include file=\"/include/footer.html\" -->\r\n		<script type=\"text/javascript\" src=\"${basejs}/js/pages/pageSwitch.js\"></script> \r\n	\r\n	<script type=\"text/javascript\">\r\n		$(function() {\r\n			$(\"#sidebar li\").eq(${name}).trigger(\"click\");\r\n			$(\"#trains div\").eq(${name}).trigger(\"click\");\r\n		})\r\n	</script>\r\n	<script type=\"text/javascript\">\r\n		var reg=location.search.match(/ts=([^&]*)/),\r\n		ts=reg&&reg[1]||\'scrollX\',\r\n		a=new pageSwitch(\'main\',{\r\n			duration:1000,\r\n			start:0,\r\n			direction:1,\r\n			loop:false,\r\n			ease:/flip(?!Paper)/.test(ts)?\'bounce\':\'ease\',\r\n			transition:ts,\r\n			mouse:true,\r\n			mousewheel:true,\r\n			arrowkey:true\r\n		});\r\n		var navs=$(\'#sidebar li\');\r\n		var trains = $(\'#trains div\');\r\n		a.on(\'before\',function(m,n){\r\n			navs[m].className=\'\';\r\n			trains[m].className=\'stepitem\';\r\n			navs[n].className=\'curStepitem\';\r\n			trains[n].className=\'stepitem curStepitem\';\r\n		});\r\n\r\n		if(/^(?:scroll3d|flip)/.test(ts)){\r\n			document.getElementById(\'main\').className=\'visible\';\r\n		}\r\n		\r\n		i=0;\r\n		for(;i<=navs.length;i++){\r\n			!function(i){\r\n				navs.eq(i).click(function(){\r\n	 				a.slide(i);\r\n				});\r\n				trains.eq(i).click(function(){\r\n					a.slide(i);\r\n				});\r\n			}(i);\r\n		}\r\n	</script>\r\n</body>\r\n</html>','1','2016-07-17 18:38:00','2016-12-12 16:16:44'),
  ('uid00000000000000000000000000001','职业经理人首页','0',NULL,NULL,'<!DOCTYPE html>\r\n<html>\r\n<head>\r\n	<meta charset=\"UTF-8\"/>\r\n	<title>职业经理人培训</title>\r\n	<link rel=\"stylesheet\" href=\"${basecss}/css/default/index.css\" />\r\n</head>\r\n<body>\r\n	<div class=\"container\">\r\n		<div class=\"videocontain\">\r\n			<div class=\"video\">\r\n				<embed pluginspage=\"http://www.macromedia.com/go/getflashplayer\" type=\"application/x-shockwave-flash\" src=\"http://p.you.video.sina.com.cn/swf/bokePlayer20150319_V4_1_43_12.swf\" width=\"700\" height=\"460\" id=\"myMovie\" allownetworking=\"all\" allowscriptaccess=\"always\" wmode=\"transparent\" allowfullscreen=\"true\" quality=\"high\" bgcolor=\"#000000\" flashvars=\"pid=1&amp;tid=334&amp;autoLoad=0&amp;as=0&amp;lightBtn=0&amp;popBtn=0&amp;wideBtn=0&amp;tj=0&amp;head=0&amp;pic=&amp;continuePlayer=0&amp;actlogActive=0&amp;realfull=1&amp;oldApi=0&amp;moz=1&amp;playerWidth=970&amp;playerHeight=520&amp;autoPlay=1&amp;tjAD=0&amp;logo=0&amp;share=0&amp;vblog=2&amp;container=swfDiv&amp;HTML5Player_autoChangeBGColor=false&amp;vid=134902230&amp;url=http://p.you.video.sina.com.cn/swf/bokePlayer20150319_V4_1_43_12.swf\" />\r\n				<a class=\"close_video\">关闭视频&nbsp;&gt;&nbsp;&gt;</a>\r\n			</div>\r\n		</div>\r\n		<div class=\"mainbox\">\r\n			<!-- 关于职业经理人培训简介部分 -->\r\n			<div class=\"introduce\">\r\n				<h1>关于职业经理人培训</h1>\r\n				<p>顾雏军先生管理公司是与众不同的，这不仅是他管理公司的理念与众不同，而且他的具体实践也与众不同。顾先生认为中国企业之所以不能屹立于世界之林，是因为没有一支纯洁的、忠诚的、全身心系于公司的职业经理人队伍。顾先生也是这样实践的，他在他曾经拥有的五家上市公司中都是最关注于职业经理人队伍的建设。在广东科龙电器股份有限公司，他亲任校长严格培训了870名有MBA学位的现代化的管理人才，即使在他遭遇厄运的那个悲惨年代，这批人都仍然在追求着他的信仰。</p>\r\n				<p>我们现在组建超天才职业经理人培训学院就是秉承他的理念，用2年时间，打造一支万人最纯洁的职业经理人队伍，以争强于世界。</p>\r\n				<p>我们定义的最纯洁的职业经理人是:</p>\r\n				<ul class=\"introducebox\">\r\n					<li>\r\n						<div class=\"numindex\">01</div>\r\n						<p>最纯洁的职业经理人绝不会利用职业生涯中所习得的技能和知识创业或参与行业竞争</p>\r\n					</li>\r\n					<li>\r\n						<div class=\"numindex\">02</div>\r\n						<p>最纯洁的职业经理人总是仅凭个人的能力和对企业的忠诚获得晋升并赢得人生事业的辉煌</p>\r\n					</li>\r\n					<li>\r\n						<div class=\"numindex\">03</div>\r\n						<p>最纯洁的职业经理人只接受职业契约的薪金和奖励，绝不谋求任何黑色或灰色的个人收入</p>\r\n					</li>\r\n				</ul>\r\n				<p>30年的市场化之路，中国职场风起云涌且尽显疲态，有识之士一直在探索中国职业经理人的强盛之路。我们坚信一定不止万人赞同我们的理念和信仰，我们希冀与志同道合者一起争胜于全球商界！</p>\r\n				<p>我们诚邀有此志者参加我们的培训，但是非诚勿扰！</p>\r\n				<div class=\"schools\">\r\n					<span>超天才职业经理人培训学院</span>\r\n					<span>超天才职业经理人高级培训学院</span>\r\n					<span>超天才职业经理人顶级培训学院</span>\r\n				</div>\r\n			</div>\r\n			<!-- 学员培训细则部分 -->\r\n			<div class=\"training\">\r\n				<h1>学员培训细则</h1>\r\n				<div class=\"trainlist\">\r\n					<a href=\"${base}/index/relus/0\" class=\"detailitem\">\r\n						<div class=\"itemcontain\">\r\n							<div class=\"trainingimg train01\"></div>\r\n						</div>\r\n						<span>学习建议</span>\r\n					</a>\r\n					<a href=\"${base}/index/relus/1\" class=\"detailitem\">\r\n						<div class=\"itemcontain\">\r\n							<div class=\"trainingimg train02\"></div>\r\n						</div>\r\n						<span>培训流程</span>\r\n					</a>\r\n					<a href=\"${base}/index/relus/2\" class=\"detailitem\">\r\n						<div class=\"itemcontain\">\r\n							<div class=\"trainingimg train03\"></div>\r\n						</div>\r\n						<span>晋级细则</span>\r\n					</a>\r\n					<a href=\"${base}/index/relus/3\" class=\"detailitem\">\r\n						<div class=\"itemcontain\">\r\n							<div class=\"trainingimg train04\"></div>\r\n						</div>\r\n						<span>毕业研究课题答辩细则</span>\r\n					</a>\r\n					<a href=\"${base}/index/relus/4\" class=\"detailitem\">\r\n						<div class=\"itemcontain\">\r\n							<div class=\"trainingimg train05\"></div>\r\n						</div>\r\n						<span>资费政策</span>\r\n					</a>\r\n					<a href=\"${base}/index/relus/5\" class=\"detailitem\">\r\n						<div class=\"itemcontain\">\r\n							<div class=\"trainingimg train06\"></div>\r\n						</div>\r\n						<span>专家管理规定</span>\r\n					</a>\r\n					<a href=\"${base}/index/relus/6\" class=\"detailitem\">\r\n						<div class=\"itemcontain\">\r\n							<div class=\"trainingimg train07\"></div>\r\n						</div>\r\n						<span