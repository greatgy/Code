<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.supergenius.xo.manager.dao.AppLeaveDao">

    <sql id="basecolumns">`uid`, `createtime` </sql>
    <sql id="othercolumns"><include refid="basecolumns"/>, `useruid`, `usertype`, `starttime`, `endtime`, `desc`, `result`, `status` </sql>
    <sql id="allcolumns"><include refid="basecolumns"/>, <include refid="othercolumns"/>, `updatetime`</sql>
    <sql id="table">appleave</sql>
    <sql id="count">count(*)</sql>
    <sql id="whereid">where uid = #{uid}</sql>
    <sql id="allfields"><include refid="othercolumns"/> </sql>
    <sql id="allparams">#{uid},#{createtime},#{useruid},#{usertype},#{starttime},#{endtime},#{desc},#{result},#{status}</sql>
    <sql id="setuseruid">`useruid` = #{useruid}</sql>
    <sql id="setusertype">`usertype` = #{usertype}</sql>
    <sql id="setuid">uid = #{uid}</sql>
    <sql id="setstarttime">`starttime` = #{starttime}</sql>
    <sql id="setendtime">`endtime` = #{endtime}</sql>
    <sql id="setdesc">`desc` = #{desc}</sql>
    <sql id="setresult">`result` = #{result}</sql>
    <sql id="setstatus">`status` = #{status}</sql>
    <sql id="allsets">set
            <include refid="setuseruid"/>,
            <include refid="setusertype"/>,
            <include refid="setstarttime"/>,
            <include refid="setendtime"/>,
            <include refid="setdesc"/>,
            <include refid="setresult"/>,
            <include refid="setstatus"/>
    </sql>
    <sql id="orderby">order by createtime</sql>

    <select id="get" parameterType="string" resultType="appleave">
        select
        <include refid="allcolumns" />
        from
        <include refid="table"/>
        <include refid="whereid"/>
    </select>

    <select id="getOne" parameterType="hashMap" resultType="appleave">
        select
        <include refid="allcolumns" />
        from
        <include refid="table"/>
        <where>
            <if test="uid != null">
                and <include refid="setuid"/>
            </if>         
            <if test="useruid != null">
                and <include refid="setuseruid"/>
            </if>
            <if test="usertype != null">
                and <include refid="setusertype"/>
            </if>
            <if test="starttime != null">
                and <include refid="setstarttime"/>
            </if>
            <if test="endtime != null">
                and <include refid="setendtime"/>
            </if>
            <if test="desc != null">
                and <include refid="setdesc"/>
            </if>
            <if test="result != null">
                and <include refid="setresult"/>
            </if>
            <if test="status != null">
                and <include refid="setstatus"/>
            </if>
        </where>
        LIMIT 0,1
    </select>

    <select id="getCount" parameterType="hashMap" resultType="int">
        select
        <include refid="count" />
        from
        <include refid="table"/>
        <where>
            <if test="useruid != null">
                and <include refid="setuseruid"/>
            </if>
            <if test="usertype != null">
                and <include refid="setusertype"/>
            </if>
            <if test="starttime != null">
                and <include refid="setstarttime"/>
            </if>
            <if test="endtime != null">
                and <include refid="setendtime"/>
            </if>
            <if test="desc != null">
                and <include refid="setdesc"/>
            </if>
            <if test="result != null">
                and <include refid="setresult"/>
            </if>
            <if test="status != null">
                and <include refid="setstatus"/>
            </if>
        </where>
    </select>

    <select id="getList" parameterType="hashMap" resultType="appleave">
        select
        <include refid="allcolumns" />
        from
        <include refid="table"/>
        <where>
            <if test="useruid != null">
                and <include refid="setuseruid"/>
            </if>
            <if test="usertype != null">
                and <include refid="setusertype"/>
            </if>
            <if test="starttime != null">
                and <include refid="setstarttime"/>
            </if>
            <if test="endtime != null">
                and <include refid="setendtime"/>
            </if>
            <if test="desc != null">
                and <include refid="setdesc"/>
            </if>
            <if test="result != null">
                and <include refid="setresult"/>
            </if>
            <if test="status != null">
                and <include refid="setstatus"/>
            </if>
        </where>
        <!-- 处理orderby语句 -->
        <choose>
            <when test="orderBy != null">
                ${orderBy}
            </when>
            <otherwise>
                <include refid="orderby"/>
                <if test="ascDesc != null">
                    ${ascDesc}
                </if>
            </otherwise>
        </choose>
        <!-- 处理limit语句 -->
        <trim prefix="LIMIT" prefixOverrides=",">
            <if test="startIndex != null and pageSize != null">
                ${startIndex}
            </if>
            <if test="pageSize != null">
                , ${pageSize}
            </if>
        </trim>
    </select>

    <insert id="insert" parameterType="appleave">
        insert into
        <include refid="table"/>
        (<include refid="allfields"/>)
        values (
        <include refid="allparams"/>
        )
    </insert>

    <update id="update" parameterType="appleave">
        update
        <include refid="table"/>
        <include refid="allsets"/>
        <include refid="whereid"/>
    </update>

    <update id="updateFields" parameterType="hashMap">
        update
        <include refid="table"/>
        <set>
            <if test="useruid != null">
                <include refid="setuseruid"/>,
            </if>
            <if test="usertype != null">
                <include refid="setusertype"/>,
            </if>
            <if test="starttime != null">
                <include refid="setstarttime"/>,
            </if>
            <if test="endtime != null">
                <include refid="setendtime"/>,
            </if>
            <if test="desc != null">
                <include refid="setdesc"/>,
            </if>
            <if test="result != null">
                <include refid="setresult"/>,
            </if>
            <if test="status != null">
                <include refid="setstatus"/>
            </if>
        </set>
        <include refid="whereid"/>
    </update>

    <delete id="delete" parameterType="string">
        delete from
        <include refid="table"/>
        <include refid="whereid"/>
    </delete>

</mapper>

