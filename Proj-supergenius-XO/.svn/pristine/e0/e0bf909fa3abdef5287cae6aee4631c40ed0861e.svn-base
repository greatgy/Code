<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.supergenius.xo.enterpriser.dao.CommentsDao">

    <sql id="basecolumns">`uid`, `createtime` </sql>
    <sql id="othercolumns">`fromuid`, `touid`, `fromuseruid`, `fromuseroid`, `fromusername`, `touseruid`, `touseroid`, `tousername`, `cataloguename`, `cid`, `content`, `channel`, `data`, `type`, `adminuid`, `status` </sql>
