package com.supergenius.xo.startup.entity;

import com.genius.model.base.entity.BaseEntity;

/**
 * 收藏实体类
 * 
 * @author 杨光
 * @date 2017年8月23日14:12:19
 */
public class Collect extends BaseEntity {

	private static final long serialVersionUID = 4966785590817770987L;
	private String useruid;// 当前用户uid
	private String refuid;// 来自哪篇文章的uid

	public String getUseruid() {
		return useruid;
	}

	public void setUseruid(String useruid) {
		this.useruid = useruid;
	}

	public String getRefuid() {
		return refuid;
	}

	public void setRefuid(String refuid) {
		this.refuid = refuid;
	}
}
