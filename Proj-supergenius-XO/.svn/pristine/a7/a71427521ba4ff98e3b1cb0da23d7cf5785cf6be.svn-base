<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.supergenius.xo.account.dao.AccountlogDao">

    <sql id="basecolumns">`uid`, `createtime` </sql>
    <sql id="othercolumns"><include refid="basecolumns"/>, `accountuid`, `accountsn`, `useruid`, `username`, `useremail`, `bank`, `accountstatefrom`, `accountstateto`, `money`, `desc`, `status` </sql>
    <sql id="allcolumns"><include refid="basecolumns"/>, <include refid="othercolumns"/></sql>
    <sql id="table">accountlog</sql>
    <sql id="count">count(*)</sql>
    <sql id="whereid">where uid = #{uid}</sql>
    <sql id="allfields"><include refid="othercolumns"/> </sql>
    <sql id="allparams">#{uid},#{createtime},#{accountuid},#{accountsn},#{useruid},#{username},#{useremail},#{bank},#{accountstatefrom},#{accountstateto},#{money},#{desc},#{status}</sql>
    <sql id="setaccountuid">accountuid = #{accountuid}</sql>
    <sql id="setaccountsn">accountsn = #{accountsn}</sql>
    <sql id="setuseruid">useruid = #{useruid}</sql>
    <sql id="setusername">username = #{username}</sql>
    <sql id="setuseremail">useremail = #{useremail}</sql>
    <sql id="setbank">bank = #{bank}</sql>
    <sql id="setaccountstatefrom">accountstatefrom = #{accountstatefrom}</sql>
    <sql id="setaccountstateto">accountstateto = #{accountstateto}</sql>
    <sql id="setmoney">money = #{money}</sql>
    <sql id="setdesc">desc = #{desc}</sql>
    <sql id="setstatus">status = #{status}</sql>
    <sql id="allsets">set
            <include refid="setaccountuid"/>,
            <include refid="setaccountsn"/>,
            <include refid="setuseruid"/>,
            <include refid="setusername"/>,
            <include refid="setuseremail"/>,
            <include refid="setbank"/>,
            <include refid="setaccountstatefrom"/>,
            <include refid="setaccountstateto"/>,
            <include refid="setmoney"/>,
            <include refid="setdesc"/>,
            <include refid="setstatus"/>
    </sql>
    <sql id="orderby">order by createtime</sql>

    <select id="get" parameterType="string" resultType="accountlog">
        select
        <include refid="allcolumns" />
        from
        <include refid="table"/>
        <include refid="whereid"/>
    </select>

    <select id="getOne" parameterType="hashMap" resultType="accountlog">
        select
        <include refid="allcolumns" />
        from
        <include refid="table"/>
        <where>
            <if test="accountuid != null">
                and <include refid="setaccountuid"/>
            </if>
            <if test="accountsn != null">
                and <include refid="setaccountsn"/>
            </if>
            <if test="useruid != null">
                and <include refid="setuseruid"/>
            </if>
            <if test="username != null">
                and <include refid="setusername"/>
            </if>
            <if test="useremail != null">
                and <include refid="setuseremail"/>
            </if>
            <if test="bank != null">
                and <include refid="setbank"/>
            </if>
            <if test="accountstatefrom != null">
                and <include refid="setaccountstatefrom"/>
            </if>
            <if test="accountstateto != null">
                and <include refid="setaccountstateto"/>
            </if>
            <if test="money != null">
                and <include refid="setmoney"/>
            </if>
            <if test="desc != null">
                and <include refid="setdesc"/>
            </if>
            <if test="status != null">
                and <include refid="setstatus"/>
            </if>
        </where>
        LIMIT 0,1
    </select>

    <select id="getCount" parameterType="hashMap" resultType="int">
        select
        <include refid="count" />
        from
        <include refid="table"/>
        <where>
            <if test="accountuid != null">
                and <include refid="setaccountuid"/>
            </if>
            <if test="accountsn != null">
                and <include refid="setaccountsn"/>
            </if>
            <if test="useruid != null">
                and <include refid="setuseruid"/>
            </if>
            <if test="username != null">
                and <include refid="setusername"/>
            </if>
            <if test="useremail != null">
                and <include refid="setuseremail"/>
            </if>
            <if test="bank != null">
                and <include refid="setbank"/>
            </if>
            <if test="accountstatefrom != null">
                and <include refid="setaccountstatefrom"/>
            </if>
            <if test="accountstateto != null">
                and <include refid="setaccountstateto"/>
            </if>
            <if test="money != null">
                and <include refid="setmoney"/>
            </if>
            <if test="desc != null">
                and <include refid="setdesc"/>
            </if>
            <if test="status != null">
                and <include refid="setstatus"/>
            </if>
        </where>
    </select>

    <select id="getList" parameterType="hashMap" resultType="accountlog">
        select
        <include refid="allcolumns" />
        from
        <include refid="table"/>
        <where>
            <if test="accountuid != null">
                and <include refid="setaccountuid"/>
            </if>
            <if test="accountsn != null">
                and <include refid="setaccountsn"/>
            </if>
            <if test="useruid != null">
                and <include refid="setuseruid"/>
            </if>
            <if test="username != null">
                and <include refid="setusername"/>
            </if>
            <if test="useremail != null">
                and <include refid="setuseremail"/>
            </if>
            <if test="bank != null">
                and <include refid="setbank"/>
            </if>
            <if test="accountstatefrom != null">
                and <include refid="setaccountstatefrom"/>
            </if>
            <if test="accountstateto != null">
                and <include refid="setaccountstateto"/>
            </if>
            <if test="money != null">
                and <include refid="setmoney"/>
            </if>
            <if test="desc != null">
                and <include refid="setdesc"/>
            </if>
            <if test="status != null">
                and <include refid="setstatus"/>
            </if>
        </where>
        <!-- 处理orderby语句 -->
        <choose>
            <when test="orderBy != null">
                ${orderBy}
            </when>
            <otherwise>
                <include refid="orderby"/>
                <if test="ascDesc != null">
                    ${ascDesc}
                </if>
            </otherwise>
        </choose>
        <!-- 处理limit语句 -->
        <trim prefix="LIMIT" prefixOverrides=",">
            <if test="startIndex != null and pageSize != null">
                ${startIndex}
            </if>
            <if test="pageSize != null">
                , ${pageSize}
            </if>
        </trim>
    </select>

    <insert id="insert" parameterType="accountlog">
        insert into
        <include refid="table"/>
        (<include refid="allfields"/>)
        values (
        <include refid="allparams"/>
        )
    </insert>

    <update id="update" parameterType="accountlog">
        update
        <include refid="table"/>
        <include refid="allsets"/>
        <include refid="whereid"/>
    </update>

    <update id="updateFields" parameterType="hashMap">
        update
        <include refid="table"/>
        <set>
            <if test="accountuid != null">
                <include refid="setaccountuid"/>,
            </if>
            <if test="accountsn != null">
                <include refid="setaccountsn"/>,
            </if>
            <if test="useruid != null">
                <include refid="setuseruid"/>,
            </if>
            <if test="username != null">
                <include refid="setusername"/>,
            </if>
            <if test="useremail != null">
                <include refid="setuseremail"/>,
            </if>
            <if test="bank != null">
                <include refid="setbank"/>,
            </if>
            <if test="accountstatefrom != null">
                <include refid="setaccountstatefrom"/>,
            </if>
            <if test="accountstateto != null">
                <include refid="setaccountstateto"/>,
            </if>
            <if test="money != null">
                <include refid="setmoney"/>,
            </if>
            <if test="desc != null">
                <include refid="setdesc"/>,
            </if>
            <if test="status != null">
                <include refid="setstatus"/>
            </if>
        </set>
        <include refid="whereid"/>
    </update>

    <delete id="delete" parameterType="string">
        delete from
        <include refid="table"/>
        <include refid="whereid"/>
    </delete>

</mapper>

