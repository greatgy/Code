package com.supergenius.core.rule;

/**
 * 数独App当前进行的游戏数规则类
 * @author ChenQi
 * @date 2017年12月14日14:15:59
 */
public class GamesIndexRlue extends GamesRule{
	protected static String p = "/index";
	
	public GamesIndexRlue() {
		this("", 0);
	}
	
	/**
	 * @param k
	 */
	public GamesIndexRlue(String k) {
		this(k, 0);
	}

	/**
	 * 
	 * @param k
	 * @param exp seconds
	 */
	public GamesIndexRlue(String k, int exp) {
		super(k, exp);
		super.path += p;
		refreshConfKey();
	}
}
