package com.supergenius.xo.finance.enums;

/**
 *	计数类别枚举类
 * @author ShangJianguo
 * @modifier architect.bian
 */
public enum ECountType {
	
	click("0"); //点击数
	
	private final String value;

	private ECountType(String v) {
		this.value = v;
	}

	public String toString() {
		return this.value;
	}

	public static ECountType get(int v) {
		String str = String.valueOf(v);
		return get(str);
	}

	public static ECountType get(String str) {
		for (ECountType e : values()) {
			if (e.toString().equals(str)) {
				return e;
			}
		}
		return null;
	}
}
