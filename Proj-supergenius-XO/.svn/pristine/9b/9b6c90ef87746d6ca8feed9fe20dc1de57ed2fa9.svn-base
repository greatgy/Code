	<sql id="allcolumns"><include refid="basecolumns"/>, <include refid="othercolumns"/>, `updatetime`</sql>
	<sql id="table">finance</sql>
	<sql id="whereid">where uid = #{uid}</sql>
	<sql id="allfields"><include refid="basecolumns"/>, <include refid="othercolumns"/></sql>
	<sql id="allparams">#{oid},#{uid},#{createtime},#{adminuid},#{type},#{title},#{shorttitle},#{grade},#{content},#{contributeuid},#{authoruid},#{author},#{imglittle},#{imgmedium},#{imgbig},#{imgoriginal},#{origin},#{originurl},#{summary},#{keywords},#{sortorder},#{istop},#{isrecommend},#{ispublic},#{config},#{data},#{publishtime},#{status}</sql>
	<sql id="count">count(*)</sql>
	<sql id="setoid">oid = #{oid}</sql>
	<sql id="setuid">uid = #{uid}</sql>
	<sql id="setadminuid">adminuid = #{adminuid}</sql>
	<sql id="settype">type = #{type}</sql>
	<sql id="wheretype">type &amp; #{type}</sql>
	<sql id="setno_type">type != #{no_type}</sql>
	<sql id="settitle">title = #{title}</sql>
	<sql id="setshorttitle">shorttitle = #{shorttitle}</sql>
	<sql id="setgrade">grade = #{grade}</sql>
	<sql id="settitle_lk">title like CONCAT('%',#{title_lk},'%')</sql>
	<sql id="setshorttitle_lk">shorttitle like CONCAT('%',#{shorttitle_lk},'%')</sql>
	<sql id="setcontent">content = #{content}</sql>
	<sql id="setcontent_lk">content like CONCAT('%',#{content_lk},'%')</sql>
	<sql id="setcontributeuid">contributeuid = #{contributeuid}</sql>
	<sql id="setauthoruid">authoruid = #{authoruid}</sql>
	<sql id="setauthor">author = #{author}</sql>
	<sql id="setauthor_lk">author like CONCAT('%',#{author_lk},'%')</sql>
	<sql id="setimglittle">imglittle = #{imglittle}</sql>
	<sql id="setimgmedium">imgmedium = #{imgmedium}</sql>
	<sql id="setimgoriginal">imgoriginal = #{imgoriginal}</sql>
	<sql id="setimgbig">imgbig = #{imgbig}</sql>
	<sql id="setorigin">origin = #{origin}</sql>
	<sql id="setorigin_lk">origin like CONCAT('%',#{origin_lk},'%')</sql>
	<sql id="setoriginurl">originurl = #{originurl}</sql>
	<sql id="setsummary">summary = #{summary}</sql>
	<sql id="setkeywords">keywords = #{keywords}</sql>
	<sql id="setkeywords_lk">keywords like CONCAT('%',#{keywords_lk},'%')</sql>
	<sql id="setsortorder">sortorder = #{sortorder}</sql>
	<sql id="setistop">istop = #{istop}</sql>
	<sql id="setisrecommend">isrecommend = #{isrecommend}</sql>
	<sql id="setispublic">ispublic = #{ispublic}</sql>
	<sql id="setconfig">config = #{config}</sql>
	<sql id="setdata">data = #{data}</sql>
	<sql id="setpublishtime">publishtime = #{publishtime}</sql>
	<sql id="setpublishtimestartne">publishtime &gt; #{publishtimestartne}</sql>
	<sql id="setpublishtimeendne">publishtime &lt; #{publishtimeendne}</sql>
	<sql id="setstatus">status = #{status}</sql>
	<sql id="setcreatetimestart">createtime &gt;= #{createtimestart}</sql>
	<sql id="setcreatetimeend">createtime &lt;= #{createtimeend}</sql>
	<sql id="setcreatetimestartne">createtime &gt; #{createtimestartne}</sql>
	<sql id="setcreatetimeendne">createtime &lt; #{createtimeendne}</sql>
	<sql id="allsets">set 
