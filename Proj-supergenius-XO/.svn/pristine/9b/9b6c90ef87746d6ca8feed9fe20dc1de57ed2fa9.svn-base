			</if>
			<if test="debate != null">
				and uid not in(
					select d.finaceuid from debatearticle d
				)
			</if>
			
			<if test="contributeuids != null">
				and contributeuid in 
				<foreach item="item" index="index" collection="contributeuids" open="(" separator="," close=")">
                 	 #{item}
            	</foreach>
			</if>
			<if test="authoruids != null">
				and authoruid in 
				<foreach item="item" index="index" collection="authoruids" open="(" separator="," close=")">
                     #{item}
                </foreach>
			</if>
		</where>
		<!-- 处理orderby语句 -->
		<choose>
			<when test="orderBy != null">
				${orderBy}
			</when>
			<otherwise>
				<include refid="orderby"/>
