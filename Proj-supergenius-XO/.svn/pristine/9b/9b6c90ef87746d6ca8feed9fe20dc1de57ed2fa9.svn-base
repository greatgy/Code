	</select>
	
	<select id="getOne" parameterType="hashMap" resultType="finance">
		select
		<include refid="allcolumns" />
		from
		<include refid="table"/>
		<where>
			<if test="oid != null">
				and <include refid="setoid"/>
			</if>
			<if test="uid != null">
				and <include refid="setuid"/>
			</if>
			<if test="contributeuid != null">
				and <include refid="setcontributeuid"/>
			</if>
			<if test="grade != null">
			    and <include refid="setgrade"></include>
			</if>
			<if test="type != null">
				and <include refid="wheretype"/>
			</if>
			<if test="status != null">
				and <include refid="setstatus"/>
			</if>
			<if test="publishtimestartne != null">
				and <include refid="setpublishtimestartne" />
			</if>
			<if test="publishtimeendne != null">
				and <include refid="setpublishtimeendne" />
			</if>
			<if test="createtimestartne != null">
				and <include refid="setcreatetimestartne" />
			</if>
			<if test="createtimeendne != null">
				and <include refid="setcreatetimeendne" />
			</if>
		</where>
		<choose>
			<when test="orderBy != null">
				${orderBy}
			</when>
			<otherwise>
				<include refid="orderby"/>
				<if test="ascDesc != null">
					${ascDesc}
				</if>
			</otherwise>
		</choose>
		LIMIT 0,1
	</select>
	
