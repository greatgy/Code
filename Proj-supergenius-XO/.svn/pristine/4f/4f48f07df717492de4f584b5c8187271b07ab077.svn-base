alter table contributor add column avatarbig  varchar(255) default '' comment '大头像' after summary;
alter table contributor add column avatarmedium  varchar(255) default '' comment '中头像' after avatarbig;
alter table contributor add column avatarlittle  varchar(255) default '' comment '小头像' after avatarmedium;
alter table contributor add column avataroriginal  varchar(255) default '' comment '原头像' after avatarlittle;
alter table contributor add column useruid   char(32) comment '会员uid' after name;

