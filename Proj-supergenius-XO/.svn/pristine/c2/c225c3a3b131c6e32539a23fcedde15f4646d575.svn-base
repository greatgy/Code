<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.supergenius.xo.finance.dao.UserStatisticsDao">

    <sql id="basecolumns">`uid`, `createtime` </sql>
    <sql id="othercolumns"><include refid="basecolumns"/>, `useruid`, `articlecount`, `topiccount`, `score`, `clickcount`, `subscribecount`, `besubscribecount`, `collectarticlecount`, `collecttopiccount`, `collectdebatecount`, `mycommentcount`, `tomeprizecount`, `status` </sql>
    <sql id="allcolumns"><include refid="basecolumns"/>, <include refid="othercolumns"/>, `updatetime`</sql>
    <sql id="table">userstatistics</sql>
    <sql id="count">count(*)</sql>
    <sql id="whereid">where uid = #{uid}</sql>
    <sql id="allfields"><include refid="othercolumns"/> </sql>
    <sql id="allparams">#{uid},#{createtime},#{useruid},#{articlecount},#{topiccount},#{score},#{clickcount},#{subscribecount},#{besubscribecount},#{collectarticlecount},#{collecttopiccount},#{collectdebatecount},#{mycommentcount},#{tomeprizecount},#{status}</sql>
    <sql id="setuseruid">useruid = #{useruid}</sql>
    <sql id="setarticlecount">articlecount = #{articlecount}</sql>
    <sql id="settopiccount">topiccount = #{topiccount}</sql>
    <sql id="setscore">score = #{score}</sql>
    <sql id="setclickcount">clickcount = #{clickcount}</sql>
    <sql id="setsubscribecount">subscribecount = #{subscribecount}</sql>
    <sql id="setbesubscribecount">besubscribecount = #{besubscribecount}</sql>
    <sql id="setcollectarticlecount">collectarticlecount = #{collectarticlecount}</sql>
    <sql id="setcollecttopiccount">collecttopiccount = #{collecttopiccount}</sql>
    <sql id="setcollectdebatecount">collectdebatecount = #{collectdebatecount}</sql>
    <sql id="setmycommentcount">mycommentcount = #{mycommentcount}</sql>
    <sql id="settomeprizecount">tomeprizecount = #{tomeprizecount}</sql>
    <sql id="setstatus">status = #{status}</sql>
    <sql id="allsets">set
            <include refid="setuseruid"/>,
            <include refid="setarticlecount"/>,
            <include refid="settopiccount"/>,
            <include refid="setscore"/>,
            <include refid="setclickcount"/>,
            <include refid="setsubscribecount"/>,
            <include refid="setbesubscribecount"/>,
            <include refid="setcollectarticlecount"/>,
            <include refid="setcollecttopiccount"/>,
            <include refid="setcollectdebatecount"/>,
            <include refid="setmycommentcount"/>,
            <include refid="settomeprizecount"/>,
            <include refid="setstatus"/>
    </sql>
    <sql id="orderby">order by createtime</sql>

    <select id="get" parameterType="string" resultType="userstatistics">
        select
        <include refid="allcolumns" />
        from
        <include refid="table"/>
        <include refid="whereid"/>
    </select>

    <select id="getOne" parameterType="hashMap" resultType="userstatistics">
        select
        <include refid="allcolumns" />
        from
        <include refid="table"/>
        <where>
            <if test="useruid != null">
                and <include refid="setuseruid"/>
            </if>
            <if test="articlecount != null">
                and <include refid="setarticlecount"/>
            </if>
            <if test="topiccount != null">
                and <include refid="settopiccount"/>
            </if>
            <if test="score != null">
                and <include refid="setscore"/>
            </if>
            <if test="clickcount != null">
                and <include refid="setclickcount"/>
            </if>
            <if test="subscribecount != null">
                and <include refid="setsubscribecount"/>
            </if>
            <if test="besubscribecount != null">
                and <include refid="setbesubscribecount"/>
            </if>
            <if test="collectarticlecount != null">
                and <include refid="setcollectarticlecount"/>
            </if>
            <if test="collecttopiccount != null">
                and <include refid="setcollecttopiccount"/>
            </if>
            <if test="collectdebatecount != null">
                and <include refid="setcollectdebatecount"/>
            </if>
            <if test="mycommentcount != null">
                and <include refid="setmycommentcount"/>
            </if>
            <if test="tomeprizecount != null">
                and <include refid="settomeprizecount"/>
            </if>
            <if test="status != null">
                and <include refid="setstatus"/>
            </if>
        </where>
        LIMIT 0,1
    </select>

    <select id="getCount" parameterType="hashMap" resultType="int">
        select
        <include refid="count" />
        from
        <include refid="table"/>
        <where>
            <if test="useruid != null">
                and <include refid="setuseruid"/>
            </if>
            <if test="articlecount != null">
                and <include refid="setarticlecount"/>
            </if>
            <if test="topiccount != null">
                and <include refid="settopiccount"/>
            </if>
            <if test="score != null">
                and <include refid="setscore"/>
            </if>
            <if test="clickcount != null">
                and <include refid="setclickcount"/>
            </if>
            <if test="subscribecount != null">
                and <include refid="setsubscribecount"/>
            </if>
            <if test="besubscribecount != null">
                and <include refid="setbesubscribecount"/>
            </if>
            <if test="collectarticlecount != null">
                and <include refid="setcollectarticlecount"/>
            </if>
            <if test="collecttopiccount != null">
                and <include refid="setcollecttopiccount"/>
            </if>
            <if test="collectdebatecount != null">
                and <include refid="setcollectdebatecount"/>
            </if>
            <if test="mycommentcount != null">
                and <include refid="setmycommentcount"/>
            </if>
            <if test="tomeprizecount != null">
                and <include refid="settomeprizecount"/>
            </if>
            <if test="status != null">
                and <include refid="setstatus"/>
            </if>
        </where>
    </select>

    <select id="getList" parameterType="hashMap" resultType="userstatistics">
        select
        <include refid="allcolumns" />
        from
        <include refid="table"/>
        <where>
            <if test="useruid != null">
                and <include refid="setuseruid"/>
            </if>
            <if test="articlecount != null">
                and <include refid="setarticlecount"/>
            </if>
            <if test="topiccount != null">
                and <include refid="settopiccount"/>
            </if>
            <if test="score != null">
                and <include refid="setscore"/>
            </if>
            <if test="clickcount != null">
                and <include refid="setclickcount"/>
            </if>
            <if test="subscribecount != null">
                and <include refid="setsubscribecount"/>
            </if>
            <if test="besubscribecount != null">
                and <include refid="setbesubscribecount"/>
            </if>
            <if test="collectarticlecount != null">
                and <include refid="setcollectarticlecount"/>
            </if>
            <if test="collecttopiccount != null">
                and <include refid="setcollecttopiccount"/>
            </if>
            <if test="collectdebatecount != null">
                and <include refid="setcollectdebatecount"/>
            </if>
            <if test="mycommentcount != null">
                and <include refid="setmycommentcount"/>
            </if>
            <if test="tomeprizecount != null">
                and <include refid="settomeprizecount"/>
            </if>
            <if test="status != null">
                and <include refid="setstatus"/>
            </if>
        </where>
        <!-- 处理orderby语句 -->
        <choose>
            <when test="orderBy != null">
                ${orderBy}
            </when>
            <otherwise>
                <include refid="orderby"/>
                <if test="ascDesc != null">
                    ${ascDesc}
                </if>
            </otherwise>
        </choose>
        <!-- 处理limit语句 -->
        <trim prefix="LIMIT" prefixOverrides=",">
            <if test="startIndex != null and pageSize != null">
                ${startIndex}
            </if>
            <if test="pageSize != null">
                , ${pageSize}
            </if>
        </trim>
    </select>

    <insert id="insert" parameterType="userstatistics">
        insert into
        <include refid="table"/>
        (<include refid="allfields"/>)
        values (
        <include refid="allparams"/>
        )
    </insert>

    <update id="update" parameterType="userstatistics">
        update
        <include refid="table"/>
        <include refid="allsets"/>
        <include refid="whereid"/>
    </update>

    <update id="updateFields" parameterType="hashMap">
        update
        <include refid="table"/>
        <set>
            <if test="useruid != null">
                <include refid="setuseruid"/>,
            </if>
            <if test="articlecount != null">
                <include refid="setarticlecount"/>,
            </if>
            <if test="topiccount != null">
                <include refid="settopiccount"/>,
            </if>
            <if test="score != null">
                <include refid="setscore"/>,
            </if>
            <if test="clickcount != null">
                <include refid="setclickcount"/>,
            </if>
            <if test="subscribecount != null">
                <include refid="setsubscribecount"/>,
            </if>
            <if test="besubscribecount != null">
                <include refid="setbesubscribecount"/>,
            </if>
            <if test="collectarticlecount != null">
                <include refid="setcollectarticlecount"/>,
            </if>
            <if test="collecttopiccount != null">
                <include refid="setcollecttopiccount"/>,
            </if>
            <if test="collectdebatecount != null">
                <include refid="setcollectdebatecount"/>,
            </if>
            <if test="mycommentcount != null">
                <include refid="setmycommentcount"/>
            </if>
            <if test="tomeprizecount != null">
                <include refid="settomeprizecount"/>
            </if>
            <if test="status != null">
                <include refid="setstatus"/>
            </if>
        </set>
        <include refid="whereid"/>
    </update>

    <delete id="delete" parameterType="string">
        delete from
        <include refid="table"/>
        <include refid="whereid"/>
    </delete>

</mapper>

