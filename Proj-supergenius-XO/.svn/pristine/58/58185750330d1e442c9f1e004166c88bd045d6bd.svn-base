			</if>
			<if test="originurl != null">
				and <include refid="setoriginurl"/>
			</if>
			<if test="config != null">
				and <include refid="setconfig"/>
			</if>
			<if test="ispublic != null">
				and <include refid="setispublic"/>
			</if>
			<if test="status != null">
				and <include refid="setstatus"/>
			</if>
			<if test="ids != null">
				and uid in
				<foreach item="item" index="index" collection="ids" 
                 	 open="(" separator="," close=")">
                 	 #{item}
            	</foreach>
			</if>
		</where>
	</delete>
</mapper>