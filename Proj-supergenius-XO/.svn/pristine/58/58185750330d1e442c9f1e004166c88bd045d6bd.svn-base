<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.supergenius.xo.finance.dao.DebateArticleDao">
	
	<sql id="basecolumns">`oid`,`uid`,`createtime`</sql>
	<sql id="othercolumns">`adminuid`, `finaceuid`, `debateuid`, `useruid`, `title`, `type`, `author`, `content`, `isred`, `imglittle`, `imgmedium`, `imgbig`, `imgoriginal`, `origin`, `originurl`, `sortorder`,`config`,`ispublic`,`status`</sql>
	<sql id="allcolumns"><include refid="basecolumns"/>, <include refid="othercolumns"/>,`updatetime`</sql>
	<sql id="table">debatearticle</sql>
	<sql id="whereid">where uid = #{uid}</sql>
	<sql id="allfields"><include refid="basecolumns"/>, <include refid="othercolumns"/></sql>
	<sql id="allparams">#{oid},#{uid},#{createtime},#{adminuid},#{finaceuid},#{debateuid},#{useruid},#{title},#{type},#{author},#{content},#{isred},#{imglittle},#{imgmedium},#{imgbig},#{imgoriginal},#{origin},#{originurl},#{sortorder},#{config},#{ispublic},#{status}</sql>
	<sql id="count">count(*)</sql>
	<sql id="setoid">oid = #{oid}</sql>
	<sql id="setuid">uid = #{uid}</sql>
	<sql id="setadminuid">adminuid = #{adminuid}</sql>
	<sql id="setfinaceuid">finaceuid = #{finaceuid}</sql>
	<sql id="setdebateuid">debateuid = #{debateuid}</sql>
	<sql id="setuseruid">useruid = #{useruid}</sql>
	<sql id="settitle">title = #{title}</sql>
	<sql id="settitle_lk">title like CONCAT('%',#{title_lk},'%')</sql>
	<sql id="settype">type = #{type}</sql>
	<sql id="setauthor">author = #{author}</sql>
	<sql id="setcontent">content = #{content}</sql>
	<sql id="setisred">isred = #{isred}</sql>
	<sql id="setimglittle">imglittle = #{imglittle}</sql>
	<sql id="setimgmedium">imgmedium = #{imgmedium}</sql>
	<sql id="setimgbig">imgbig = #{imgbig}</sql>
	<sql id="setimgoriginal">imgoriginal = #{imgoriginal}</sql>
	<sql id="setorigin">origin = #{origin}</sql>
	<sql id="setoriginurl">originurl = #{originurl}</sql>
	<sql id="setsortorder">sortorder = #{sortorder}</sql>
	<sql id="setconfig">config = #{config}</sql>
	<sql id="setispublic">ispublic = #{ispublic}</sql>
	<sql id="setstatus">status = #{status}</sql>
	<sql id="setids">uid in #{ids}</sql>
	<sql id="allsets">set 
	<include refid="setoid"/>,
    <include refid="setuid"/>,
	<include refid="setadminuid"/>,
	<include refid="setfinaceuid"/>,
	<include refid="setdebateuid"/>,
	<include refid="setuseruid"/>,
	<include refid="settitle"/>,
	<include refid="settype"/>,
	<include refid="setauthor"/>,
	<include refid="setcontent"/>,
	<include refid="setisred"/>,
	<include refid="setimglittle"/>,
	<include refid="setimgmedium"/>,
	<include refid="setimgbig"/>,
	<include refid="setimgoriginal"/>,
	<include refid="setorigin"/>,
	<include refid="setoriginurl"/>,
	<include refid="setsortorder"/>,
	<include refid="setconfig"/>,
	<include refid="setispublic"/>,
	<include refid="setstatus"/></sql>
	<sql id="orderby">order by createtime</sql>
	
	<select id="get" parameterType="string" resultType="debatearticle">
		select
		<include refid="allcolumns" />
		from
		<include refid="table"/>
		<include refid="whereid"/>
	</select>
	
	<select id="getOne" parameterType="hashMap" resultType="debatearticle">
		select
		<include refid="allcolumns" />
		from
		<include refid="table"/>
		<where>
			<if test="oid != null">
				and <include refid="setoid"/>
			</if>
			<if test="uid != null">
				and <include refid="setuid"/>
			</if>
		</where>
		LIMIT 0,1
	</select>
	
	<select id="getCount" parameterType="hashMap" resultType="int">
		select
		<include refid="count" />
		from
		<include refid="table"/>
		<where>
			<if test="oid != null">
				and <include refid="setoid"/>
			</if>
			<if test="uid != null">
				and <include refid="setuid"/>
			</if>
			<if test="adminuid != null">
				and <include refid="setadminuid"/>
			</if>
			<if test="finaceuid != null">
				and <include refid="setfinaceuid"/>
			</if>
			<if test="debateuid != null">
				and <include refid="setdebateuid"/>
			</if>
			<if test="useruid != null">
				and <include refid="setuseruid"/>
			</if>
			<if test="title != null">
				and <include refid="settitle"/>
			</if>
			<if test="title_lk != null">
				and <include refid="settitle_lk"/>
			</if>
			<if test="type != null">
				and <include refid="settype"/>
			</if>
			<if test="author != null">
				and <include refid="setauthor"/>
			</if>
			<if test="content != null">
				and <include refid="setcontent"/>
			</if>
			<if test="isred != null">
				and <include refid="setisred"/>
			</if>
			<if test="imglittle != null">
				and <include refid="setimglittle"/>
			</if>
			<if test="imgmedium != null">
				and <include refid="setimgmedium"/>
			</if>
			<if test="imgbig != null">
				and <include refid="setimgbig"/>
			</if>
			<if test="imgoriginal != null">
				and <include refid="setimgoriginal"/>
			</if>
			<if test="origin != null">
				and <include refid="setorigin"/>
			</if>
			<if test="originurl != null">
				and <include refid="setoriginurl"/>
			</if>
			<if test="config != null">
				and <include refid="setconfig"/>
			</if>
			<if test="ispublic != null">
				and <include refid="setispublic"/>
			</if>
			<if test="status != null">
				and <include refid="setstatus"/>
			</if>
		</where>
	</select>
	
	<select id="getList" parameterType="hashMap" resultType="debatearticle">
		select
		<include refid="allcolumns" />
		from
		<include refid="table"/>
		<where>
			<if test="oid != null">
				and <include refid="setoid"/>
			</if>
			<if test="uid != null">
				and <include refid="setuid"/>
			</if>
			<if test="adminuid != null">
				and <include refid="setadminuid"/>
			</if>
			<if test="finaceuid != null">
				and <include refid="setfinaceuid"/>
			</if>
			<if test="debateuid != null">
				and <include refid="setdebateuid"/>
			</if>
			<if test="useruid != null">
				and <include refid="setuseruid"/>
			</if>
			<if test="title != null">
				and <include refid="settitle"/>
			</if>
			<if test="title_lk != null">
				and <include refid="settitle_lk"/>
			</if>
			<if test="type != null">
				and <include refid="settype"/>
			</if>
			<if test="author != null">
				and <include refid="setauthor"/>
			</if>
			<if test="content != null">
				and <include refid="setcontent"/>
			</if>
			<if test="isred != null">
				and <include refid="setisred"/>
			</if>
			<if test="origin != null">
				and <include refid="setorigin"/>
			</if>
			<if test="originurl != null">
				and <include refid="setoriginurl"/>
			</if>
			<if test="config != null">
				and <include refid="setconfig"/>
			</if>
			<if test="ispublic != null">
				and <include refid="setispublic"/>
			</if>
			<if test="status != null">
				and <include refid="setstatus"/>
			</if>
		</where>
		<!-- 处理orderby语句 -->
		<choose>
			<when test="orderBy != null">
				${orderBy}
			</when>
			<otherwise>
				<include refid="orderby"/>
				<if test="ascDesc != null">
					${ascDesc}
				</if>
			</otherwise>
		</choose>
		<!-- 处理limit语句 -->
		<trim prefix="LIMIT" prefixOverrides=",">
			<if test="startIndex != null and pageSize != null">
				${startIndex}
			</if>
			<if test="pageSize != null">
			 , ${pageSize}
			 </if>
		</trim>
	</select>
	
	<insert id="insert" parameterType="debatearticle">
		<selectKey resultType="int" keyProperty="oid">
			SELECT LAST_INSERT_ID()
		</selectKey>
		insert into
		<include refid="table"/>
		(<include refid="allfields"/>)
		values (
			<include refid="allparams"/>
		)
	</insert>

	<insert id="insertList" parameterType="list">
		insert into
		<include refid="table"/>
		(<include refid="allfields"/>)
		values 
		<foreach collection="list" item="item" index="index" separator=",">
		(
			#{item.oid},
			#{item.uid},
			#{item.createtime},
			#{item.adminuid},
			#{item.finaceuid},
			#{item.debateuid},
			#{item.useruid},
			#{item.title},
			#{item.type},
			#{item.author},
			#{item.content},
			#{item.isred},
			#{item.imglittle},
			#{item.imgmedium},
			#{item.imgbig},
			#{item.imgoriginal},
			#{item.origin},
			#{item.originurl},
			#{item.sortorder},
			#{item.config},
			#{item.ispublic},
			#{item.status}
		)
 		</foreach>
	</insert>
	
	<update id="update" parameterType="debatearticle">
		update
		<include refid="table"/>
		<include refid="allsets"/>
		<include refid="whereid"/>
	</update>

	<update id="updateFields" parameterType="hashMap">
		update
		<include refid="table"/>
		<set>
			<if test="adminuid != null">
				 <include refid="setadminuid"/>,
			</if>
			<if test="finaceuid != null">
