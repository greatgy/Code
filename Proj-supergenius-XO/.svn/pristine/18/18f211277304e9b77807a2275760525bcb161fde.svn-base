package com.supergenius.xo.manager.entity;

import org.joda.time.DateTime;

import com.genius.model.base.entity.BaseEntity;

/**
 * 我的视频
 * 
 * @author liubin
 * @date 2016-8-12 上午10:37:21
 */
public class MyVideo extends BaseEntity {

	private static final long serialVersionUID = 4438720091159372238L;
	private String useruid;// 用户uid
	private String videouid;// 视频uid
	private DateTime expiretime;// 过期时间

	public MyVideo() {
		super();
	}

	public MyVideo(String useruid, String videouid, DateTime expiretime) {
		super();
		this.useruid = useruid;
		this.videouid = videouid;
		this.expiretime = expiretime;
	}

	public String getUseruid() {
		return useruid;
	}

	public void setUseruid(String useruid) {
		this.useruid = useruid;
	}

	public String getVideouid() {
		return videouid;
	}

	public void setVideouid(String videouid) {
		this.videouid = videouid;
	}

	public DateTime getExpiretime() {
		return expiretime;
	}

	public void setExpiretime(DateTime expiretime) {
		this.expiretime = expiretime;
	}
}
